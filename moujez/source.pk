aV\u0645\u062f\u06cc\u0631 \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0648 \u0647\u0634\u062f\u0627\u0631 \u0633\u0631\u06cc\u0639 \u0633\u0627\u0632\u0645\u0627\u0646 \u0647\u0648\u0627\u0634\u0646\u0627\u0633\u06cc \u06a9\u0634\u0648\u0631 \u0627\u0632 \u0631\u06af\u0628\u0627\u0631 \u0648 \u0631\u0639\u062f \u0648 \u0628\u0631\u0642 \u062f\u0631 \u0628\u0631\u062e\u06cc \u0627\u0632 \u0645\u0646\u0627\u0637\u0642 \u06a9\u0634\u0648\u0631 \u062f\u0631 \u062f\u0648 \u0631\u0648\u0632 \u0622\u06cc\u0646\u062f\u0647 \u062e\u0628\u0631 \u062f\u0627\u062f.\u0645\u0647\u0646\u062f\u0633 \u0627\u062d\u062f \u0648\u0638\u06cc\u0641\u0647 \u0631\u0648\u0632 \u062c\u0645\u0639\u0647 \u062f\u0631 \u06af\u0641\u062a\u200c\u0648\u06af\u0648 \u0628\u0627 \u0627\u06cc\u0633\u0646\u0627\u060c \u0627\u0641\u0632\u0648\u062f: \u0628\u0631\u0627\u0633\u0627\u0633 \u0646\u0642\u0634\u0647\u200c\u0647\u0627\u06cc \u067e\u06cc\u0634\u200c\u06cc\u0627\u0628\u06cc \u0647\u0648\u0627\u0634\u0646\u0627\u0633\u06cc \u0637\u06cc \u062f\u0648 \u0631\u0648\u0632 \u0622\u06cc\u0646\u062f\u0647 \u062f\u0631 \u0634\u0645\u0627\u0644 \u063a\u0631\u0628\u060c \u0633\u0648\u0627\u062d\u0644 \u062f\u0631\u06cc\u0627\u06cc \u062e\u0632\u0631\u060c \u062f\u0627\u0645\u0646\u0647\u200c\u0647\u0627\u06cc \u062c\u0646\u0648\u0628\u06cc \u0627\u0644\u0628\u0631\u0632\u060c \u062f\u0627\u0645\u0646\u0647\u200c\u0647\u0627\u06cc \u0645\u0631\u06a9\u0632\u06cc \u0632\u0627\u06af\u0631\u0633 \u0627\u0641\u0632\u0627\u06cc\u0634 \u0627\u0628\u0631\u060c \u0631\u06af\u0628\u0627\u0631 \u0648 \u0631\u0639\u062f \u0648 \u0628\u0631\u0642 \u0648 \u0648\u0632\u0634 \u0628\u0627\u062f \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0645\u06cc\u200c\u0634\u0648\u062f \u0648 \u0627\u06cc\u0646 \u0648\u0636\u0639\u06cc\u062a \u0631\u0648\u0632 \u062f\u0648\u0634\u0646\u0628\u0647 \u062f\u0631 \u062f\u0627\u0645\u0646\u0647\u200c\u0647\u0627\u06cc \u0634\u0631\u0642\u06cc \u0627\u0644\u0628\u0631\u0632\u060c\u200c \u0633\u0648\u0627\u062d\u0644 \u0645\u0631\u06a9\u0632\u06cc \u0648 \u0634\u0631\u0642\u06cc \u062e\u0632\u0631\u060c \u0646\u0648\u0627\u062d\u06cc \u0645\u0631\u06a9\u0632\u06cc\u060c \u0634\u0645\u0627\u0644 \u0634\u0631\u0642 \u0648 \u0628\u0631\u062e\u06cc \u0645\u0646\u0627\u0637\u0642 \u0634\u0631\u0642 \u06a9\u0634\u0648\u0631 \u0631\u062e \u062e\u0648\u0627\u0647\u062f \u062f\u0627\u062f.\u0648\u06cc \u0641\u0632\u0648\u062f: \u062f\u0631 \u0631\u0648\u0632 \u0634\u0646\u0628\u0647\u060c \u062f\u0631 \u0627\u0633\u062a\u0627\u0646\u200c\u0647\u0627\u06cc \u0622\u0630\u0631\u0628\u0627\u06cc\u062c\u0627\u0646 \u0634\u0631\u0642\u06cc\u060c\u200c \u0622\u0630\u0631\u0628\u0627\u06cc\u062c\u0627\u0646 \u063a\u0631\u0628\u06cc\u060c \u0627\u0631\u062f\u0628\u06cc\u0644\u060c \u06a9\u0631\u062f\u0633\u062a\u0627\u0646\u060c \u06a9\u0631\u0645\u0627\u0646\u0634\u0627\u0647\u060c \u0644\u0631\u0633\u062a\u0627\u0646\u060c\u200c \u0647\u0645\u062f\u0627\u0646\u060c \u0645\u0631\u06a9\u0632\u06cc\u060c \u0632\u0646\u062c\u0627\u0646\u060c \u0642\u0632\u0648\u06cc\u0646\u060c \u0627\u0644\u0628\u0631\u0632\u060c \u062a\u0647\u0631\u0627\u0646\u060c \u0634\u0645\u0627\u0644 \u0633\u0645\u0646\u0627\u0646\u060c\u200c \u06af\u06cc\u0644\u0627\u0646\u060c \u0645\u0627\u0632\u0646\u062f\u0631\u0627\u0646 \u0648 \u0628\u0627 \u0634\u062f\u062a \u06a9\u0645\u062a\u0631\u06cc \u062f\u0631 \u0627\u0633\u062a\u0627\u0646\u200c\u0647\u0627\u06cc \u0627\u06cc\u0644\u0627\u0645\u060c \u0686\u0647\u0627\u0631\u0645\u062d\u0627\u0644 \u0648 \u0628\u062e\u062a\u06cc\u0627\u0631\u06cc\u060c \u0627\u0635\u0641\u0647\u0627\u0646\u060c\u200c \u0642\u0645 \u0648 \u06af\u0644\u0633\u062a\u0627\u0646 \u0634\u0631\u0627\u06cc\u0637 \u06cc\u0627\u062f\u0634\u062f\u0647 \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0645\u06cc\u200c\u0634\u0648\u062f. \u0627\u06cc\u0646 \u0648\u0636\u0639\u06cc\u062a \u062f\u0631 \u0631\u0648\u0632 \u06cc\u06a9\u0634\u0646\u0628\u0647 \u062f\u0631 \u0627\u0633\u062a\u0627\u0646\u200c\u0647\u0627\u06cc \u0627\u0631\u062f\u0628\u06cc\u0644\u060c \u0647\u0645\u062f\u0627\u0646\u060c \u0645\u0631\u06a9\u0632\u06cc\u060c \u0642\u0632\u0648\u06cc\u0646\u060c \u0627\u0644\u0628\u0631\u0632\u060c \u062a\u0647\u0631\u0627\u0646\u060c \u0642\u0645\u060c \u0634\u0645\u0627\u0644 \u0633\u0645\u0646\u0627\u0646\u060c \u06af\u06cc\u0644\u0627\u0646\u060c \u0645\u0627\u0632\u0646\u062f\u0631\u0627\u0646\u060c \u0627\u0635\u0641\u0647\u0627\u0646\u060c \u0686\u0647\u0627\u0631\u0645\u062d\u0627\u0644 \u0648 \u0628\u062e\u062a\u06cc\u0627\u0631\u06cc \u0648 \u0628\u0627 \u0634\u062f\u062a \u06a9\u0645\u062a\u0631\u06cc \u062f\u0631 \u0627\u0633\u062a\u0627\u0646\u200c\u0647\u0627\u06cc \u0622\u0630\u0631\u0628\u0627\u06cc\u062c\u0627\u0646 \u0634\u0631\u0642\u06cc\u060c \u0622\u0630\u0631\u0628\u0627\u06cc\u062c\u0627\u0646 \u063a\u0631\u0628\u06cc\u060c \u0632\u0646\u062c\u0627\u0646\u060c \u06cc\u0632\u062f \u0648 \u0634\u0645\u0627\u0644 \u06a9\u0631\u0645\u0627\u0646 \u0627\u062f\u0627\u0645\u0647 \u062e\u0648\u0627\u0647\u062f \u062f\u0627\u0634\u062a.\u0645\u062f\u06cc\u0631 \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0648 \u0647\u0634\u062f\u0627\u0631 \u0633\u0631\u06cc\u0639 \u0633\u0627\u0632\u0645\u0627\u0646 \u0647\u0648\u0627\u0634\u0646\u0627\u0633\u06cc \u06a9\u0634\u0648\u0631 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0648\u06cc\u0698\u06af\u06cc\u200c \u0628\u0627\u0631\u0634\u200c\u0647\u0627\u06cc \u0628\u0647\u0627\u0631\u06cc \u06a9\u0647 \u0628\u0647 \u0635\u0648\u0631\u062a \u0645\u062d\u0644\u06cc\u060c \u0634\u062f\u06cc\u062f \u0648 \u06a9\u0648\u062a\u0627\u0647 \u0645\u062f\u062a \u0627\u0633\u062a\u060c \u06af\u0641\u062a: \u0628\u0631\u0627\u06cc \u067e\u06cc\u0634\u06af\u06cc\u0631\u06cc \u0627\u0632 \u062e\u0633\u0627\u0631\u0627\u062a \u0627\u062d\u062a\u0645\u0627\u0644\u06cc \u062a\u0648\u0642\u0641 \u062f\u0631 \u062d\u0627\u0634\u06cc\u0647 \u0631\u0648\u062f\u062e\u0627\u0646\u0647\u200c\u0647\u0627 \u0648 \u0645\u0633\u06cc\u0644\u200c\u0647\u0627 \u0628\u0627 \u0627\u062d\u062a\u06cc\u0627\u0637 \u0644\u0627\u0632\u0645 \u0627\u0646\u062c\u0627\u0645 \u0634\u0648\u062f \u0648 \u062a\u0627 \u062d\u062f \u0627\u0645\u06a9\u0627\u0646 \u062e\u0648\u062f\u062f\u0627\u0631\u06cc \u0634\u0648\u062f.\u0645\u062f\u06cc\u0631 \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0648 \u0647\u0634\u062f\u0627\u0631 \u0633\u0631\u06cc\u0639 \u0633\u0627\u0632\u0645\u0627\u0646 \u0647\u0648\u0627\u0634\u0646\u0627\u0633\u06cc \u06a9\u0634\u0648\u0631 \u0628\u0647 \u0648\u0636\u0639 \u0647\u0648\u0627\u06cc \u062a\u0647\u0631\u0627\u0646 \u0627\u0634\u0627\u0631\u0647 \u06a9\u0631\u062f \u0648 \u06af\u0641\u062a: \u0628\u0631 \u0627\u06cc\u0646 \u0627\u0633\u0627\u0633 \u0622\u0633\u0645\u0627\u0646 \u062a\u0647\u0631\u0627\u0646 \u062f\u0631 \u0631\u0648\u0632 \u06cc\u06a9\u0634\u0646\u0628\u0647 \u0646\u06cc\u0645\u0647 \u0627\u0628\u0631\u06cc \u0627\u0648\u0627\u06cc\u0644 \u0634\u0628 \u0628\u0627\u0631\u0634 \u067e\u0631\u0627\u06a9\u0646\u062f\u0647 \u062f\u0631 \u067e\u0627\u0631\u0647\u200c\u0627\u06cc \u0646\u0642\u0627\u0637 \u0648\u0632\u0634 \u0628\u0627\u062f \u062e\u0648\u0627\u0647\u062f \u0628\u0648\u062f \u06a9\u0647 \u062d\u062f\u0627\u0642\u0644 \u0648 \u062d\u062f\u0627\u06a9\u062b\u0631 \u062f\u0645\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0631\u0648\u0632 \u0628\u0647 19 \u0648 31 \u062f\u0631\u062c\u0647 \u0633\u0627\u0646\u062a\u06cc\u200c\u06af\u0631\u0627\u062f \u0628\u0627\u0644\u0627\u06cc \u0635\u0641\u0631 \u0645\u06cc\u200c\u0631\u0633\u062f. \u0647\u0645\u0686\u0646\u06cc\u0646 \u062f\u0631 \u0631\u0648\u0632 \u062f\u0648\u0634\u0646\u0628\u0647 \u0622\u0633\u0645\u0627\u0646 \u067e\u0627\u06cc\u062a\u062e\u062a \u0646\u06cc\u0645\u0647\u200c\u0627\u0628\u0631\u06cc \u0648 \u0648\u0632\u0634 \u0628\u0627\u062f \u067e\u06cc\u0634\u200c\u0628\u06cc\u0646\u06cc \u0634\u062f\u0647 \u0627\u0633\u062a \u06a9\u0647 \u062d\u062f\u0627\u0642\u0644 \u0648 \u062d\u062f\u0627\u06a9\u062b\u0631 \u062f\u0645\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0631\u0648\u0632 \u0628\u0627 \u062f\u0648 \u062f\u0631\u062c\u0647 \u0627\u0641\u0632\u0627\u06cc\u0634 \u0628\u0647 21 \u0648 33 \u062f\u0631\u062c\u0647 \u0633\u0627\u0646\u062a\u06cc\u200c\u06af\u0631\u0627\u062f \u0628\u0627\u0644\u0627\u06cc \u0635\u0641\u0631 \u062e\u0648\u0627\u0647\u062f \u0631\u0633\u06cc\u062f.
