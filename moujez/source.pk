p118
aV\u0648\u0644\u0627\u062f\u06cc\u0645\u06cc\u0631 \u067e\u0648\u062a\u06cc\u0646 \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0631 \u06a9\u0646\u0641\u0631\u0627\u0646\u0633 \u062e\u0628\u0631\u06cc \u0628\u0627 \u062e\u0628\u0631\u0646\u06af\u0627\u0631\u0627\u0646\u060c \u0636\u0645\u0646 \u0627\u0646\u062a\u0642\u0627\u062f \u0627\u0632 \u063a\u0631\u0628 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u062d\u0645\u0627\u06cc\u062a \u0627\u0632 "\u06a9\u0648\u062f\u062a\u0627"\u060c \u062f\u062e\u0627\u0644\u062a\u200c\u0647\u0627\u06cc \u063a\u0631\u0628 \u0631\u0627 \u0639\u0627\u0645\u0644 \u0628\u0631 \u0647\u0645 \u062e\u0648\u0631\u062f\u0646 \u0631\u0648\u0646\u062f \u0633\u0627\u0632\u0646\u062f\u0647 \u0645\u0630\u0627\u06a9\u0631\u0627\u062a \u062f\u0631 \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u0648 \u0628\u0627\u0644\u0627 \u06af\u0631\u0641\u062a\u0646 \u062a\u0646\u0634\u200c\u0647\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u062f\u0627\u0646\u0633\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0641\u0627\u0631\u0633\u060c\u0648\u06cc \u062f\u0631 \u062f\u06cc\u062f\u0627\u0631 \u0628\u0627 \u0631\u0648\u0633\u0627\u06cc \u0634\u0631\u06a9\u062a\u200c\u0647\u0627\u06cc \u0627\u0646\u0631\u0698\u06cc \u062e\u0627\u0631\u062c\u06cc \u0648 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0627\u062e\u062a\u0644\u0627\u0641 \u06a9\u0634\u0648\u0631\u0634 \u0648 \u062f\u0648\u0644\u062a \u0645\u0648\u0642\u062a \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u0628\u0631 \u0633\u0631 \u0642\u06cc\u0645\u062a \u06af\u0627\u0632 \u06a9\u0647 \u062e\u0637\u0631 \u0642\u0637\u0639 \u06af\u0627\u0632 \u0635\u0627\u0631\u062f\u0627\u062a\u06cc \u0628\u0647 \u0627\u0631\u0648\u067e\u0627 \u0631\u0627 \u062f\u0631 \u0628\u0631 \u062f\u0627\u0631\u062f \u06af\u0641\u062a: \u062f\u0648\u0644\u062a \u0645\u0648\u0642\u062a \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u0628\u0647 \u0647\u06cc\u0686 \u0648\u062c\u0647 \u062d\u0642 \u0646\u062f\u0627\u0631\u062f \u0628\u0631\u0627\u06cc \u06af\u0627\u0632 \u0648\u0627\u0631\u062f\u0627\u062a\u06cc \u0627\u0632 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u062a\u062e\u0641\u06cc\u0641 \u0642\u06cc\u0645\u062a \u0628\u062f\u0647\u062f.\u067e\u0648\u062a\u06cc\u0646 \u0628\u0647 \u06af\u0645\u0627\u0646\u0647 \u0632\u0646\u06cc\u200c\u0647\u0627 \u062f\u0631 \u062e\u0635\u0648\u0635 \u0627\u0641\u0632\u0627\u06cc\u0634 \u0628\u06cc\u0634 \u0627\u0632 \u067e\u06cc\u0634 \u062a\u0646\u0634 \u0647\u0627 \u062f\u0631 \u0631\u0648\u0627\u0628\u0637 \u063a\u0631\u0628 \u0648 \u0631\u0648\u0633\u06cc\u0647 \u0627\u0634\u0627\u0631\u0647 \u06a9\u0631\u062f \u0648 \u06af\u0641\u062a: \u06a9\u0633\u06cc \u0639\u0644\u0627\u0642\u0647 \u0628\u0647 \u0631\u0627\u0647 \u0627\u0641\u062a\u0627\u062f\u0646 \u062c\u0646\u06af \u0633\u0631\u062f \u062c\u062f\u06cc\u062f \u062f\u0631 \u062f\u0646\u06cc\u0627 \u0646\u062f\u0627\u0631\u062f \u0648 \u0645\u0646 \u0646\u06cc\u0632 \u0641\u06a9\u0631 \u0646\u0645\u06cc\u200c\u06a9\u0646\u0645 \u0627\u06cc\u0646 \u062c\u0646\u06af \u062f\u0648\u0628\u0627\u0631\u0647 \u0645\u06cc\u0627\u0646 \u0631\u0648\u0633\u06cc\u0647 \u0648 \u063a\u0631\u0628 \u0648\u0627\u0642\u0639 \u0634\u0648\u062f.\u0631\u0626\u06cc\u0633\u200c\u062c\u0645\u0647\u0648\u0631 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647  \u0628\u0647 \u0646\u06af\u0631\u0627\u0646\u06cc \u06a9\u0634\u0648\u0631\u0634 \u0627\u0632 \u0631\u0633\u06cc\u062f\u0646 \u0646\u0627\u062a\u0648 \u0628\u0647 \u0645\u0631\u0632\u0647\u0627\u06cc \u0631\u0648\u0633\u06cc\u0647 \u06af\u0641\u062a: \u0686\u0647 \u06a9\u0633\u06cc \u0645\u06cc \u062a\u0648\u0627\u0646\u062f \u062a\u0636\u0645\u06cc\u0646 \u0628\u062f\u0647\u062f \u06a9\u0647 \u067e\u0633 \u0627\u0632 \u0627\u0646\u0642\u0644\u0627\u0628 \u0646\u0627\u0631\u0646\u062c\u06cc \u062c\u062f\u06cc\u062f \u2013 \u0631\u0648\u06cc \u06a9\u0627\u0631 \u0622\u0645\u062f\u0646 \u062f\u0648\u0644\u062a \u0645\u0648\u0642\u062a \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u2013 \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0646\u0627\u062a\u0648 \u0628\u0647 \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u0627\u0639\u0627\u0632\u0645 \u0646\u0634\u0648\u0646\u062f.\u0648\u06cc \u0628\u0627 \u0627\u0646\u062a\u0642\u0627\u062f \u062f\u0648\u0628\u0627\u0631\u0647 \u0627\u0632 \u062a\u062d\u0631\u06cc\u0645\u200c\u0647\u0627\u06cc \u063a\u0631\u0628 \u0639\u0644\u06cc\u0647 \u06a9\u0634\u0648\u0631\u0634 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u0627\u0644\u062d\u0627\u0642 \u0634\u0628\u0647 \u062c\u0632\u06cc\u0631\u0647 \u06a9\u0631\u06cc\u0645\u0647 \u0628\u0647 \u0631\u0648\u0633\u06cc\u0647\u060c \u0627\u06cc\u0646 \u062a\u062d\u0631\u06cc\u0645\u200c\u0647\u0627 \u0631\u0627 «\u063a\u06cc\u0631\u0633\u0627\u0632\u0646\u062f\u0647 \u0648 \u0628\u06cc \u0627\u0631\u062a\u0628\u0627\u0637 \u0628\u0647 \u0642\u0648\u0627\u0646\u06cc\u0646 \u0628\u06cc\u0646 \u0627\u0644\u0645\u0644\u0644» \u062e\u0648\u0627\u0646\u062f \u0648 \u062a\u0623\u06a9\u06cc\u062f \u06a9\u0631\u062f: \u0645\u0646\u0632\u0648\u06cc \u06a9\u0631\u062f\u0646 \u06a9\u0634\u0648\u0631\u06cc \u0686\u0648\u0646 \u0631\u0648\u0633\u06cc\u0647 \u063a\u06cc\u0631\u0645\u0645\u06a9\u0646 \u0627\u0633\u062a.\u0648\u0644\u0627\u062f\u06cc\u0645\u06cc\u0631 \u067e\u0648\u062a\u06cc\u0646 \u062f\u0631 \u0627\u06cc\u0646 \u06a9\u0646\u0641\u0631\u0627\u0646\u0633 \u062e\u0628\u0631\u06cc \u06a9\u0647 \u0628\u0627 \u062d\u0636\u0648\u0631 \u062e\u0628\u0631\u0646\u06af\u0627\u0631\u0627\u0646 \u0631\u0648\u0633 \u0648 \u0627\u0635\u062d\u0627\u0628 \u0645\u0637\u0628\u0648\u0639\u0627\u062a \u0627\u0632 \u062f\u06cc\u06af\u0631 \u06a9\u0634\u0648\u0631\u0647\u0627 \u0628\u0631\u06af\u0632\u0627\u0631 \u0634\u062f\u060c \u0628\u0627\u0631 \u062f\u06cc\u06af\u0631 \u0628\u0631 \u0622\u0645\u0627\u062f\u06af\u06cc \u06a9\u0634\u0648\u0631\u0634 \u0628\u0631\u0627\u06cc \u06af\u0641\u062a\u06af\u0648\u06cc \u0633\u0627\u0632\u0646\u062f\u0647 \u0628\u0627 \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u062f\u0631 \u062e\u0635\u0648\u0635 \u062a\u0639\u06cc\u06cc\u0646 \u0634\u0631\u0627\u06cc\u0637 \u067e\u0631\u062f\u0627\u062e\u062a \u0628\u062f\u0647\u06cc\u200c\u0647\u0627\u06cc \u06af\u0627\u0632\u06cc \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u0628\u0647 \u06a9\u0634\u0648\u0631\u0634 \u062a\u0623\u06a9\u06cc\u062f \u06a9\u0631\u062f.\u067e\u0648\u062a\u06cc\u0646 \u062f\u0631 \u062e\u0635\u0648\u0635 \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0631\u06cc\u0627\u0633\u062a \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u0648\u06a9\u0631\u0627\u06cc\u0646 \u06a9\u0647 \u0642\u0631\u0627\u0631 \u0627\u0633\u062a \u0641\u0631\u062f\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0628\u0631\u06af\u0632\u0627\u0631 \u0634\u0648\u062f \u06af\u0641\u062a: \u0631\u0648\u0633\u06cc\u0647 \u0646\u062a\u06cc\u062c\u0647 \u0627\u06cc\u0646 \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0631\u0627 \u0628\u0647 \u0631\u0633\u0645\u06cc\u062a \u062e\u0648\u0627\u0647\u062f \u0634\u0646\u0627\u062e\u062a.\u0631\u0626\u06cc\u0633\u200c\u062c\u0645\u0647\u0648\u0631 \u0631\u0648\u0633\u06cc\u0647 \u062a\u0623\u06a9\u06cc\u062f \u06a9\u0631\u062f\u060c \u0627\u06cc\u0646 \u0627\u0638\u0647\u0627\u0631\u0646\u0638\u0631\u0627\u062a \u06a9\u0647 \u0645\u0627 \u0645\u06cc\u200c\u062e\u0648\u0627\u06cc\u0645 \u0627\u062a\u062d\u0627\u062f \u062c\u0645\u0627\u0647\u06cc\u0631 \u0634\u0648\u0631\u0648\u06cc \u0631\u0627 \u0627\u062d\u06cc\u0627 \u06a9\u0646\u06cc\u0645\u060c \u062f\u0631\u0633\u062a \u0646\u06cc\u0633\u062a.\u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0628\u0647 \u0627\u062e\u062a\u0644\u0627\u0641\u0627\u062a \u0627\u0631\u0636\u06cc \u06a9\u0634\u0648\u0631\u0634 \u062e\u0648\u062f \u0628\u0627 \u0647\u0645\u0633\u0627\u06cc\u0647 \u0634\u0631\u0642\u06cc \u06cc\u0639\u0646\u06cc \u0698\u0627\u067e\u0646 \u067e\u0631\u062f\u0627\u062e\u062a \u0648 \u06af\u0641\u062a\u200c:\u0645\u0627 \u0628\u0631\u0627\u06cc \u06af\u0641\u062a\u200c\u0648\u06af\u0648 \u062f\u0631 \u062e\u0635\u0648\u0635 \u062c\u0632\u0627\u06cc\u0631 \u06a9\u0648\u0631\u06cc\u0644 \u0622\u0645\u0627\u062f\u0647\u200c\u0627\u06cc\u0645 \u0627\u0645\u0627 \u0645\u0637\u0645\u0626\u0646 \u0646\u06cc\u0633\u062a\u06cc\u0645 \u0637\u0631\u0641 \u0698\u0627\u067e\u0646\u06cc \u0622\u0645\u0627\u062f\u06af\u06cc \u0686\u0646\u06cc\u0646 \u0645\u0630\u0627\u06a9\u0631\u0627\u062a\u06cc \u0631\u0627 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f.
