p45
aV\u0641\u0627\u0631\u0633 - \u0627\u0645\u0627\u0645 \u062c\u0645\u0639\u0647 \u062e\u0627\u0631\u06af \u06af\u0641\u062a: \u0627\u0645\u0631\u0648\u0632\u0647 \u06cc\u06a9\u06cc \u0627\u0632 \u0645\u0633\u0627\u0626\u0644\u06cc \u06a9\u0647 \u0645\u0648\u062c\u0628 \u0627\u0641\u0632\u0627\u06cc\u0634 \u0637\u0644\u0627\u0642 \u0634\u062f\u0647\u060c \u0639\u062f\u0645 \u06af\u0630\u0634\u062a \u0632\u0648\u062c\u06cc\u0646 \u062f\u0631 \u0632\u0646\u062f\u06af\u06cc \u0632\u0646\u0627\u0634\u0648\u06cc\u06cc \u0627\u0633\u062a.\u062d\u062c\u062a\u200c\u0627\u0644\u0627\u0633\u0644\u0627\u0645 \u0645\u0648\u0633\u06cc \u06a9\u0645\u0627\u0644\u06cc \u062c\u0645\u0639\u0647 \u0634\u0628 \u062f\u0631 \u062f\u06cc\u062f\u0627\u0631 \u0642\u0634\u0631\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0645\u0631\u062f\u0645 \u062e\u0627\u0631\u06af \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0622\u06cc\u0627\u062a\u06cc \u062f\u0631 \u0645\u0648\u0631\u062f \u0637\u0644\u0627\u0642 \u0627\u0638\u0647\u0627\u0631 \u062f\u0627\u0634\u062a: \u0637\u0644\u0627\u0642 \u06cc\u06a9\u06cc \u0627\u0632 \u0645\u0648\u0627\u0631\u062f\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0648\u0642\u062a\u06cc \u0635\u0648\u0631\u062a \u0645\u06cc\u200c\u06af\u06cc\u0631\u062f\u060c \u0639\u0631\u0634 \u062e\u062f\u0627\u0648\u0646\u062f \u0628\u0647 \u0644\u0631\u0632\u0647 \u062f\u0631 \u0645\u06cc\u200c\u0622\u06cc\u062f.\u0648\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0627\u06cc\u0646\u06a9\u0647 \u0637\u0644\u0627\u0642 \u0647\u0645\u06cc\u0634\u0647 \u0622\u062e\u0631\u06cc\u0646 \u0631\u0627\u0647 \u062d\u0644 \u0645\u062d\u0633\u0648\u0628 \u0645\u06cc\u200c\u0634\u0648\u062f\u060c \u0627\u0641\u0632\u0648\u062f: \u0632\u0646\u062f\u06af\u06cc \u0647\u0645\u06cc\u0634\u0647 \u067e\u0631 \u0627\u0632 \u0633\u062e\u062a\u06cc\u200c\u0647\u0627 \u0648 \u0645\u0634\u06a9\u0644\u0627\u062a \u0627\u0633\u062a \u0648 \u0646\u0645\u06cc\u200c\u0634\u0648\u062f \u062f\u0631 \u0622\u0646 \u0633\u062e\u062a\u06cc \u0648 \u06af\u0631\u0641\u062a\u0627\u0631\u06cc \u0646\u0628\u0627\u0634\u062f\u060c \u0627\u0645\u0627 \u0628\u0631\u062e\u06cc \u0627\u0632 \u062c\u0648\u0627\u0646\u0627\u0646 \u062e\u06cc\u0627\u0644 \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u0627\u06cc\u0646 \u0645\u0634\u06a9\u0644\u0627\u062a \u0646\u06cc\u0633\u062a \u0648 \u0632\u0646\u062f\u06af\u06cc \u0628\u0627\u06cc\u062f \u0631\u0648\u06cc\u0627\u06cc\u06cc \u0628\u0627\u0634\u062f.\u0627\u0645\u0627\u0645 \u062c\u0645\u0639\u0647 \u062e\u0627\u0631\u06af \u062e\u0627\u0637\u0631\u0646\u0634\u0627\u0646 \u06a9\u0631\u062f: \u0628\u0627\u06cc\u062f \u0628\u0647 \u062c\u0648\u0627\u0646\u0627\u0646 \u062e\u0648\u062f \u0628\u06cc\u0627\u0645\u0648\u0632\u06cc\u0645 \u06a9\u0647 \u0632\u0646\u062f\u06af\u06cc \u0628\u062f\u0648\u0646 \u0645\u0634\u06a9\u0644 \u0648 \u0633\u062e\u062a\u06cc\u060c \u062a\u0646\u0647\u0627 \u062f\u0631 \u0628\u0647\u0634\u062a \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u0648 \u062f\u0631 \u0632\u0646\u062f\u06af\u06cc \u062f\u0646\u06cc\u0627\u06cc\u06cc \u0627\u06cc\u0646\u06af\u0648\u0646\u0647 \u0646\u06cc\u0633\u062a \u0648 \u0627\u06af\u0631 \u0632\u0645\u0627\u0646\u06cc \u0632\u0646\u062f\u06af\u06cc \u06a9\u0631\u062f\u0646 \u0628\u0627 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u0645\u0645\u06a9\u0646 \u0646\u0628\u0648\u062f\u060c \u0647\u0645\u0627\u0646 \u0637\u0648\u0631 \u06a9\u0647 \u062f\u0631 \u0642\u0631\u0622\u0646 \u06af\u0641\u062a\u0647 \u0634\u062f\u0647\u060c \u0628\u0647 \u062f\u0631\u0633\u062a\u06cc\u060c \u0628\u062f\u0648\u0646 \u062f\u0644\u062e\u0648\u0631\u06cc \u0648 \u06a9\u06cc\u0646\u0647 \u0627\u0632 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u062c\u062f\u0627 \u0634\u0648\u06cc\u0645.\u0627\u06cc\u0646 \u0645\u0633\u0626\u0648\u0644 \u0628\u06cc\u0627\u0646 \u062f\u0627\u0634\u062a: \u0645\u062a\u0627\u0633\u0641\u0627\u0646\u0647 \u0627\u0645\u0631\u0648\u0632\u0647 \u0645\u0633\u0626\u0644\u0647 \u0627\u0632\u062f\u0648\u0627\u062c \u0648 \u0637\u0644\u0627\u0642 \u0628\u0631\u0627\u06cc \u0628\u0631\u062e\u06cc \u0627\u0632 \u062e\u0627\u0646\u0645\u200c\u0647\u0627\u060c \u0645\u0627\u06cc\u0647 \u06a9\u0627\u0633\u0628\u06cc \u0634\u062f\u0647 \u0648 \u0645\u06cc\u200c\u06af\u0648\u06cc\u0646\u062f \u0628\u0627 \u0627\u06cc\u0646 \u0641\u0631\u062f \u0627\u0632\u062f\u0648\u0627\u062c \u0645\u06cc\u200c\u06a9\u0646\u06cc\u0645 \u0648 \u0627\u06af\u0631 \u062e\u0648\u0628 \u0646\u0628\u0648\u062f \u0645\u0647\u0631\u06cc\u0647 \u0631\u0627 \u0645\u06cc\u200c\u06af\u06cc\u0631\u06cc\u0645 \u0648 \u0645\u062c\u062f\u062f\u0627\u064b \u0627\u0632\u062f\u0648\u0627\u062c \u0645\u06cc\u200c\u06a9\u0646\u06cc\u0645 \u0648 \u0627\u06cc\u0646 \u0627\u0645\u0631 \u0645\u062f\u0627\u0645 \u062a\u06a9\u0631\u0627\u0631 \u0645\u06cc\u200c\u0634\u0648\u062f.\u06a9\u0645\u0627\u0644\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0627\u06cc\u0646\u06a9\u0647 \u0647\u0645\u06cc\u0634\u0647 \u0646\u0628\u0627\u06cc\u062f \u062a\u0641\u0627\u0647\u0645 \u06a9\u0627\u0645\u0644 \u0628\u06cc\u0646 \u0632\u0648\u062c\u06cc\u0646 \u0628\u0631\u0642\u0631\u0627\u0631 \u0628\u0627\u0634\u062f\u060c \u0639\u0646\u0648\u0627\u0646 \u06a9\u0631\u062f: \u0628\u0646\u062f\u0647 \u0647\u0645\u06cc\u0634\u0647 \u067e\u0633 \u0627\u0632 \u062e\u0648\u0627\u0646\u062f\u0646 \u0635\u06cc\u063a\u0647 \u0639\u0642\u062f \u0628\u0647 \u0632\u0648\u062c\u06cc\u0646 \u0645\u06cc\u200c\u06af\u0648\u06cc\u0645 \u06a9\u0647 \u062e\u06cc\u0627\u0644 \u0646\u06a9\u0646\u06cc\u062f \u0647\u0645\u06cc\u0634\u0647 \u0628\u0627\u06cc\u062f \u062a\u0641\u0627\u0647\u0645 \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u06cc\u062f \u0648 \u0627\u06af\u0631 \u06f5\u06f0\u060c \u06f5\u06f0 \u0628\u0648\u062f\u060c \u062e\u06cc\u0644\u06cc \u062e\u0648\u0628 \u0627\u0633\u062a \u0648 \u062d\u062a\u06cc \u062a\u0627 \u06f3\u06f0 \u0628\u0647 \u06f7\u06f0 \u0647\u0645 \u0645\u06cc\u200c\u0634\u0648\u062f \u0628\u0627 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u06a9\u0646\u0627\u0631 \u0622\u0645\u062f.
p46
aV\u0627\u06cc\u0631\u0646\u0627 - \u06cc\u06a9 \u0645\u0646\u0628\u0639 \u0622\u06af\u0627\u0647 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f\u060c \u0639\u0628\u062f\u0627\u0644\u0631\u0636\u0627 \u062f\u0627\u0648\u0631\u06cc \u06a9\u0647 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u06cc\u06a9\u06cc \u0627\u0632 \u0686\u0647\u0631\u0647\u200c\u0647\u0627\u06cc \u0627\u0635\u0644\u06cc \u0631\u0633\u0627\u0646\u0647 \u0627\u06cc \u0646\u0632\u062f\u06cc\u06a9 \u0628\u0647 \u062c\u0631\u06cc\u0627\u0646 \u0627\u0646\u062d\u0631\u0627\u0641\u06cc \u0628\u0647 \u0648\u06cc\u200d\u0698\u0647 \u062f\u0631 \u062f\u0648 \u0633\u0627\u0644 \u067e\u0627\u06cc\u0627\u0646\u06cc \u0639\u0645\u0631 \u062f\u0648\u0644\u062a \u062f\u0647\u0645 \u0634\u0646\u0627\u062e\u062a\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f\u060c \u0634\u0646\u0628\u0647 (\u0627\u0645\u0631\u0648\u0632) \u0628\u0631\u0627\u0633\u0627\u0633 \u0627\u062a\u0647\u0627\u0645\u0627\u062a \u0627\u0646\u062a\u0633\u0627\u0628 \u062f\u0631\u0648\u063a \u0628\u0647 \u0645\u0633\u0626\u0648\u0644\u0627\u0646 \u0648 \u0646\u0634\u0631 \u0627\u06a9\u0627\u0630\u06cc\u0628 \u0628\u0627\u0632\u062f\u0627\u0634\u062a \u0634\u062f.\u0627\u06cc\u0646 \u0645\u0646\u0628\u0639 \u067e\u06cc\u0634\u062a\u0631 \u0627\u0632 \u0635\u062f\u0648\u0631 \u062d\u06a9\u0645 \u062c\u0644\u0628 \u0628\u0631\u0627\u06cc \u062f\u0627\u0648\u0631\u06cc \u062e\u0628\u0631 \u062f\u0627\u062f\u0647 \u0628\u0648\u062f.\u0628\u0646\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u06af\u0632\u0627\u0631\u0634\u060c \u0648\u06cc \u062a\u0627\u06a9\u0646\u0648\u0646 \u0627\u0632 \u062d\u0636\u0648\u0631 \u062f\u0631 \u0645\u0631\u0627\u062c\u0639 \u0642\u0636\u0627\u06cc\u06cc \u062e\u0648\u062f\u062f\u0627\u0631\u06cc \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.\u062f\u0627\u0648\u0631\u06cc \u06a9\u0647 \u062f\u0631 \u0633\u0627\u0644\u200c\u0647\u0627\u06cc \u067e\u0627\u06cc\u0627\u0646 \u062f\u0648\u0644\u062a \u062f\u0647\u0645 \u0631\u06cc\u0627\u0633\u062a \u0635\u0646\u062f\u0648\u0642 \u0628\u0627\u0632\u0646\u0634\u0633\u062a\u06af\u06cc \u0645\u0633 \u06a9\u0634\u0648\u0631 \u0631\u0627 \u0628\u0631\u0639\u0647\u062f\u0647 \u062f\u0627\u0634\u062a\u060c \u062f\u0631 \u0637\u0648\u0644 \u062f\u0648\u0644\u062a \u0646\u0647\u0645 \u0648 \u062f\u0647\u0645 \u0627\u0632 \u0627\u0635\u0644\u06cc\u200c\u062a\u0631\u06cc\u0646 \u0686\u0647\u0631\u0647\u200c\u0647\u0627\u06cc \u0631\u0633\u0627\u0646\u0647 \u0627\u06cc \u0648\u0627\u0628\u0633\u062a\u0647 \u0628\u0647 \u062c\u0631\u06cc\u0627\u0646 \u062d\u0627\u0645\u06cc\u0627\u0646 \u0645\u0634\u0627\u06cc\u06cc \u0634\u0646\u0627\u062e\u062a\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u067e\u06cc\u0634 \u0627\u0632 \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0633\u0627\u0644 \u06f8\u06f8 \u0646\u06cc\u0632 \u0645\u0633\u0626\u0648\u0644\u06cc\u062a \u0627\u062f\u0627\u0631\u0647 \u06a9\u0644 \u067e\u0698\u0648\u0647\u0634 \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0645\u0639\u0627\u0648\u0646\u062a \u062e\u0628\u0631 \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0631\u0627 \u0628\u0631\u0639\u0647\u062f\u0647 \u062f\u0627\u0634\u062a.\u0648\u06cc \u062f\u0631 \u062c\u0631\u06cc\u0627\u0646 \u062b\u0628\u062a \u0646\u0627\u0645 \u0646\u0627\u0645\u0632\u062f\u0647\u0627 \u0628\u0631\u0627\u06cc \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0631\u06cc\u0627\u0633\u062a \u062c\u0645\u0647\u0648\u0631\u06cc \u062e\u0631\u062f\u0627\u062f \u0645\u0627\u0647 \u06f9\u06f2 \u062a\u0627 \u067e\u06cc\u0634 \u0627\u0632 \u0627\u0639\u0644\u0627\u0645 \u0635\u0644\u0627\u062d\u06cc\u062a\u200c\u0647\u0627\u060c \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u06cc\u06a9\u06cc \u0627\u0632 \u062d\u0627\u0645\u06cc\u0627\u0646 \u0627\u0635\u0644\u06cc \u0646\u0627\u0645\u0632\u062f \u0645\u0648\u0631\u062f \u067e\u0634\u062a\u06cc\u0628\u0627\u0646\u06cc \u0627\u062d\u0645\u062f\u06cc \u0646\u0698\u0627\u062f\u060c \u062f\u0631 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627 \u0641\u0639\u0627\u0644\u06cc\u062a \u062f\u0627\u0634\u062a.
p47
aV\u0645\u062f\u06cc\u0631\u0639\u0627\u0645\u0644 \u0645\u0631\u06a9\u0632 \u0641\u0631\u0647\u0646\u06af \u0648 \u0647\u0646\u0631 \u0633\u067e\u06cc\u062f \u062a\u0627\u0628\u0627\u0646 \u06a9\u0647 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u062a\u0648\u0642\u06cc\u0641 \u0634\u062f\u0647 \u0642\u0627\u0646\u0648\u0646 \u0632\u06cc\u0631\u0645\u062c\u0645\u0648\u0639\u0647 \u0627\u06cc\u0646 \u0645\u0631\u06a9\u0632 \u0627\u0633\u062a \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0622\u062e\u0631\u06cc\u0646 \u0627\u0642\u062f\u0627\u0645\u0627\u062a \u0635\u0648\u0631\u062a \u06af\u0631\u0641\u062a\u0647 \u0628\u0631\u0627\u06cc \u0631\u0641\u0639 \u062a\u0648\u0642\u06cc\u0641 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u06af\u0641\u062a: \u0647\u0641\u062a\u0647 \u06af\u0630\u0634\u062a\u0647 \u0628\u0627 \u062d\u0636\u0648\u0631 \u062f\u0631 \u062f\u0627\u062f\u0633\u0631\u0627 \u062a\u0641\u0647\u06cc\u0645 \u0627\u062a\u0647\u0627\u0645 \u0634\u062f\u06cc\u0645.\u0639\u0644\u06cc\u0631\u0636\u0627 \u0646\u06cc\u06a9\u0648\u06cc\u06cc \u0631\u0648\u0632 \u0634\u0646\u0628\u0647 \u062f\u0631\u06af\u0641\u062a \u0648 \u06af\u0648 \u0628\u0627 \u0627\u06cc\u0631\u0646\u0627 \u0627\u0638\u0647\u0627\u0631 \u06a9\u0631\u062f \u067e\u0633 \u0627\u0632 \u062d\u0636\u0648\u0631 \u062f\u0631 \u062f\u0627\u062f\u0633\u0631\u0627 \u062a\u0641\u0647\u06cc\u0645 \u0627\u062a\u0647\u0627\u0645 \u0634\u062f\u0647 \u0648 \u0627\u0632 \u0622\u0646 \u0631\u0648\u0632 \u0628\u0647 \u0628\u0639\u062f \u062e\u0628\u0631 \u062c\u062f\u06cc\u062f\u06cc \u062f\u0631 \u062f\u0633\u062a \u0646\u06cc\u0633\u062a.\u0648\u06cc \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u0646\u0634\u0631 \u0627\u06a9\u0627\u0630\u06cc\u0628 \u0627\u062a\u0647\u0627\u0645\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0628\u0647 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0642\u0627\u0646\u0648\u0646 \u0648\u0627\u0631\u062f \u0634\u062f\u0647\u060c \u0627\u062f\u0627\u0645\u0647 \u062f\u0627\u062f: \u0628\u0627\u06cc\u062f \u0645\u0646\u062a\u0638\u0631 \u0646\u0634\u0633\u062a\u200c\u0647\u0627\u06cc \u0628\u0639\u062f\u06cc \u0628\u0627\u0634\u06cc\u0645 \u062a\u0627 \u0628\u062a\u0648\u0627\u0646\u06cc\u0645 \u0627\u0632 \u0627\u062a\u0647\u0627\u0645 \u0648\u0627\u0631\u062f\u0647 \u062f\u0641\u0627\u0639 \u06a9\u0646\u06cc\u0645.\u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0642\u0627\u0646\u0648\u0646 \u06f1\u06f7 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a \u0645\u0627\u0647 \u0627\u0632 \u0633\u0648\u06cc \u062f\u0627\u062f\u0633\u062a\u0627\u0646\u06cc \u062a\u0647\u0631\u0627\u0646 \u062a\u0648\u0642\u06cc\u0641 \u0634\u062f.\u0686\u0627\u067e \u062e\u0628\u0631\u06cc \u062f\u0631 \u0645\u0648\u0631\u062f \u0622\u0632\u0627\u062f\u06cc \u06cc\u06a9\u06cc \u0627\u0632 \u0627\u0641\u0631\u0627\u062f \u0628\u0627\u0632\u062f\u0627\u0634\u062a \u0634\u062f\u0647 \u062f\u0631 \u067e\u0631\u0648\u0646\u062f\u0647 \u0633\u062a\u0627\u062f \u0633\u0648\u062e\u062a \u0639\u0644\u062a \u062a\u0648\u0642\u06cc\u0641 \u0627\u0639\u0644\u0627\u0645 \u0634\u062f\u0647 \u0628\u0648\u062f.
p48
aV\u0631\u0627\u0646\u0646\u062f\u06af\u06cc \u0628\u0627 \u062d\u0627\u0644\u062a \u0645\u0633\u062a\u06cc \u062c\u0627\u0646 \u06f5 \u0646\u0641\u0631 \u0631\u0627 \u062f\u0631 \u0645\u062d\u0648\u0631 \u062f\u0631\u06af\u0632- \u0642\u0648\u0686\u0627\u0646 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u06af\u0631\u0641\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0633\u0631\u0647\u0646\u06af \u0645\u062d\u0645\u062f \u0635\u0641\u0627\u06cc\u06cc\u060c \u0631\u0626\u06cc\u0633 \u067e\u0644\u06cc\u0633 \u0631\u0627\u0647 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u062f\u0631 \u0646\u0634\u0633\u062a \u062e\u0628\u0631\u06cc \u06a9\u0647 \u0627\u0645\u0631\u0648\u0632\u060c \u0633\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u062f\u0631 \u062f\u0641\u062a\u0631 \u0645\u0639\u0627\u0648\u0646\u062a \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0646\u0627\u062c\u0627 \u0628\u0631\u06af\u0632\u0627\u0631 \u0634\u062f\u060c \u0627\u0632 \u0648\u0642\u0648\u0639 \u062a\u0635\u0627\u062f\u0641\u06cc \u062f\u0631 \u0633\u0627\u0639\u062a \u06f1\u06f6 \u0631\u0648\u0632 \u062c\u0645\u0639\u0647\u060c \u062f\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u062f\u0631 \u062c\u0627\u062f\u0647 \u062a\u0628\u0627\u0631\u06a9 \u062f\u0631 \u0645\u062d\u0648\u0631 \u062f\u0631\u06af\u0632- \u0642\u0648\u0686\u0627\u0646 \u062e\u0628\u0631 \u062f\u0627\u062f.\u0648\u06cc \u062f\u0631 \u062a\u0634\u0631\u06cc\u062d \u0627\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u0627\u0638\u0647\u0627\u0631 \u06a9\u0631\u062f: \u062f\u0631 \u0627\u06cc\u0646 \u062a\u0635\u0627\u062f\u0641 \u06cc\u06a9 \u062f\u0633\u062a\u06af\u0627\u0647 \u062e\u0648\u062f\u0631\u0648\u06cc \u0647\u06cc\u0648\u0646\u062f\u0627 \u0622\u0632\u0631\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0645\u0633\u06cc\u0631 \u0628\u0627 \u0627\u0646\u062d\u0631\u0627\u0641 \u0628\u0647 \u0686\u067e \u0628\u0647 \u06cc\u06a9 \u062f\u0633\u062a\u06af\u0627\u0647 \u062e\u0648\u062f\u0631\u0648\u06cc \u067e\u0631\u0627\u06cc\u062f \u0628\u0647\u200c\u0635\u0648\u0631\u062a \u0631\u062e \u0628\u0647 \u0631\u062e \u0628\u0631\u062e\u0648\u0631\u062f \u06a9\u0631\u062f \u06a9\u0647 \u0628\u0631 \u0627\u062b\u0631 \u0622\u0646 \u062a\u0645\u0627\u0645\u06cc \u0633\u0631\u0646\u0634\u06cc\u0646\u0627\u0646 \u067e\u0631\u0627\u06cc\u062f \u062c\u0627\u0646 \u062e\u0648\u062f \u0631\u0627 \u0627\u0632 \u062f\u0633\u062a \u062f\u0627\u062f\u0646\u062f \u0627\u0645\u0627 \u0631\u0627\u0646\u0646\u062f\u0647 \u0648 \u0633\u0631\u0646\u0634\u06cc\u0646 \u0622\u0632\u0631\u0627 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u062a\u062c\u0647\u06cc\u0632 \u062e\u0648\u062f\u0631\u0648 \u0628\u0647 \u0633\u06cc\u0633\u062a\u0645 \u0627\u06cc\u0645\u0646\u06cc \u0648 \u0627\u06cc\u0631\u0628\u06af\u060c \u0635\u062f\u0645\u0647 \u0646\u062f\u06cc\u062f\u0646\u062f.\u0635\u0641\u0627\u06cc\u06cc \u0627\u0641\u0632\u0648\u062f: \u062a\u062d\u0642\u06cc\u0627\u062a \u0627\u0648\u0644\u06cc\u0647 \u067e\u0644\u06cc\u0633 \u0628\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u062f\u0633\u062a\u06af\u0627\u0647 \u0627\u0644\u06a9\u0644\u200c\u0633\u0646\u062c \u062d\u0627\u06a9\u06cc \u0627\u0632 \u0645\u0633\u062a \u0628\u0648\u062f\u0646 \u0631\u0627\u0646\u0646\u062f\u0647 \u0627\u0633\u062a \u0627\u0645\u0627 \u0627\u06cc\u0646 \u0645\u0633\u0627\u0644\u0647 \u0647\u0646\u0648\u0632 \u0628\u0637\u0648\u0631 \u0642\u0637\u0639 \u062b\u0627\u0628\u062a \u0646\u0634\u062f\u0647 \u0627\u0633\u062a \u0648 \u062a\u062d\u0642\u06cc\u0642\u0627\u062a \u0645\u0642\u0627\u0645\u0627\u062a \u0642\u0636\u0627\u06cc\u06cc \u0628\u0631 \u0631\u0648\u06cc \u0627\u06cc\u0646 \u067e\u0631\u0648\u0646\u062f\u0647 \u0627\u062f\u0627\u0645\u0647 \u062f\u0627\u0631\u062f.\u0631\u06cc\u06cc\u0633 \u067e\u0644\u06cc\u0633 \u0631\u0627\u0647 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u0627\u0632 \u06a9\u0627\u0647\u0634 \u06f3\u06f2 \u062f\u0631\u0635\u062f\u06cc \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u0627\u0633\u062a\u0627\u0646 \u062f\u0631 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a\u200c\u0645\u0627\u0647 \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u062e\u0628\u0631 \u062f\u0627\u062f \u0648 \u0627\u0641\u0632\u0648\u062f: \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u062f\u0631 \u0641\u0631\u0648\u0631\u062f\u06cc\u0646 \u0627\u0645\u0633\u0627\u0644 \u06a9\u0627\u0647\u0634 \u06f6 \u062f\u0631\u0635\u062f\u06cc \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a.\u0628\u0631 \u0627\u0633\u0627\u0633 \u0622\u0645\u0627\u0631 \u0645\u0631\u06a9\u0632 \u0641\u0631\u0645\u0627\u0646\u062f\u0647\u06cc \u0648 \u06a9\u0646\u062a\u0631\u0644 \u067e\u0644\u06cc\u0633 \u06f1\u06f1\u06f0\u060c \u062a\u0639\u062f\u0627\u062f \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u0627\u0633\u062a\u0627\u0646 \u062f\u0631 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a \u062a\u0627 \u0627\u06cc\u0646 \u0644\u062d\u0638\u0647 \u0628\u0627 \u06a9\u0627\u0647\u0634 \u06f3\u06f2 \u062f\u0631\u0635\u062f\u06cc \u0646\u0633\u0628\u062a \u0628\u0647 \u0645\u062f\u062a \u0645\u0634\u0627\u0628\u0647 \u0633\u0627\u0644 \u0642\u0628\u0644\u060c \u06f1\u06f9 \u0646\u0641\u0631 \u0627\u0639\u0644\u0627\u0645 \u0634\u062f\u0647 \u0627\u0633\u062a.\u0635\u0641\u0627\u06cc\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u062a\u0634\u062f\u06cc\u062f \u0627\u062c\u0631\u0627\u06cc \u0637\u0631\u062d \u0627\u0645\u0646\u06cc\u062a \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0648 \u0627\u062e\u0644\u0627\u0642\u06cc \u0646\u0627\u062c\u0627 \u0627\u0632 \u0627\u0648\u0644 \u062e\u0631\u062f\u0627\u062f \u062e\u0627\u0637\u0631\u0646\u0634\u0627\u0646 \u06a9\u0631\u062f: \u067e\u0644\u06cc\u0633\u200c\u0631\u0627\u0647 \u0628\u0647 \u0627\u06cc\u0646 \u0637\u0631\u062d \u0627\u0632 \u0628\u0639\u062f \u062a\u0631\u0627\u0641\u06cc\u06a9\u06cc \u0646\u06af\u0627\u0647 \u0645\u06cc\u200c\u06a9\u0646\u062f \u0648 \u0628\u0627 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u0627\u06cc\u062c\u0627\u062f \u0646\u0627\u0627\u0645\u0646\u06cc \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0648 \u0627\u062e\u0644\u0627\u0642\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u0628\u0631\u062e\u0648\u0631\u062f \u062e\u0648\u0627\u0647\u062f \u06a9\u0631\u062f.\u0648\u06cc \u0627\u0632 \u0628\u0631\u062e\u0648\u0631\u062f \u0648 \u0627\u0639\u0645\u0627\u0644 \u0642\u0627\u0646\u0648\u0646 \u0628\u0627 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u0633\u06af\u200c\u06af\u0631\u062f\u0627\u0646\u06cc \u06cc\u0627 \u0647\u0631 \u062d\u06cc\u0648\u0627\u0646 \u062f\u06cc\u06af\u0631\u06cc \u062f\u0631 \u0645\u0639\u0627\u0628\u0631 \u0634\u0647\u0631\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u062e\u0628\u0631 \u062f\u0627\u062f \u0648 \u06af\u0641\u062a: \u0627\u06cc\u0646 \u0627\u0641\u0631\u0627\u062f \u0648 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u062d\u06cc\u0646 \u0631\u0627\u0646\u0646\u062f\u06af\u06cc \u0641\u0631\u0632\u0646\u062f \u062e\u0648\u062f \u0631\u0627 \u062f\u0631 \u0622\u063a\u0648\u0634 \u0645\u06cc\u200c\u06af\u06cc\u0631\u0646\u062f\u060c \u062f\u0631 \u0631\u0627\u0647\u200c\u0647\u0627\u06cc \u0627\u0635\u0644\u06cc \u0648 \u06a9\u0644\u0627\u0646\u200c\u0634\u0647\u0631\u0647\u0627 \u06f5\u06f0 \u0647\u0632\u0627\u0631 \u062a\u0648\u0645\u0627\u0646 \u0648 \u062f\u0631 \u0634\u0647\u0631\u0647\u0627\u06cc \u06a9\u0648\u0686\u06a9 \u06f4\u06f0 \u0647\u0632\u0627\u0631 \u062a\u0648\u0645\u0627\u0646 \u062c\u0631\u06cc\u0645\u0647 \u062e\u0648\u0627\u0647\u0646\u062f \u0634\u062f.\u0635\u0641\u0627\u06cc\u06cc \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0628\u0648\u0642\u200c\u0647\u0627\u06cc \u0634\u06cc\u067e\u0648\u0631\u06cc \u062f\u0631 \u0628\u06cc\u0627\u0628\u0627\u0646 \u0645\u062c\u0627\u0632 \u0627\u0633\u062a\u060c \u0639\u0646\u0648\u0627\u0646 \u06a9\u0631\u062f: \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0628\u0648\u0642 \u0628\u06cc\u0627\u0628\u0627\u0646\u06cc \u062f\u0631 \u0634\u0647\u0631 \u0645\u0645\u0646\u0648\u0639 \u0627\u0633\u062a \u0648 \u062a\u062e\u0644\u0641 \u0645\u062d\u0633\u0648\u0628 \u0645\u06cc\u200c\u0634\u0648\u062f.
p49
aV\u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0639\u0631\u0628 \u0627\u0644\u06cc\u0648\u0645\u060c \u0686\u0627\u067e \u0627\u0645\u0627\u0646 \u062f\u0631 \u06af\u0632\u0627\u0631\u0634\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0627\u062c\u0644\u0627\u0633 \u0627\u062e\u06cc\u0631 \u0648\u0632\u06cc\u0631\u0627\u0646 \u062e\u0627\u0631\u062c\u0647 \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u0646\u0648\u0634\u062a\u060c \u0648\u0632\u06cc\u0631 \u062e\u0627\u0631\u062c\u0647 \u0639\u0645\u0627\u0646 \u0636\u0645\u0646 \u0627\u0646\u062a\u0642\u0627\u062f \u0634\u062f\u06cc\u062f \u0627\u0632 \u0639\u0631\u0628\u0633\u062a\u0627\u0646 \u0648 \u0642\u0637\u0631 \u06af\u0641\u062a: \u06a9\u0634\u0648\u0631 \u0645\u062a\u0628\u0648\u0639\u0634 \u0642\u0635\u062f \u0646\u062f\u0627\u0634\u062a\u0647 \u062f\u0631 \u0627\u06cc\u0646 \u0627\u062c\u0644\u0627\u0633 \u0634\u0631\u06a9\u062a \u06a9\u0646\u062f \u0686\u0648\u0646 \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u0628\u0647 \u0645\u0631\u062d\u0644\u0647 \u0641\u0631\u0648\u067e\u0627\u0634\u06cc \u0631\u0633\u06cc\u062f\u0647 \u0648 \u0627\u062c\u0644\u0627\u0633 \u0645\u0627 \u0633\u0648\u062f\u06cc \u0646\u062f\u0627\u0631\u062f \u0648 \u062a\u0627 \u06cc\u06a9 \u0628\u062d\u0631\u0627\u0646 \u062d\u0644 \u0646\u0634\u062f\u0647\u060c \u0628\u062d\u0631\u0627\u0646 \u062f\u06cc\u06af\u0631\u06cc \u0638\u0647\u0648\u0631 \u0645\u06cc\u200c\u06a9\u0646\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u06cc\u0648\u0633\u0641 \u0628\u0646 \u0639\u0644\u0648\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u0627\u062e\u062a\u0644\u0627\u0641 \u062f\u0631 \u0645\u0648\u0627\u0636\u0639 \u0633\u06cc\u0627\u0633\u06cc \u0639\u0645\u0627\u0646 \u0648 \u062f\u06cc\u06af\u0631 \u0627\u0639\u0636\u0627\u06cc \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u062e\u0644\u06cc\u062c \u0641\u0627\u0631\u0633 \u0627\u0641\u0632\u0648\u062f\u060c \u062d\u0627\u0641\u0638 \u0627\u0633\u062f \u0628\u0627 \u062a\u0647\u0631\u0627\u0646 \u0647\u0645\u200c\u067e\u06cc\u0645\u0627\u0646 \u0628\u0648\u062f \u0627\u0645\u0627 \u0647\u0631\u06af\u0632 \u062f\u0631 \u0645\u0633\u0627\u0626\u0644 \u062e\u0644\u06cc\u062c \u0641\u0627\u0631\u0633 \u062f\u062e\u0627\u0644\u062a \u0648 \u06cc\u0627 \u0627\u0632 \u062a\u0639\u0647\u062f\u0627\u062a \u062e\u0648\u062f \u06a9\u0648\u062a\u0627\u0647\u06cc \u0646\u06a9\u0631\u062f\u060c \u0628\u0647 \u0647\u0645\u06cc\u0646 \u0644\u062d\u0627\u0638 \u0645\u0644\u06a9 \u0641\u0647\u062f \u062f\u0631 \u067e\u0627\u0633\u062e \u0628\u0647 \u0634\u0628\u0647\u0647 \u0648 \u0627\u0646\u062a\u0642\u0627\u062f \u0634\u06cc\u062e \u062c\u0627\u0628\u0631 \u0639\u0644\u06cc\u0647 \u0627\u0633\u062f \u06af\u0641\u062a\u060c \u0631\u0648\u0627\u0628\u0637 \u0633\u0648\u0631\u06cc\u0647 \u0648 \u0627\u06cc\u0631\u0627\u0646 \u062f\u0631 \u0631\u0627\u0633\u062a\u0627\u06cc \u0645\u0646\u0627\u0641\u0639 \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u0627\u0633\u062a \u0648 \u0633\u0648\u0631\u06cc\u0647 \u067e\u0644 \u0627\u0631\u062a\u0628\u0627\u0637\u06cc \u0645\u0627 \u0648 \u0627\u06cc\u0631\u0627\u0646 \u0627\u0633\u062a.\u0628\u0647 \u06af\u0641\u062a\u0647 \u0627\u0648 \u0627\u0639\u0644\u0627\u0646 \u062c\u0646\u06af \u0639\u0644\u06cc\u0647 \u0633\u0648\u0631\u06cc\u0647 \u0647\u06cc\u0686 \u0646\u0641\u0639\u06cc \u0628\u0631\u0627\u06cc \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u0646\u062f\u0627\u0634\u062a\u0647 \u0648 \u0634\u0645\u0627 \u0645\u06cc\u200c\u062e\u0648\u0627\u0647\u06cc\u062f \u062f\u0627\u0639\u0634 \u0648 \u0627\u0644\u0646\u0635\u0631\u0647 \u062f\u0631 \u067e\u0627\u06cc\u062a\u062e\u062a\u200c\u0647\u0627\u06cc \u062e\u0644\u06cc\u062c \u0641\u0627\u0631\u0633 \u062d\u0636\u0648\u0631 \u06cc\u0627\u0628\u0646\u062f\u061f\u0648\u06cc \u0627\u0641\u0632\u0648\u062f \u062d\u0645\u0627\u06cc\u062a \u0634\u0645\u0627 \u0627\u0632 \u062f\u0627\u0639\u0634 \u062f\u0631 \u0639\u0631\u0627\u0642 \u0648 \u062a\u0631\u0648\u0631\u06cc\u0633\u062a\u200c\u0647\u0627 \u062f\u0631 \u0635\u062d\u0631\u0627\u06cc \u0633\u06cc\u0646\u0627 \u0686\u0647 \u0646\u0641\u0639\u06cc \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a\u061f\u0628\u0646 \u0639\u0644\u0648\u06cc \u0627\u0642\u062a\u062f\u0627\u0631 \u0645\u0635\u0631 \u0648 \u0633\u0648\u0631\u06cc\u0647 \u0631\u0627 \u0628\u0647 \u0646\u0641\u0639 \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u062f\u0627\u0646\u0633\u062a.\u06cc\u0646 \u0639\u0644\u0648\u06cc \u0627\u0641\u0632\u0648\u062f: \u0628\u0631\u062e\u06cc \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u062e\u0644\u06cc\u062c \u0641\u0627\u0631\u0633 \u0641\u06a9\u0631 \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u0622\u0645\u0631\u06cc\u06a9\u0627 \u0648 \u0646\u0627\u062a\u0648 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0622\u0646\u200c\u0647\u0627 \u062f\u0633\u062a \u0628\u0647 \u0627\u0642\u062f\u0627\u0645 \u0645\u06cc\u200c\u0632\u0646\u0646\u062f \u0648 \u0647\u0645\u06cc\u0646 \u062a\u0641\u06a9\u0631 \u0634\u0648\u0631\u0627\u06cc \u0647\u0645\u06a9\u0627\u0631\u06cc \u0631\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u0645\u0631\u062d\u0644\u0647 \u0627\u0632 \u0633\u0642\u0648\u0637 \u0631\u0633\u0627\u0646\u062f\u0647 \u0632\u06cc\u0631\u0627 \u0622\u0646\u200c\u0647\u0627 \u062f\u0631\u06a9 \u0646\u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u06a9\u0647 \u0622\u0645\u0631\u06cc\u06a9\u0627 \u062f\u0646\u0628\u0627\u0644 \u0645\u0646\u0627\u0641\u0639 \u062e\u0648\u062f \u0627\u0633\u062a \u0648 \u062a\u0648\u062c\u0647\u06cc \u0628\u0647 \u0645\u0646\u0627\u0641\u0639 \u0645\u0627 \u0646\u062f\u0627\u0631\u062f\u060c \u0628\u0646\u0627\u0628\u0631\u0627\u06cc\u0646 \u0632\u0645\u0627\u0646\u06cc \u06a9\u0647 \u06a9\u0627\u0631 \u0633\u06cc\u0627\u0633\u06cc \u0628\u0627 \u0627\u06cc\u0631\u0627\u0646 \u0631\u0627 \u0634\u0631\u0648\u0639 \u06a9\u0631\u062f\u0646\u062f \u0627\u0632 \u0645\u0627 \u0633\u0648\u0627\u0644 \u0646\u06a9\u0631\u062f\u0646\u062f.
p50
aV\u0645\u0634\u0627\u0648\u0631 \u062c\u0646\u062c\u0627\u0644\u06cc \u0627\u0631\u062f\u0648\u063a\u0627\u0646 \u06a9\u0647 \u0627\u062e\u06cc\u0631\u0627 \u06cc\u06a9 \u0645\u0639\u062a\u0631\u0636 \u0636\u062f \u062f\u0648\u0644\u062a\u06cc \u0631\u0627 \u0628\u0647 \u0628\u0627\u062f \u06a9\u062a\u06a9 \u06af\u0631\u0641\u062a\u0647 \u0628\u0648\u062f\u060c \u0627\u062e\u0631\u0627\u062c \u0634\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0628\u0647 \u0646\u0642\u0644 \u0627\u0632 \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u0631\u0648\u06cc\u062a\u0631\u0632\u060c \u06cc\u0648\u0633\u0641 \u06cc\u0631\u06a9\u0644\u060c \u06cc\u06a9\u06cc \u0627\u0632 \u0645\u0634\u0627\u0648\u0631\u0627\u0646 \u0631\u062c\u0628 \u0637\u06cc\u0628 \u0627\u0631\u062f\u0648\u063a\u0627\u0646\u060c \u0646\u062e\u0633\u062a\u200c\u0648\u0632\u06cc\u0631 \u062a\u0631\u06a9\u06cc\u0647 \u067e\u0633 \u0627\u0632 \u0627\u0639\u062a\u0631\u0627\u0636\u0627\u062a \u0645\u0631\u062f\u0645\u06cc \u0639\u0644\u06cc\u0647 \u062d\u0627\u062f\u062b\u0647 \u062e\u0648\u0646\u0628\u0627\u0631 \u0645\u0639\u062f\u0646 \u0633\u0648\u0645\u0627 \u0628\u0627 \u06f3\u06f0\u06f1 \u06a9\u0634\u062a\u0647\u060c \u06cc\u06a9\u06cc \u0627\u0632 \u0645\u0639\u062a\u0631\u0636\u0627\u0646 \u0631\u0627 \u06a9\u062a\u06a9 \u0632\u062f \u06a9\u0647 \u0641\u06cc\u0644\u0645 \u0627\u0642\u062f\u0627\u0645 \u0648\u06cc \u0628\u0647 \u0633\u0631\u0639\u062a \u062f\u0631 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627 \u0645\u0646\u062a\u0634\u0631 \u0634\u062f.\u0627\u0642\u062f\u0627\u0645 \u0648\u06cc \u0628\u0631\u0627\u06cc \u062f\u0648\u0644\u062a \u0627\u0631\u062f\u0648\u063a\u0627\u0646 \u06a9\u0647 \u062e\u0648\u062f \u0628\u0627 \u0686\u0627\u0644\u0634\u200c\u0647\u0627\u06cc \u0628\u06cc\u0634\u0645\u0627\u0631\u06cc \u0645\u0648\u0627\u062c\u0647 \u0627\u0633\u062a \u0628\u0647 \u0633\u0631\u0639\u062a \u062f\u0631 \u0633\u0631\u0627\u0633\u0631 \u062c\u0647\u0627\u0646 \u062e\u0628\u0631\u0633\u0627\u0632 \u0634\u062f \u0648 \u062f\u0631\u062f\u0633\u0631 \u062a\u0627\u0632\u0647\u200c\u0627\u06cc \u0628\u0631\u0627\u06cc \u0648\u06cc \u0631\u0642\u0645 \u0632\u062f.\u062a\u0635\u0627\u0648\u06cc\u0631 \u0645\u0646\u062a\u0634\u0631 \u0634\u062f\u0647 \u0627\u0632 \u0627\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u0646\u0634\u0627\u0646 \u0645\u06cc\u200c\u062f\u0647\u062f \u06a9\u0647 \u0627\u06cc\u0646 \u0641\u0631\u062f \u0631\u0648\u06cc \u0632\u0645\u06cc\u0646 \u0627\u0641\u062a\u0627\u062f\u0647 \u0648 \u062f\u0648 \u0646\u06cc\u0631\u0648\u06cc \u0627\u0645\u0646\u06cc\u062a\u06cc \u0648\u06cc \u0631\u0627 \u0646\u06af\u0647 \u062f\u0627\u0634\u062a\u0647\u200c\u0627\u0646\u062f \u0648 \u062f\u0631 \u0647\u0645\u0627\u0646 \u0647\u0646\u06af\u0627\u0645 \u0627\u06cc\u0646 \u0645\u0634\u0627\u0648\u0631 \u0627\u0631\u062f\u0648\u063a\u0627\u0646 \u0628\u0647 \u0648\u06cc \u0644\u06af\u062f \u0645\u06cc\u200c\u0632\u0646\u062f.\u0627\u0642\u062f\u0627\u0645 \u0648\u06cc \u0628\u0627 \u0648\u0627\u06a9\u0646\u0634 \u0634\u062f\u06cc\u062f \u0645\u0631\u062f\u0645 \u062a\u0631\u06a9\u06cc\u0647 \u0631\u0648\u0628\u0631\u0648 \u0634\u062f \u06a9\u0647 \u0627\u0632 \u0633\u06cc\u0627\u0633\u062a\u200c\u0647\u0627\u06cc \u0627\u0631\u062f\u0648\u063a\u0627\u0646 \u062f\u0631 \u0645\u0648\u0627\u062c\u0647 \u0628\u0627 \u062d\u0627\u062f\u062b\u0647 \u0645\u0639\u062f\u0646 \u0627\u0646\u062a\u0642\u0627\u062f \u0645\u06cc\u200c\u06a9\u0631\u062f\u0646\u062f.\u062f\u0631 \u067e\u0627\u06cc\u0627\u0646 \u06af\u0641\u062a\u06af\u0648 \u062f\u0631 \u0627\u06cc\u0646 \u0628\u0627\u0631\u0647\u060c \u0627\u0631\u062f\u0648\u063a\u0627\u0646 \u0634\u062e\u0635\u0627 \u0648\u06cc \u0631\u0627 \u0627\u062e\u0631\u0627\u062c \u06a9\u0631\u062f.
p51
aV\u0647\u0645\u0633\u0631 \u0645\u0647\u200c\u0622\u0641\u0631\u06cc\u062f \u0627\u0645\u06cc\u0631\u062e\u0633\u0631\u0648\u06cc \u062c\u0632\u0626\u06cc\u0627\u062a \u0622\u062e\u0631\u06cc\u0646 \u0645\u0644\u0627\u0642\u0627\u062a \u0628\u0627 \u0647\u0645\u0633\u0631\u0634 \u0648 \u062d\u0631\u0641\u200c\u0647\u0627\u06cc \u0648\u06cc \u0631\u0627 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0645\u0647\u0631\u060c \u0633\u0627\u0631\u0627 \u062e\u0633\u0631\u0648\u06cc \u06af\u0641\u062a: \u0627\u0645\u0631\u0648\u0632 \u0635\u0628\u062d \u0627\u0632 \u0637\u0631\u06cc\u0642 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627 \u0645\u0637\u0644\u0639 \u0634\u062f\u06cc\u0645 \u06a9\u0647 \u062d\u06a9\u0645 \u0627\u0639\u062f\u0627\u0645 \u0647\u0645\u0633\u0631\u0645 \u062f\u0631 \u0632\u0646\u062f\u0627\u0646 \u0627\u062c\u0631\u0627 \u0634\u062f\u0647 \u0627\u0633\u062a.\u062f\u0631 \u0627\u0628\u062a\u062f\u0627 \u062a\u0635\u0648\u0631 \u0645\u06cc\u200c\u06a9\u0631\u062f\u06cc\u0645 \u0627\u06cc\u0646 \u0645\u0648\u0636\u0648\u0639 \u0635\u062d\u062a \u0646\u062f\u0627\u0631\u062f\u060c \u0627\u0645\u0627 \u0648\u06a9\u06cc\u0644\u0645\u0627\u0646 \u0628\u0627 \u067e\u06cc\u06af\u06cc\u0631\u06cc \u0645\u0627\u062c\u0631\u0627 \u0648 \u0645\u0634\u0627\u0647\u062f\u0647 \u062c\u0633\u062f \u0645\u0647\u200c\u0622\u0641\u0631\u06cc\u062f \u0628\u0647 \u0645\u0627 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f \u062d\u06a9\u0645 \u0627\u062c\u0631\u0627 \u0634\u062f\u0647 \u0627\u0633\u062a.\u0647\u0645\u0633\u0631 \u0645\u0647\u200c\u0622\u0641\u0631\u06cc\u062f \u062e\u0633\u0631\u0648\u06cc \u062f\u0631 \u062e\u0635\u0648\u0635 \u0622\u062e\u0631\u06cc\u0646 \u0645\u0644\u0627\u0642\u0627\u062a \u0628\u0627 \u0647\u0645\u0633\u0631\u0634 \u06af\u0641\u062a: \u062f\u06cc\u0631\u0648\u0632 \u0628\u0647 \u0645\u0627 \u0627\u062c\u0627\u0632\u0647 \u062f\u0627\u062f\u0646\u062f \u06a9\u0647 \u062f\u0631 \u0645\u0642\u0627\u0628\u0644 \u062f\u0627\u062f\u0633\u0631\u0627 \u0628\u0627 \u0645\u0647\u200c\u0622\u0641\u0631\u06cc\u062f \u062f\u06cc\u062f\u0627\u0631 \u06a9\u0646\u06cc\u0645.\u062f\u0631 \u0627\u06cc\u0646 \u062f\u06cc\u062f\u0627\u0631 \u0631\u0648\u062d\u06cc\u0647 \u0647\u0645\u0633\u0631\u0645 \u0645\u062b\u0644 \u0647\u0645\u06cc\u0634\u0647 \u062e\u0648\u0628 \u0628\u0648\u062f \u0648 \u0646\u0645\u06cc\u200c\u062f\u0627\u0646\u0633\u062a \u06a9\u0647 \u0641\u0631\u062f\u0627 \u0642\u0631\u0627\u0631 \u0627\u0633\u062a \u062d\u06a9\u0645 \u0645\u062c\u0627\u0632\u0627\u062a\u0634 \u0627\u062c\u0631\u0627 \u0634\u0648\u062f.\u0627\u0648 \u062f\u0631 \u0627\u06cc\u0646 \u062f\u06cc\u062f\u0627\u0631 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f \u0633\u0647 \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0631\u0627 \u0645\u0637\u0631\u062d \u06a9\u0631\u062f\u0647 \u06a9\u0647 \u06cc\u06a9\u06cc \u0627\u0632 \u0627\u06cc\u0646 \u062f\u0631\u062e\u0648\u0627\u0633\u062a\u200c\u0647\u0627 \u0627\u06cc\u0646 \u0628\u0648\u062f \u06a9\u0647 \u0628\u0631\u0627\u06cc\u0634 \u062a\u0644\u0641\u0646 \u0641\u0631\u0627\u0647\u0645 \u06a9\u0631\u062f\u0647 \u062a\u0627 \u0627\u0648 \u0628\u062a\u0648\u0627\u0646\u062f \u0628\u0627 \u062a\u0644\u0641\u0646 \u0635\u062d\u0628\u062a \u06a9\u0646\u062f.\u06a9\u0627\u0634 \u0647\u0645\u0633\u0631\u0645 \u0645\u06cc\u200c\u062f\u0627\u0646\u0633\u062a \u06a9\u0647 \u0627\u06cc\u0646 \u0622\u062e\u0631\u06cc\u0646 \u0645\u0644\u0627\u0642\u0627\u062a \u0627\u0633\u062a \u062a\u0627 \u0627\u06af\u0631 \u0648\u0635\u06cc\u062a\u06cc \u062f\u0627\u0634\u062a\u060c \u0622\u0646 \u0631\u0627 \u0628\u0647 \u0645\u0627 \u0645\u06cc\u200c\u06af\u0641\u062a.
p52
aV\u06cc\u06a9 \u0639\u0636\u0648 \u06a9\u0627\u0631\u06af\u0631\u0648\u0647 \u062a\u0639\u06cc\u06cc\u0646 \u0645\u0635\u0627\u062f\u06cc\u0642 \u0645\u062d\u062a\u0648\u0627\u06cc \u0645\u062c\u0631\u0645\u0627\u0646\u0647 \u0627\u0632 \u062f\u0633\u062a\u0648\u0631 \u062f\u0627\u062f\u0633\u062a\u0627\u0646 \u06a9\u0644 \u06a9\u0634\u0648\u0631 \u0628\u0647 \u0648\u0632\u0627\u0631\u062a \u0627\u0631\u062a\u0628\u0627\u0637\u0627\u062a \u0645\u0628\u0646\u06cc \u0628\u0631 \u0641\u06cc\u0644\u062a\u0631\u06cc\u0646\u06af \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645 \u0627\u0628\u0631\u0627\u0632 \u0628\u06cc\u200c\u0627\u0637\u0644\u0627\u0639\u06cc \u06a9\u0631\u062f \u0648 \u06af\u0641\u062a \u06a9\u0647 \u0627\u062d\u062a\u0645\u0627\u0644\u0627 \u0627\u06cc\u0646 \u0645\u0648\u0636\u0648\u0639 \u062f\u0631 \u062c\u0644\u0633\u0647 \u0647\u0641\u062a\u0647 \u0622\u06cc\u0646\u062f\u0647 \u06a9\u0627\u0631\u06af\u0631\u0648\u0647 \u062f\u0631 \u062f\u0633\u062a\u0648\u0631 \u06a9\u0627\u0631 \u0642\u0631\u0627\u0631 \u062e\u0648\u0627\u0647\u062f \u06af\u0631\u0641\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627 \u0631\u0645\u0636\u0627\u0646\u0639\u0644\u06cc \u0633\u0628\u062d\u0627\u0646\u06cc\u200c\u0641\u0631\u062f \u06af\u0641\u062a: \u062f\u0631 \u062c\u0644\u0633\u0647\u200c\u0647\u0627\u06cc \u06af\u0630\u0634\u062a\u0647 \u06a9\u0627\u0631\u06af\u0631\u0648\u0647 \u062a\u0639\u06cc\u06cc\u0646 \u0645\u0635\u0627\u062f\u06cc\u0642 \u0645\u062d\u062a\u0648\u0627\u06cc \u0645\u062c\u0631\u0645\u0627\u0646\u0647 \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0628\u0631\u0631\u0633\u06cc \u0641\u06cc\u0644\u062a\u0631\u06cc\u0646\u06af \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645 \u062a\u0634\u06a9\u06cc\u0644 \u0634\u062f\u0647 \u0628\u0648\u062f\u060c \u062f\u0631 \u0622\u0631\u0627\u06cc \u0645\u0627\u062e\u0648\u0630\u0647 \u0627\u0632 \u0627\u0639\u0636\u0627 \u0628\u0631\u0627\u06cc \u0641\u06cc\u0644\u062a\u0631\u06cc\u0646\u06af \u0627\u06cc\u0646 \u0646\u0631\u0645\u200c\u0627\u0641\u0632\u0627\u0631 \u062a\u0634\u06a9\u06cc\u06a9 \u0634\u062f \u0648 \u0646\u062a\u06cc\u062c\u0647 \u0627\u06cc\u0646 \u0634\u062f \u06a9\u0647 \u0627\u06cc\u0646 \u0646\u0631\u0645\u200c\u0627\u0641\u0632\u0627\u0631 \u0641\u06cc\u0644\u062a\u0631 \u0646\u0634\u062f.\u0627\u0648 \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u06a9\u0627\u0631\u06af\u0631\u0648\u0647 \u062f\u0631 \u0647\u0641\u062a\u0647 \u062c\u0627\u0631\u06cc \u0628\u0647 \u062f\u0644\u06cc\u0644 \u062a\u0639\u0637\u06cc\u0644\u06cc \u0645\u0628\u0639\u062b \u062c\u0644\u0633\u0647\u200c\u0627\u06cc \u0646\u062f\u0627\u0631\u062f\u060c \u06cc\u0627\u062f\u0622\u0648\u0631 \u0634\u062f: \u0627\u062d\u062a\u0645\u0627\u0644\u0627 \u0646\u0627\u0645\u0647 \u062f\u0627\u062f\u0633\u062a\u0627\u0646 \u0628\u0631\u0627\u06cc \u0641\u06cc\u0644\u062a\u0631\u06cc\u0646\u06af \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645 \u062f\u0631 \u062f\u0633\u062a\u0648\u0631 \u06a9\u0627\u0631 \u062c\u0644\u0633\u0647 \u0647\u0641\u062a\u0647 \u0628\u0639\u062f \u06a9\u0627\u0631\u06af\u0631\u0648\u0647 \u062a\u0639\u06cc\u06cc\u0646 \u0645\u0635\u0627\u062f\u06cc\u0642 \u0645\u062d\u062a\u0648\u0627\u06cc \u0645\u062c\u0631\u0645\u0627\u0646\u0647 \u0642\u0631\u0627\u0631 \u0645\u06cc\u200c\u06af\u06cc\u0631\u062f \u0648 \u062f\u0631 \u0622\u0646 \u062c\u0644\u0633\u0647 \u062f\u0631 \u0627\u06cc\u0646 \u0628\u0627\u0631\u0647 \u062a\u0635\u0645\u06cc\u0645 \u06af\u06cc\u0631\u06cc \u062e\u0648\u0627\u0647\u062f \u0634\u062f.\u0646\u0645\u0627\u06cc\u0646\u062f\u0647 \u0645\u0631\u062f\u0645 \u0633\u0628\u0632\u0648\u0627\u0631 \u062f\u0631 \u0645\u062c\u0644\u0633 \u0647\u0645\u0686\u0646\u06cc\u0646 \u06af\u0641\u062a \u06a9\u0647 \u0648\u06cc \u067e\u06cc\u0634\u062a\u0631 \u0631\u0627\u06cc \u0628\u0647 \u0641\u06cc\u0644\u062a\u0631\u06cc\u0646\u06af \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645 \u0646\u062f\u0627\u062f\u0647 \u0628\u0648\u062f.
p53
aV\u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u02c8\u0627\u06cc\u0646\u062f\u06cc\u067e\u0646\u062f\u0646\u062a\u02c8 \u0646\u0648\u0634\u062a\u060c \u0628\u0627 \u06af\u0631\u0645 \u0634\u062f\u0646 \u0631\u0648\u0627\u0628\u0637 \u0627\u06cc\u0631\u0627\u0646 \u0628\u0627 \u06a9\u0634\u0648\u0631\u0647\u0627\u06cc \u062e\u0627\u0631\u062c\u06cc \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u062e\u0627\u0631\u062c\u06cc \u0628\u0647 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0647\u062c\u0648\u0645 \u0622\u0648\u0631\u062f\u0647 \u0627\u0646\u062f \u0648 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u062e\u0648\u062f \u0631\u0627 \u0622\u0645\u0627\u062f\u0647 \u0647\u062c\u0648\u0645 \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u063a\u0631\u0628\u06cc \u0645\u06cc\u200c\u06a9\u0646\u062f.\u02c8\u06af\u0644\u0646\u0627\u0631 \u0645\u062a\u0648\u0644\u06cc\u02c8 \u062e\u0628\u0631\u0646\u06af\u0627\u0631 \u0627\u06cc\u0646 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0627\u0646\u06af\u0644\u06cc\u0633\u06cc \u0632\u0628\u0627\u0646 \u062f\u0631 \u06af\u0632\u0627\u0631\u0634\u06cc \u062f\u0631 \u0627\u06cc\u0646 \u0632\u0645\u06cc\u0646\u0647 \u0627\u0641\u0632\u0648\u062f: \u0647\u062c\u0648\u0645 \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u062e\u0627\u0631\u062c\u06cc \u0645\u0644\u0645\u0648\u0633\u200c\u062a\u0631\u06cc\u0646 \u062a\u0627\u062b\u06cc\u0631 \u062a\u0644\u0627\u0634\u200c\u0647\u0627\u06cc \u02c8\u062d\u0633\u0646 \u0631\u0648\u062d\u0627\u0646\u06cc\u02c8 \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0628\u0631\u0627\u06cc \u062a\u0631\u0645\u06cc\u0645 \u0631\u0648\u0627\u0628\u0637 \u06a9\u0634\u0648\u0631\u0634 \u0628\u0627 \u0622\u0645\u0631\u06cc\u06a9\u0627 \u0648 \u0627\u0631\u0648\u067e\u0627 \u0627\u0633\u062a.\u0627\u06cc\u0646 \u062f\u0631 \u062d\u0627\u0644\u06cc\u0633\u062a \u06a9\u0647 \u0632\u0646\u062c\u06cc\u0631\u0647 \u0647\u062a\u0644\u200c\u0647\u0627\u06cc \u0628\u06cc\u0646 \u0627\u0644\u0645\u0644\u0644\u06cc \u062e\u0648\u062f \u0631\u0627 \u0622\u0645\u0627\u062f\u0647 \u0641\u0639\u0627\u0644\u06cc\u062a \u062f\u0648\u0628\u0627\u0631\u0647 \u062f\u0631 \u0627\u06cc\u0631\u0627\u0646 \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u0648 \u062e\u0637\u0648\u0637 \u0647\u0648\u0627\u06cc\u06cc \u0627\u0631\u0648\u067e\u0627\u06cc\u06cc \u0628\u0647 \u062f\u0646\u0628\u0627\u0644 \u0627\u0632 \u0633\u0631\u06af\u06cc\u0631\u06cc \u0631\u0648\u0627\u0628\u0637 \u0634\u0627\u0646 \u0628\u0627 \u062a\u0647\u0631\u0627\u0646 \u0647\u0633\u062a\u0646\u062f.\u02c8\u0645\u0631\u062a\u0636\u06cc \u0631\u062d\u0645\u0627\u0646\u06cc \u0645\u0648\u062d\u062f\u06cc\u02c8 \u0645\u0639\u0627\u0648\u0646 \u0633\u0627\u0632\u0645\u0627\u0646 \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u0648 \u0645\u06cc\u0631\u0627\u062b \u0641\u0631\u0647\u0646\u06af\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0628\u0647 \u062a\u0627\u0632\u06af\u06cc \u06af\u0641\u062a: \u062f\u0631 \u0641\u0627\u0635\u0644\u0647 \u0632\u0645\u0627\u0646\u06cc \u0628\u06cc\u0646 \u0627\u0648\u0644 \u062a\u0627 \u06f3\u06f1 \u0641\u0631\u0648\u0631\u062f\u06cc\u0646 \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u06f4 \u0647\u0632\u0627\u0631 \u0648 \u06f5\u06f9\u06f4 \u06af\u0631\u0648\u0647 \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u062e\u0627\u0631\u062c\u06cc \u0627\u0632 \u0627\u06cc\u0631\u0627\u0646 \u062f\u06cc\u062f\u0646 \u06a9\u0631\u062f\u0647 \u0627\u0646\u062f \u06a9\u0647 \u0628\u06cc\u0634 \u0627\u0632 \u062f\u0648 \u0628\u0631\u0627\u0628\u0631 \u062c\u0645\u0639\u06cc\u062a \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0646\u0646\u062f\u0647 \u062f\u0631 \u0647\u0645\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0627\u0632 \u0633\u0627\u0644 \u0642\u0628\u0644 \u0627\u0633\u062a.\u0627\u06cc\u0631\u0627\u0646 \u0628\u0647 \u062f\u0646\u0628\u0627\u0644 \u0622\u0633\u0627\u0646\u200c\u062a\u0631 \u06a9\u0631\u062f\u0646 \u0634\u0631\u0627\u06cc\u0637 \u062f\u0631\u06cc\u0627\u0641\u062a \u0631\u0648\u0627\u062f\u06cc\u062f \u0628\u0631\u0627\u06cc \u06f1\u06f2 \u06a9\u0634\u0648\u0631 \u0628\u0627 \u0647\u062f\u0641 \u062c\u0630\u0628 \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u0628\u06cc\u0634\u062a\u0631 \u0627\u0633\u062a.\u06af\u0631\u0648\u0647 \u0631\u0648\u062a\u0627\u0646\u0627 (Rotana Group) \u0639\u0631\u0628\u0633\u062a\u0627\u0646 \u0642\u0635\u062f \u062f\u0627\u0631\u062f \u0645\u062c\u0645\u0648\u0639\u0647 \u0647\u062a\u0644\u200c\u0647\u0627\u06cc \u067e\u0646\u062c \u0633\u062a\u0627\u0631\u0647 \u0631\u0627 \u062f\u0631 \u062a\u0647\u0631\u0627\u0646 \u0648 \u0634\u0647\u0631 \u0645\u0630\u0647\u0628\u06cc \u0645\u0634\u0647\u062f \u062a\u0627\u0633\u06cc\u0633 \u06a9\u0646\u062f.\u062e\u0637\u0648\u0637 \u0647\u0648\u0627\u06cc\u06cc \u0627\u062a\u0631\u06cc\u0634 \u0646\u06cc\u0632 \u067e\u0631\u0648\u0627\u0632\u0647\u0627\u06cc \u0645\u0633\u062a\u0642\u06cc\u0645 \u062e\u0648\u062f \u0628\u0647 \u062a\u0647\u0631\u0627\u0646 \u0631\u0627 \u062f\u0631 \u0645\u0627\u0631\u0633 (\u0627\u0633\u0641\u0646\u062f) \u06af\u0630\u0634\u062a\u0647 \u0627\u0632 \u0633\u0631 \u06af\u0631\u0641\u062a \u0648 \u0645\u0642\u0627\u0645\u0627\u062a \u0627\u06cc\u0631\u0627\u0646\u06cc \u0645\u0634\u063a\u0648\u0644 \u0645\u0630\u0627\u06a9\u0631\u0647 \u0628\u0627 \u0645\u0642\u0627\u0645\u0627\u062a \u0627\u06cc\u062a\u0627\u0644\u06cc\u0627 \u0628\u0627 \u0647\u062f\u0641 \u0627\u0641\u0632\u0627\u06cc\u0634 \u0645\u0633\u06cc\u0631 \u067e\u0631\u0648\u0627\u0632\u0647\u0627\u06cc \u0634\u0631\u06a9\u062a \u0647\u0648\u0627\u067e\u06cc\u0645\u0627\u06cc\u06cc \u02c8\u0622\u0644\u06cc\u062a\u0627\u0644\u06cc\u0627\u02c8\u06cc \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0647\u0633\u062a\u0646\u062f.\u0631\u062d\u0645\u0627\u0646\u06cc \u0645\u0648\u062d\u062f\u06cc \u06af\u0641\u062a: \u0646\u0645\u0627\u06cc\u0646\u062f\u06af\u0627\u0646\u06cc \u0627\u0632 \u0627\u06cc\u062a\u0627\u0644\u06cc\u0627\u060c \u0627\u0644\u0645\u0627\u0646\u060c \u0644\u0647\u0633\u062a\u0627\u0646 \u0648 \u0686\u0646\u062f \u06a9\u0634\u0648\u0631 \u0639\u0631\u0628\u06cc \u0627\u0632 \u062c\u0645\u0644\u0647 \u0639\u0645\u0627\u0646 \u0648 \u06a9\u0648\u06cc\u062a \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u0628\u0631\u0631\u0633\u06cc \u0641\u0631\u0635\u062a\u200c\u0647\u0627\u06cc \u0622\u062a\u06cc \u0633\u0631\u0645\u0627\u06cc\u0647 \u06af\u0630\u0627\u0631\u06cc \u062f\u0631 \u0628\u062e\u0634 \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0627\u0632\u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u062f\u06cc\u062f\u0646 \u06a9\u0631\u062f\u0647 \u0627\u0646\u062f.\u06cc\u06a9\u06cc \u0627\u0632 \u0648\u0639\u062f\u0647\u200c\u0647\u0627\u06cc \u0627\u0648\u0644\u06cc\u0647 \u0631\u0648\u062d\u0627\u0646\u06cc \u062f\u0631 \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0631\u06cc\u0627\u0633\u062a \u062c\u0645\u0647\u0648\u0631\u06cc \u0633\u0627\u0644 \u06af\u0630\u0634\u062a\u0647\u060c \u0628\u0647\u0628\u0648\u062f \u06a9\u06cc\u0641\u06cc \u0635\u0646\u0639\u062a \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u0648 \u062c\u0630\u0628 \u0628\u06cc\u0634\u062a\u0631 \u06af\u0631\u062f\u0634\u06af\u0631\u062e\u0627\u0631\u062c\u06cc \u0628\u0648\u062f.\u06f8 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0646\u0641\u0631 \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u062e\u0627\u0631\u062c\u06cc \u0627\u0632 \u0627\u06cc\u0631\u0627\u0646 \u0628\u0627\u0632\u062f\u06cc\u062f \u06a9\u0631\u062f\u0647 \u0627\u0646\u062f.\u062f\u0631 \u0639\u06cc\u0646 \u062d\u0627\u0644\u060c \u0627\u0646\u062a\u0634\u0627\u0631\u0627\u062a \u02c8\u0644\u0648\u0646\u0644\u06cc \u067e\u0644\u0627\u0646\u062a\u02c8 (\u0633\u06cc\u0627\u0631\u0647 \u062a\u0646\u0647\u0627) \u06a9\u0647 \u0645\u0639\u062a\u0628\u0631\u062a\u0631\u06cc\u0646 \u0648 \u0645\u0639\u0631\u0648\u0641\u062a\u0631\u06cc\u0646 \u0627\u0646\u062a\u0634\u0627\u0631\u0627\u062a \u0631\u0627\u0647\u0646\u0645\u0627\u06cc \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u062f\u0631 \u062c\u0647\u0627\u0646 \u0627\u0633\u062a\u060c \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f\u0647 \u06a9\u0647 \u0627\u0641\u0632\u0627\u06cc\u0634 \u062a\u0645\u0627\u06cc\u0644 \u0645\u0631\u062f\u0645 \u0628\u0647 \u06a9\u0633\u0628 \u0627\u0637\u0644\u0627\u0639\u0627\u062a\u06cc \u062f\u0631\u0628\u0627\u0631\u0647 \u0627\u06cc\u0631\u0627\u0646\u060c \u0627\u06cc\u0646 \u0646\u0627\u0634\u0631 \u0631\u0627 \u0628\u0631 \u0622\u0646 \u062f\u0627\u0634\u062a\u0647 \u062a\u0627 \u062f\u0631 \u0648\u06cc\u0631\u0627\u0633\u062a \u062c\u062f\u06cc\u062f \u06a9\u062a\u0627\u0628 \u02c8\u0631\u0627\u0647\u0646\u0645\u0627\u06cc \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u062e\u0627\u0648\u0631\u0645\u06cc\u0627\u0646\u0647\u02c8 \u0641\u0635\u0644\u06cc \u0631\u0627 \u0628\u0647 \u0645\u0639\u0631\u0641\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0627\u062e\u062a\u0635\u0627\u0635 \u062f\u0647\u062f.
p54
aV\u062f\u0631  \u0631\u0627\u0633\u062a\u0627\u06cc \u0627\u0631\u062a\u0642\u0627\u06cc \u0638\u0631\u0641\u06cc\u062a\u200c\u0647\u0627\u06cc \u0631\u0642\u0627\u0628\u062a\u06cc \u0646\u0633\u0628\u062a \u0628\u0647 \u062f\u06cc\u06af\u0631 \u0628\u0627\u0646\u06a9\u200c\u0647\u0627 \u0648\u0627\u0641\u0632\u0627\u06cc\u0634 \u0633\u0637\u062d \u0631\u0636\u0627\u06cc\u062a  \u0645\u0634\u062a\u0631\u06cc\u0627\u0646\u060c \u06a9\u0645\u06cc\u062a\u0647 \u0637\u0631\u0627\u062d\u06cc \u0648 \u062a\u0648\u0633\u0639\u0647 \u0645\u062d\u0635\u0648\u0644 \u0628\u0627\u0646\u06a9 \u0627\u0646\u0635\u0627\u0631 \u062f\u0631 \u0633\u0648\u0645\u06cc\u0646 \u062c\u0644\u0633\u0647 \u062e\u0648\u062f \u0637\u0631\u062d \u0631\u0627\u0647  \u0627\u0646\u062f\u0627\u0632\u06cc \u0633\u0627\u0645\u0627\u0646\u0647 «\u0622\u0646\u06cc \u0686\u06a9» \u062f\u0631 \u0634\u0639\u0628 \u0645\u0645\u062a\u0627\u0632 \u0648 \u0645\u0646\u062a\u062e\u0628 \u0631\u0627 \u0628\u0647 \u062a\u0635\u0648\u06cc\u0628 \u0631\u0633\u0627\u0646\u062f \u062a\u0627 \u0645\u0634\u062a\u0631\u06cc\u0627\u0646  \u062e\u0648\u0634 \u062d\u0633\u0627\u0628 \u0627\u06cc\u0646 \u0628\u0627\u0646\u06a9 \u0628\u0647 \u0635\u0648\u0631\u062a \u0622\u0646\u06cc \u062f\u0633\u062a\u0647 \u0686\u06a9 \u062e\u0648\u062f \u0631\u0627 \u062f\u0631\u06cc\u0627\u0641\u062a \u06a9\u0646\u0646\u062f.\u0628\u0647  \u06af\u0632\u0627\u0631\u0634 \u0627\u062f\u0627\u0631\u0647 \u06a9\u0644 \u0628\u0627\u0632\u0627\u0631\u06cc\u0627\u0628\u06cc \u0648\u062a\u0628\u0644\u06cc\u063a\u0627\u062a \u0628\u0627\u0646\u06a9 \u0627\u0646\u0635\u0627\u0631\u060c \u0622\u0642\u0627\u06cc \u0633\u06cc\u062f\u062d\u0645\u06cc\u062f\u062e\u0627\u0644\u0642\u06cc \u0645\u062f\u06cc\u0631\u06a9\u0644  \u0628\u0627\u0632\u0627\u0631\u06cc\u0627\u0628\u06cc \u0648 \u062a\u0628\u0644\u06cc\u063a\u0627\u062a \u0627\u06cc\u0646 \u0628\u0627\u0646\u06a9 \u0637\u06cc \u06af\u0641\u062a\u06af\u0648\u06cc\u06cc \u0633\u0627\u0645\u0627\u0646\u0647 \u0622\u0646\u06cc \u0686\u06a9 \u0631\u0627 \u06cc\u06a9 \u062e\u062f\u0645\u062a \u062c\u062f\u06cc\u062f  \u062f\u0627\u0646\u0633\u062a \u06a9\u0647 \u0628\u0627\u0639\u062b \u062c\u0644\u0648\u06af\u06cc\u0631\u06cc \u0627\u0632 \u0627\u062a\u0644\u0627\u0641 \u0648\u0642\u062a \u0645\u0634\u062a\u0631\u06cc\u0627\u0646 \u0634\u062f\u0647 \u0648 \u0641\u0631\u0627\u06cc\u0646\u062f \u062f\u0631\u062e\u0648\u0627\u0633\u062a \u0648  \u0648\u0627\u06af\u0630\u0627\u0631\u06cc \u062f\u0633\u062a\u0647 \u0686\u06a9 \u0631\u0627 \u06a9\u0647 \u062f\u0631\u06af\u0630\u0634\u062a\u0647 \u0637\u06cc \u0686\u0646\u062f \u0631\u0648\u0632 \u0635\u0648\u0631\u062a \u0645\u06cc\u200c\u06af\u0631\u0641\u062a\u060c \u0628\u0647 \u062a\u0646\u0647\u0627 \u0686\u0646\u062f \u062f\u0642\u06cc\u0642\u0647  \u062a\u0642\u0644\u06cc\u0644 \u0645\u06cc\u200c\u062f\u0647\u062f.\u0645\u062f\u06cc\u0631\u06a9\u0644 \u0628\u0627\u0632\u0627\u0631\u06cc\u0627\u0628\u06cc \u0648 \u062a\u0628\u0644\u06cc\u063a\u0627\u062a \u0628\u0627\u0646\u06a9 \u0627\u0646\u0635\u0627\u0631 \u062f\u0631 \u067e\u0627\u06cc\u0627\u0646 \u0627\u0636\u0627\u0641\u0647 \u06a9\u0631\u062f  \u06a9\u0647 \u0628\u0647 \u0637\u0648\u0631 \u0637\u0628\u06cc\u0639\u06cc \u0627\u06cc\u0646 \u062e\u062f\u0645\u062a \u062c\u062f\u06cc\u062f \u0628\u0647 \u0645\u0634\u062a\u0631\u06cc\u0627\u0646 \u062e\u0648\u0634 \u062d\u0633\u0627\u0628 \u062a\u0639\u0644\u0642 \u0645\u06cc\u200c\u06af\u06cc\u0631\u062f \u0648 \u062a\u062d\u0648\u06cc\u0644  \u0622\u0646 \u0628\u0647 \u0635\u0648\u0631\u062a \u0622\u0646\u06cc \u0627\u0633\u062a.\u0628\u0627\u0646\u06a9 \u0627\u0646\u0635\u0627\u0631 \u0628\u0631\u0627\u06cc \u0646\u062e\u0633\u062a\u06cc\u0646 \u0628\u0627\u0631 \u0648 \u062f\u0631 \u0631\u0627\u0633\u062a\u0627\u06cc \u062a\u0648\u0633\u0639\u0647 \u062e\u062f\u0645\u0627\u062a \u0628\u0627\u0646\u06a9\u06cc \u0648 \u062c\u0644\u0628 \u0631\u0636\u0627\u06cc\u062a \u0647\u0631\u0686\u0647 \u0628\u06cc\u0634\u062a\u0631 \u0645\u0634\u062a\u0631\u06cc\u0627\u0646 \u0628\u0647 \u0632\u0648\u062f\u06cc \u062f\u0631 \u0634\u0639\u0628 \u0645\u0645\u062a\u0627\u0632 \u0648 \u0645\u0646\u062a\u062e\u0628 \u062e\u0648\u062f \u0633\u0627\u0645\u0627\u0646\u0647 «\u0622\u0646\u06cc \u0686\u06a9» \u0631\u0627 \u0631\u0627\u0647 \u0627\u0646\u062f\u0627\u0632\u06cc \u0645\u06cc\u200c\u06a9\u0646\u062f \u062a\u0627 \u0635\u062f\u0648\u0631 \u062f\u0633\u062a\u0647 \u0686\u06a9 \u0628\u0631\u0627\u06cc \u0645\u0634\u062a\u0631\u06cc\u0627\u0646 \u0645\u062a\u0642\u0627\u0636\u06cc \u062e\u0648\u0634 \u062d\u0633\u0627\u0628 \u0628\u0647 \u0635\u0648\u0631\u062a \u0622\u0646\u06cc \u0635\u0648\u0631\u062a \u06af\u06cc\u0631\u062f.
p55
aV\u0645\u0637\u0627\u0628\u0642 \u0642\u0627\u0646\u0648\u0646 \u06a9\u0627\u0631 \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0627\u06cc\u0631\u0627\u0646\u060c \u06a9\u0627\u0631\u06af\u0631\u0627\u0646 \u06a9\u0634\u0648\u0631 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0633\u0647 \u062a\u0634\u06a9\u0644 \u0635\u0646\u0641\u06cc \u0628\u0631\u0627\u06cc \u062e\u0648\u062f \u0627\u06cc\u062c\u0627\u062f \u0646\u0645\u0627\u06cc\u0646\u062f.\u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631\u060c \u0627\u0646\u062c\u0645\u0646 \u0635\u0646\u0641\u06cc \u0648 \u0646\u0645\u0627\u06cc\u0646\u062f\u0647 \u06a9\u0627\u0631\u06af\u0631\u06f3 \u062a\u0634\u06a9\u0644\u06cc \u0647\u0633\u062a\u0646\u062f \u06a9\u0647 \u0637\u0628\u0642 \u0642\u0627\u0646\u0648\u0646 \u06a9\u0627\u0631\u0645\u06cc \u062a\u0648\u0627\u0646 \u0646\u0633\u0628\u062a \u0628\u0647 \u0627\u06cc\u062c\u0627\u062f \u0622\u0646 \u0627\u0642\u062f\u0627\u0645 \u06a9\u0631\u062f..\u062f\u0631 \u0627\u06cc\u0646 \u0628\u06cc\u0646 \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u0627\u06cc\u0646 \u06a9\u0647 \u062a\u0634\u06a9\u06cc\u0644 \u0622\u0646 (\u0642\u0627\u0646\u0648\u0646 \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631) \u062f\u0627\u0631\u0627\u06cc \u0642\u0627\u0646\u0648\u0646 \u0645\u062c\u0632\u0627\u06cc\u06cc \u0627\u0633\u062a \u06a9\u0647 \u062f\u0631 \u0633\u0627\u0644 \u06f1\u06f3\u06f6\u06f3 \u0628\u0647 \u062a\u0635\u0648\u06cc\u0628 \u0645\u062c\u0644\u0633 \u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0631\u0633\u06cc\u062f\u0647 \u0648 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u0638\u0631\u0641\u06cc\u062a\u200c\u0647\u0627\u06cc \u0642\u0627\u0646\u0648\u0646\u06cc \u0648 \u0627\u062e\u062a\u06cc\u0627\u0631\u0627\u062a \u0648\u0633\u06cc\u0639 \u0627\u0632 \u0627\u0633\u062a\u0642\u0628\u0627\u0644 \u0628\u06cc\u0634\u062a\u0631\u06cc \u062f\u0631 \u0645\u06cc\u0627\u0646 \u06a9\u0627\u0631\u06af\u0631\u0627\u0646 \u0628\u0631\u062e\u0648\u0631\u062f\u0627\u0631 \u0628\u0648\u062f\u0647 \u0648 \u0647\u0633\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0639\u0635\u0631 \u062e\u0628\u0631\u060c \u0637\u0628\u0642 \u0642\u0627\u0646\u0648\u0646 \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631\u060c \u06a9\u0644\u06cc\u0647 \u0648\u0627\u062d\u062f\u200c\u0647\u0627\u06cc \u062a\u0648\u0644\u06cc\u062f\u06cc\u060c \u0635\u0646\u0639\u062a\u06cc\u060c \u06a9\u0634\u0627\u0648\u0631\u0632\u06cc \u0648 \u062e\u062f\u0645\u0627\u062a\u06cc \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0634\u0648\u0631\u0627\u06cc\u06cc \u0645\u0631\u06a9\u0628 \u0627\u0632 \u0646\u0645\u0627\u06cc\u0646\u062f\u06af\u0627\u0646 \u06a9\u0627\u0631\u06af\u0631\u0627\u0646 \u0648 \u06a9\u0627\u0631\u06a9\u0646\u0627\u0646 \u062a\u0634\u06a9\u06cc\u0644 \u062f\u0647\u0646\u062f.\u0627\u0644\u0628\u062a\u0647 \u0634\u0631\u0637 \u062a\u0634\u06a9\u06cc\u0644 \u0634\u0648\u0631\u0627 \u0627\u06cc\u0646 \u0627\u0633\u062a \u06a9\u0647 \u0648\u0627\u062d\u062f \u062f\u0627\u0631\u0627\u06cc \u0628\u06cc\u0634 \u0627\u0632 \u06f3\u06f5 \u0646\u0641\u0631 \u06a9\u0627\u0631\u06af\u0631 \u0628\u0627\u0634\u062f \u062f\u0631 \u063a\u06cc\u0631 \u0627\u06cc\u0646 \u0635\u0648\u0631\u062a \u0648 \u062f\u0631 \u0634\u0631\u0627\u06cc\u0637\u06cc \u06a9\u0647 \u062a\u0639\u062f\u0627\u062f \u06a9\u0627\u0631\u06af\u0631\u0627\u0646 \u06a9\u0645\u062a\u0631 \u0627\u0632 \u0627\u06cc\u0646 \u062a\u0639\u062f\u0627\u062f \u0628\u0627\u0634\u062f \u0646\u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u062a\u0634\u06a9\u06cc\u0644 \u062f\u0627\u062f \u0648 \u0628\u0627\u06cc\u0633\u062a\u06cc \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u062a\u0634\u06a9\u06cc\u0644 \u0627\u0646\u062c\u0645\u0646 \u0635\u0646\u0641\u06cc \u0648 \u0646\u0645\u0627\u06cc\u0646\u062f\u0647 \u06a9\u0627\u0631\u06af\u0631 \u06a9\u0631\u062f.\u0627\u0648\u062c \u0641\u0639\u0627\u0644\u06cc\u062a\u060c \u062a\u0639\u062f\u0627\u062f \u0648 \u0627\u062b\u0631\u06af\u0630\u0627\u0631\u06cc \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u062f\u0631 \u062f\u0647\u0647 \u06f7\u06f0 \u0628\u0648\u062f \u0628\u0647 \u06af\u0648\u0646\u0647 \u0627\u06cc \u06a9\u0647 \u062a\u0639\u062f\u0627\u062f \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u0628\u0647 \u062d\u062f\u0648\u062f \u06f3\u06f5\u06f0\u06f0 \u0634\u0648\u0631\u0627 \u0631\u0633\u06cc\u062f.\u0627\u0645\u0627 \u062f\u0631 \u062f\u0647\u0647 \u06f8\u06f0 \u0628\u0647 \u062e\u0635\u0648\u0635 \u0627\u0632 \u0646\u06cc\u0645\u0647 \u062f\u0648\u0645 \u0622\u0646 \u0648 \u0628\u0627 \u0631\u0648\u06cc \u06a9\u0627\u0631 \u0622\u0645\u062f\u0646 \u062f\u0648\u0644\u062a \u0645\u062d\u0645\u0648\u062f \u0627\u062d\u0645\u062f\u06cc \u0646\u0698\u0627\u062f \u0628\u0647 \u0645\u0631\u0648\u0631 \u0627\u0641\u062a \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u062f\u0631 \u06a9\u0634\u0648\u0631 \u0634\u0631\u0648\u0639 \u0634\u062f \u0628\u0647 \u06af\u0648\u0646\u0647 \u0627\u06cc \u06a9\u0647 \u0627\u0645\u0631\u0648\u0632\u0647 \u062a\u0639\u062f\u0627\u062f \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u0628\u0633\u06cc\u0627\u0631 \u06a9\u0645\u062a\u0631 \u0627\u0632 \u0622\u0646 \u062f\u0648\u0631\u0627\u0646 \u0634\u062f\u0647 \u0627\u0633\u062a.\u0627\u0645\u0627 \u0628\u0627 \u0631\u0648\u06cc \u06a9\u0627\u0631 \u0622\u0645\u062f\u0646 \u062f\u0648\u0644\u062a \u062a\u062f\u0628\u06cc\u0631 \u0648 \u0627\u0645\u06cc\u062f \u0628\u0647 \u062e\u0635\u0648\u0635 \u0627\u06cc\u0646 \u06a9\u0647 \u0648\u0632\u06cc\u0631 \u062a\u0639\u0627\u0648\u0646\u060c \u06a9\u0627\u0631 \u0648 \u0631\u0641\u0627\u0647 \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0627\u06cc\u0646 \u062f\u0648\u0644\u062a \u062f\u06a9\u062a\u0631 \u0639\u0644\u06cc \u0631\u0628\u06cc\u0639\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0633\u0627\u0628\u0642\u0647 \u062d\u0636\u0648\u0631 \u062f\u0631 \u062a\u0634\u06a9\u0644\u0627\u062a \u0648 \u0645\u062c\u0627\u0645\u0639 \u06a9\u0627\u0631\u06af\u0631\u06cc \u0631\u0627 \u062f\u0627\u0631\u062f\u060c \u06a9\u0627\u0631\u06af\u0631\u0627\u0646 \u0627\u0645\u06cc\u062f\u0648\u0627\u0631 \u0647\u0633\u062a\u0646\u062f \u06a9\u0647 \u0628\u0627 \u0627\u06cc\u0646 \u0633\u0627\u0628\u0642\u0647 \u0648\u0632\u06cc\u0631 \u06a9\u0627\u0631 \u0648 \u0627\u06cc\u0646 \u06a9\u0647 '\u062d\u0645\u0627\u06cc\u062a \u0648 \u06af\u0633\u062a\u0631\u0634 \u0627\u0632 \u062a\u0634\u06a9\u0644\u200c\u0647\u0627\u06cc \u06a9\u0627\u0631\u06af\u0631\u06cc \u0648 \u06a9\u0627\u0631\u0641\u0631\u0645\u0627\u06cc\u06cc' \u0627\u0632 \u0628\u0631\u0646\u0627\u0645\u0647\u200c\u0647\u0627\u06cc \u0648\u0632\u06cc\u0631 \u062a\u0639\u0627\u0648\u0646\u060c \u06a9\u0627\u0631 \u0648 \u0631\u0641\u0627\u0647 \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0628\u0647 \u0646\u0645\u0627\u06cc\u0646\u062f\u06af\u0627\u0646 \u0645\u0644\u062a \u0627\u0631\u0627\u0626\u0647 \u062f\u0627\u062f\u0647 \u0648 \u0631\u0633\u0645\u0627 \u0627\u0639\u0644\u0627\u0645 \u0634\u062f\u0647 \u0627\u0633\u062a\u060c \u0645\u062c\u062f\u062f\u0627 \u0634\u0627\u0647\u062f \u0627\u0641\u0632\u0627\u06cc\u0634 \u062a\u0639\u062f\u0627\u062f \u0648 \u06a9\u0627\u0631\u06a9\u0631\u062f \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u062f\u0631 \u0633\u0631\u0627\u0633\u0631 \u0648\u0627\u062d\u062f\u200c\u0647\u0627\u06cc \u06a9\u0634\u0648\u0631 \u0628\u0648\u062f.\u062f\u0631 \u0627\u06cc\u0646 \u062e\u0635\u0648\u0635 \u0646\u0627\u0635\u0631 \u0628\u0631\u0647\u0627\u0646\u06cc \u0631\u06cc\u06cc\u0633 \u0647\u06cc\u0627\u062a \u0645\u062f\u06cc\u0631\u0647 \u06a9\u0627\u0646\u0648\u0646 \u0647\u0645\u0627\u0647\u0646\u06af\u06cc \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u0627\u0633\u062a\u0627\u0646 \u062a\u0647\u0631\u0627\u0646 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u062c\u0644\u0633\u0627\u062a\u06cc \u06a9\u0647 \u0628\u0627 \u0648\u0632\u06cc\u0631 \u06a9\u0627\u0631 \u0628\u0631\u06af\u0632\u0627\u0631 \u0634\u062f\u0647 \u0627\u0633\u062a\u060c \u062e\u0627\u0637\u0631 \u0646\u0634\u0627\u0646 \u06a9\u0631\u062f: \u0637\u0628\u0642 \u0642\u0648\u0644 \u0645\u0633\u0626\u0648\u0644\u0627\u0646 \u0648\u0632\u0627\u0631\u062a \u06a9\u0627\u0631 \u062a\u0639\u062f\u0627\u062f \u0634\u0648\u0631\u0627\u0647\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u0633\u0631\u0627\u0633\u0631 \u06a9\u0634\u0648\u0631 \u06f5\u06f0 \u062f\u0631\u0635\u062f \u0627\u0641\u0632\u0627\u06cc\u0634 \u062e\u0648\u0627\u0647\u062f \u062f\u0627\u0634\u062a.\u0628\u0631\u0647\u0627\u0646\u06cc \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0627\u06cc\u0646 \u06a9\u0647 \u062f\u0631 \u062d\u0627\u0644 \u062d\u0627\u0636\u0631 \u06f2\u06f6\u06f0 \u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0627\u0631 \u062f\u0631 \u0627\u0633\u062a\u0627\u0646 \u062a\u0647\u0631\u0627\u0646 \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f\u060c \u06af\u0641\u062a: \u062f\u0647\u0647 \u06f7\u06f0 \u062d\u062f\u0648\u062f \u06f6\u06f0\u06f0 \u0634\u0648\u0631\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0627\u0633\u062a\u0627\u0646 \u0641\u0639\u0627\u0644\u06cc\u062a \u0645\u06cc\u200c\u06a9\u0631\u062f\u0646\u062f.
p56
aV\u0633\u0631\u06cc\u0627\u0644 "\u0628\u0632\u0646\u06af\u0627\u0647" \u0628\u0647 \u06a9\u0627\u0631\u06af\u0631\u062f\u0627\u0646\u06cc \u0631\u0636\u0627 \u0639\u0637\u0627\u0631\u0627\u0646 \u062a\u0627 \u067e\u0627\u06cc\u0627\u0646 \u0647\u0641\u062a\u0647 \u0627\u0632 \u0631\u0627\u062f\u06cc\u0648 \u0646\u0645\u0627\u06cc\u0634 \u067e\u062e\u0634 \u0645\u06cc\u200c\u0634\u0648\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c «\u0628\u0632\u0646\u06af\u0627\u0647» \u0639\u0646\u0648\u0627\u0646 \u0645\u062c\u0645\u0648\u0639\u0647\u200c\u0627\u06cc \u062a\u0644\u0648\u06cc\u0632\u06cc\u0648\u0646\u06cc \u0628\u0647 \u06a9\u0627\u0631\u06af\u0631\u062f\u0627\u0646\u06cc \u0631\u0636\u0627 \u0639\u0637\u0627\u0631\u0627\u0646 \u0627\u0633\u062a.\u0627\u06cc\u0646 \u0645\u062c\u0645\u0648\u0639\u0647 \u0628\u0631 \u067e\u0627\u06cc\u0647\u0654 \u0641\u06cc\u0644\u0645\u200c\u0646\u0627\u0645\u0647\u200c\u0627\u06cc \u0627\u0632 \u0633\u0631\u0648\u0634 \u0635\u062d\u062a \u0648 \u0645\u062d\u0645\u062f\u0631\u0636\u0627 \u0622\u0631\u06cc\u0627\u0646 \u0646\u0648\u0634\u062a\u0647 \u0634\u062f\u0647 \u0648 \u062f\u0627\u0633\u062a\u0627\u0646 \u0645\u0631\u062f\u06cc \u0645\u0639\u062a\u0627\u062f \u0627\u0633\u062a \u06a9\u0647 \u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u062f\u062e\u062a\u0631\u0634 \u062f\u0631 \u06a9\u0631\u062c \u0632\u0646\u062f\u06af\u06cc \u0645\u06cc\u200c\u06a9\u0646\u062f \u0648 \u0627\u0632 \u0647\u0645\u0633\u0631\u0634 \u0637\u0644\u0627\u0642 \u06af\u0631\u0641\u062a\u0647 \u0627\u0633\u062a.\u0628\u0631 \u0627\u0633\u0627\u0633 \u0627\u06cc\u0646 \u062e\u0628\u0631\u060c \u0641\u0631\u0634\u0627\u062f \u0622\u0630\u0631 \u0646\u06cc\u0627 \u062a\u0647\u06cc\u0647\u200c\u06a9\u0646\u0646\u062f\u06af\u06cc \u0648 \u0633\u0631\u062f\u0628\u06cc\u0631\u06cc\u060c \u0622\u0631\u0645\u0627\u0646 \u063a\u0641\u0627\u0631\u06cc\u0627\u0646 \u06af\u0648\u06cc\u0646\u062f\u06af\u06cc \u0648 \u062a\u0646\u0638\u06cc\u0645 \u0631\u0627\u062f\u06cc\u0648\u06cc\u06cc \u0648 \u0639\u0644\u06cc \u062d\u0627\u062c\u06cc \u0646\u0648\u0631\u0648\u0632\u06cc \u0635\u062f\u0627\u0628\u0631\u062f\u0627\u0631\u06cc \u0627\u06cc\u0646 \u0631\u0627\u062f\u06cc\u0648 \u0641\u06cc\u0644\u0645 \u0631\u0627 \u0628\u0631 \u0639\u0647\u062f\u0647 \u062f\u0627\u0631\u0646\u062f.\u0627\u06cc\u0646 \u0633\u0631\u06cc\u0627\u0644 \u0628\u0627 \u062d\u0636\u0648\u0631 \u0628\u0627\u0632\u06cc\u06af\u0631\u0627\u0646\u06cc \u0686\u0648\u0646 \u0631\u0636\u0627 \u0639\u0637\u0627\u0631\u0627\u0646\u060c \u062d\u0645\u06cc\u062f \u0644\u0648\u0644\u0627\u06cc\u06cc\u060c \u0645\u0631\u062c\u0627\u0646\u0647 \u06af\u0644\u0686\u06cc\u0646\u060c \u0627\u062d\u0645\u062f \u067e\u0648\u0631\u0645\u062e\u0628\u0631\u060c \u0639\u0637\u06cc\u0647 \u0645\u0639\u0635\u0648\u0645\u06cc\u060c \u0639\u0644\u06cc \u0635\u0627\u062f\u0642\u06cc\u060c \u06af\u06cc\u0644\u062f\u0627 \u0631\u0646\u062c\u0628\u0631\u0627\u0646\u060c \u0633\u0648\u0633\u0646 \u067e\u0631\u0648\u0631\u060c \u0647\u0646\u06af\u0627\u0645\u0647 \u062d\u0645\u06cc\u062f\u0632\u0627\u062f\u0647 \u0648 \u063a\u0644\u0627\u0645\u0631\u0636\u0627 \u0646\u06cc\u06a9\u062e\u0648\u0627\u0647 \u062a\u0647\u06cc\u0647 \u0634\u062f\u0647 \u0627\u0633\u062a.
p57
aV\u062a\u0635\u0627\u0648\u06cc\u0631 \u06a9\u0627\u0645\u0644 \u0627\u06cc\u0646 \u0645\u062c\u0645\u0648\u0639\u0647 \u062f\u0631 \u0627\u0646\u062f\u0627\u0632\u0647 \u0628\u06a9 \u06af\u0631\u0627\u0646\u062f \u0631\u0627 \u0627\u06cc\u0646\u062c\u0627 \u062f\u0631\u06cc\u0627\u0641\u062a \u06a9\u062a\u06cc\u062f
p58
aV\u0627\u0646\u062a\u0634\u0627\u0631 \u06af\u0632\u0627\u0631\u0634 \u0631\u0648\u0632 \u06af\u0630\u0634\u062a\u0647 \u0622\u0698\u0627\u0646\u0633 \u0628\u06cc\u0646\u200c\u0627\u0644\u0645\u0644\u0644\u06cc \u0627\u0646\u0631\u0698\u06cc \u0627\u062a\u0645\u06cc \u062f\u0631\u0628\u0627\u0631\u0647 \u0628\u0631\u0646\u0627\u0645\u0647 \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0627\u06cc\u0631\u0627\u0646 \u062a\u062d\u0644\u06cc\u0644\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641\u06cc \u0631\u0627 \u062f\u0631 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627\u06cc \u062c\u0647\u0627\u0646\u06cc \u062f\u0631 \u067e\u06cc\u200c \u062f\u0627\u0634\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0635\u0647\u06cc\u0648\u0646\u06cc\u0633\u062a\u06cc \u0647\u0627\u0622\u0631\u062a\u0635 \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0622\u0698\u0627\u0646\u0633 \u0628\u06cc\u0646\u200c\u0627\u0644\u0645\u0644\u0644\u06cc \u0627\u0646\u0631\u0698\u06cc \u0627\u062a\u0645\u06cc \u0645\u0628\u0646\u06cc \u0628\u0631 \u0627\u06cc\u0646 \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0686\u0647\u0627\u0631\u067e\u0646\u062c\u0645 \u0630\u062e\u06cc\u0631\u0647 \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u063a\u0646\u06cc\u200c \u0634\u062f\u0647 \u062e\u0648\u062f \u062f\u0631 \u0633\u0637\u0648\u062d \u0628\u0627\u0644\u0627 \u0631\u0627 \u062e\u0646\u062b\u06cc \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a\u060c \u0645\u062f\u0639\u06cc \u0634\u062f \u0627\u06cc\u0646 \u062d\u062c\u0645 \u0627\u0632 \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u062f\u0631 \u0627\u062e\u062a\u06cc\u0627\u0631 \u062f\u0627\u0634\u062a\u060c \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0633\u062a \u0628\u0631\u0627\u06cc \u062a\u0648\u0644\u06cc\u062f \u06cc\u06a9 \u0633\u0644\u0627\u062d \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0634\u0648\u062f.\u0627\u06cc\u0646 \u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0628\u0647 \u0646\u0642\u0644 \u0627\u0632 \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u0622\u0633\u0648\u0634\u06cc\u062a\u062f\u067e\u0631\u0633 \u0646\u0648\u0634\u062a \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0628\u062e\u0634 \u0627\u0639\u0638\u0645 \u0630\u062e\u06cc\u0631\u0647 \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u062d\u0633\u0627\u0633 \u062e\u0648\u062f \u0631\u0627 \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u0627\u062c\u0631\u0627\u06cc \u062a\u0648\u0627\u0641\u0642 \u0627\u0648\u0644\u06cc\u0647\u200c\u0627\u0634 \u0628\u0627 \u06af\u0631\u0648\u0647 \u06f1+\u06f5 \u06a9\u0647 \u062f\u0631 \u0646\u0648\u0627\u0645\u0628\u0631 \u06af\u0630\u0634\u062a\u0647 \u0628\u0647 \u062f\u0633\u062a \u0622\u0645\u062f\u060c \u06a9\u0627\u0647\u0634 \u062f\u0627\u062f\u0647 \u0627\u0633\u062a.\u0628\u0631\u0627\u0633\u0627\u0633 \u06af\u0632\u0627\u0631\u0634 \u0622\u0698\u0627\u0646\u0633 \u0627\u06cc\u0631\u0627\u0646 \u0647\u0645\u200c\u0627\u06a9\u0646\u0648\u0646 \u06a9\u0645\u200c\u062a\u0631 \u0627\u0632 \u06f4\u06f0 \u06a9\u06cc\u0644\u0648\u06af\u0631\u0645 \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u063a\u0646\u06cc\u200c \u0634\u062f\u0647 \u062f\u0631 \u0633\u0637\u0648\u062d \u0628\u0627\u0644\u0627 \u062f\u0627\u0631\u062f.\u0627\u06cc\u0631\u0627\u0646 \u0647\u0645\u0648\u0627\u0631\u0647 \u0627\u062a\u0647\u0627\u0645\u0627\u062a \u063a\u0631\u0628 \u062f\u0631 \u0627\u06cc\u0646 \u062e\u0635\u0648\u0635 \u06a9\u0647 \u062a\u0644\u0627\u0634 \u0645\u06cc\u200c\u06a9\u0646\u062f \u062f\u0631 \u0641\u0639\u0627\u0644\u06cc\u062a\u200c\u0647\u0627\u06cc \u0647\u0633\u062a\u0647\u200c\u0627\u06cc\u200c\u0627\u0634 \u0628\u0647 \u0638\u0631\u0641\u06cc\u062a \u062a\u0648\u0644\u06cc\u062f \u0628\u0645\u0628 \u0627\u062a\u0645 \u0628\u0631\u0633\u062f \u0631\u0627 \u0631\u062f \u0645\u06cc\u200c\u06a9\u0646\u062f.\u0647\u0645\u200c\u0686\u0646\u06cc\u0646 \u0634\u0628\u06a9\u0647 \u062e\u0628\u0631\u06cc \u0627\u06cc\u200c\u0628\u06cc\u200c\u0633\u06cc \u0646\u06cc\u0648\u0632 \u06af\u0632\u0627\u0631\u0634 \u062f\u0627\u062f \u0628\u0646\u0627\u0628\u0631 \u0627\u0639\u0644\u0627\u0645 \u0622\u0698\u0627\u0646\u0633 \u0628\u06cc\u0646\u200c\u0627\u0644\u0645\u0644\u0644\u06cc \u0627\u0646\u0631\u0698\u06cc \u0627\u062a\u0645\u06cc\u060c \u0627\u06cc\u0631\u0627\u0646 \u0630\u062e\u06cc\u0631\u0647 \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u062f\u0631 \u0633\u0637\u0648\u062d \u0628\u0627\u0644\u0627\u06cc\u0634 \u0631\u0627 \u062a\u0627 \u062d\u062f \u0632\u06cc\u0627\u062f\u06cc \u06a9\u0627\u0647\u0634 \u062f\u0627\u062f\u0647 \u0627\u0633\u062a \u06a9\u0647 \u0633\u0628\u0628 \u0634\u062f\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0647\u0645\u200c\u0627\u06a9\u0646\u0648\u0646 \u062a\u0646\u0647\u0627 \u0627\u0632 \u063a\u0646\u06cc\u200c\u0633\u0627\u0632\u06cc \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u062f\u0631 \u0633\u0637\u062d \u06f5 \u062f\u0631\u0635\u062f \u0628\u0631\u062e\u0648\u0631\u062f\u0627\u0631 \u0628\u0627\u0634\u062f.\u0627\u06cc\u0646 \u0634\u0628\u06a9\u0647 \u0646\u06cc\u0632 \u0628\u0627 \u0627\u0631\u0627\u0626\u0647 \u0628\u062e\u0634\u06cc \u0627\u0632 \u062c\u0632\u06cc\u06cc\u0627\u062a \u06af\u0632\u0627\u0631\u0634 \u0622\u0698\u0627\u0646\u0633\u060c \u0622\u0648\u0631\u062f\u0647 \u0627\u0633\u062a: \u0627\u06cc\u0631\u0627\u0646 \u0639\u0644\u0627\u0648\u0647 \u0628\u0631 \u0627\u06cc\u0646 \u0628\u0647 \u062f\u06cc\u06af\u0631 \u062a\u0639\u0647\u062f\u0627\u062a\u0634 \u0628\u0631\u0627\u0633\u0627\u0633 \u062a\u0648\u0627\u0641\u0642 \u0627\u0648\u0644\u06cc\u0647 \u0698\u0646\u0648 \u0639\u0645\u0644 \u0645\u06cc\u200c\u06a9\u0646\u062f.\u0627\u06cc\u0646 \u062f\u0631 \u062d\u0627\u0644\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0647\u0631\u06af\u0648\u0646\u0647 \u062a\u0645\u0627\u06cc\u0644 \u0628\u0631\u0627\u06cc \u062f\u0633\u062a\u06cc\u0627\u0628\u06cc \u0628\u0647 \u0633\u0644\u0627\u062d \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0631\u0627 \u0631\u062f \u0645\u06cc\u200c\u06a9\u0646\u062f \u0627\u0645\u0627 \u0637\u0628\u0642 \u062a\u0648\u0627\u0641\u0642 \u0627\u0648\u0644\u06cc\u0647\u200c\u0627\u0634 \u0628\u0627 \u06af\u0631\u0648\u0647 \u06f1+\u06f5 \u06a9\u0647 \u06a9\u0627\u0647\u0634 \u0628\u062e\u0634\u06cc \u0627\u0632 \u062a\u062d\u0631\u06cc\u0645\u200c\u0647\u0627\u06cc \u063a\u0631\u0628 \u0631\u0627 \u062f\u0631 \u067e\u06cc \u062f\u0627\u0634\u062a\u060c \u0641\u0639\u0627\u0644\u06cc\u062a\u200c\u0647\u0627\u06cc \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u062e\u0648\u062f \u0631\u0627 \u0645\u062d\u062f\u0648\u062f \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.\u0637\u0628\u0642 \u0627\u06cc\u0646 \u062a\u0648\u0627\u0641\u0642\u060c \u0627\u06cc\u0631\u0627\u0646 \u0645\u062a\u0639\u0647\u062f \u0634\u062f \u062a\u0627 \u0630\u062e\u06cc\u0631\u0647 \u0627\u0648\u0631\u0627\u0646\u06cc\u0648\u0645 \u063a\u0646\u06cc\u200c\u0634\u062f\u0647 \u062e\u0648\u062f \u062a\u0627 \u0633\u0637\u062d \u06f2\u06f0 \u062f\u0631\u0635\u062f \u0631\u0627 \u06a9\u0627\u0647\u0634 \u062f\u0647\u062f \u06cc\u0627 \u062a\u0628\u062f\u06cc\u0644 \u06a9\u0646\u062f.\u0627\u06cc\u0646 \u0634\u0628\u06a9\u0647 \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0622\u0648\u0631\u062f\u0647 \u0627\u0633\u062a\u060c \u0622\u0698\u0627\u0646\u0633 \u0628\u06cc\u0646\u200c\u0627\u0644\u0645\u0644\u0644\u06cc \u0627\u0646\u0631\u0698\u06cc \u0627\u062a\u0645\u06cc \u062f\u0631 \u0627\u0631\u0632\u06cc\u0627\u0628\u06cc\u200c\u0647\u0627\u06cc\u0634 \u062f\u0631\u0628\u0627\u0631\u0647 \u0647\u0645\u06a9\u0627\u0631\u06cc\u200c\u0647\u0627\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0628\u0627 \u062a\u0644\u0627\u0634\u200c\u0647\u0627\u06cc \u0622\u0698\u0627\u0646\u0633 \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u062a\u062d\u0642\u06cc\u0642\u0627\u062a \u062f\u0631 \u0645\u0648\u0631\u062f \u0627\u06cc\u0646 \u0627\u062a\u0647\u0627\u0645 \u06a9\u0647 \u0627\u06cc\u0631\u0627\u0646 \u0628\u0647 \u0637\u0648\u0631 \u0645\u062d\u0631\u0645\u0627\u0646\u0647 \u0631\u0648\u06cc \u062a\u0648\u0633\u0639\u0647 \u0633\u0644\u0627\u062d\u200c\u0647\u0627\u06cc \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u06a9\u0627\u0631 \u0645\u06cc\u200c\u06a9\u0646\u062f\u060c \u06a9\u0645\u200c\u062a\u0631 \u0645\u062b\u0628\u062a \u0628\u0648\u062f.\u0631\u0648\u0632\u0646\u0627\u0645\u0647 \u0635\u0647\u06cc\u0648\u0646\u06cc\u0633\u062a\u06cc \u0627\u0648\u0631\u0634\u0644\u06cc\u0645 \u067e\u0633\u062a \u0646\u06cc\u0632 \u0628\u0627 \u0627\u0646\u062a\u0634\u0627\u0631 \u0628\u062e\u0634\u200c\u0647\u0627\u06cc\u06cc \u0627\u0632 \u06af\u0632\u0627\u0631\u0634 \u0622\u0698\u0627\u0646\u0633 \u062f\u0631 \u0645\u0648\u0631\u062f \u067e\u0627\u06cc\u0628\u0646\u062f\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0628\u0647 \u062a\u0639\u0647\u062f\u0627\u062a\u0634 \u062f\u0631 \u0637\u0631\u062d \u0627\u0642\u062f\u0627\u0645 \u0645\u0634\u062a\u0631\u06a9 \u06a9\u0647 \u062f\u0631 \u0646\u0648\u0627\u0645\u0628\u0631 \u0633\u0627\u0644 \u06af\u0630\u0634\u062a\u0647 \u062f\u0631 \u0698\u0646\u0648 \u0628\u0647 \u062a\u0635\u0648\u06cc\u0628 \u0631\u0633\u06cc\u062f\u060c \u0646\u0648\u0634\u062a: \u0646\u062a\u0627\u06cc\u062c \u0628\u0647 \u062f\u0633\u062a \u0622\u0645\u062f\u0647 \u062f\u0631 \u06af\u0632\u0627\u0631\u0634 \u0641\u0635\u0644\u06cc \u0622\u0698\u0627\u0646\u0633 \u0628\u0647 \u0627\u062d\u062a\u0645\u0627\u0644 \u0632\u06cc\u0627\u062f \u0645\u0648\u0631\u062f \u0627\u0633\u062a\u0642\u0628\u0627\u0644 \u0634\u0634 \u0642\u062f\u0631\u062a \u062c\u0647\u0627\u0646\u06cc \u06a9\u0647 \u062a\u0644\u0627\u0634 \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f \u0628\u0647 \u062a\u0648\u0627\u0641\u0642 \u0646\u0647\u0627\u06cc\u06cc \u0628\u0627 \u0627\u06cc\u0631\u0627\u0646 \u0628\u0631\u0633\u0646\u062f\u060c \u0642\u0631\u0627\u0631 \u062e\u0648\u0627\u0647\u062f \u06af\u0631\u0641\u062a.\u062f\u0631 \u067e\u06cc \u0627\u0646\u062a\u0634\u0627\u0631 \u063a\u06cc\u0631\u0639\u0644\u0646\u06cc \u06af\u0632\u0627\u0631\u0634 \u0631\u0648\u0632 \u06af\u0630\u0634\u062a\u0647 \u0622\u0698\u0627\u0646\u0633\u060c \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u0641\u0631\u0627\u0646\u0633\u0647 \u0646\u06cc\u0632 \u062f\u0631 \u0627\u06cc\u0646\u200c\u0628\u0627\u0631\u0647 \u0646\u0648\u0634\u062a: \u0647\u0645\u200c\u0632\u0645\u0627\u0646 \u06a9\u0647 \u0636\u0631\u0628\u200c\u0627\u0644\u0627\u062c\u0644 \u06f2\u06f0 \u0698\u0648\u06cc\u06cc\u0647 \u0628\u0631\u0627\u06cc \u062a\u0648\u0627\u0641\u0642 \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0628\u0627 \u0627\u06cc\u0631\u0627\u0646 \u0646\u0632\u062f\u06cc\u06a9 \u0645\u06cc\u200c\u0634\u0648\u062f\u060c \u06af\u0632\u0627\u0631\u0634 \u062f\u06cc\u062f\u0647\u200c\u0628\u0627\u0646 \u0647\u0633\u062a\u0647\u200c\u0627\u06cc \u0633\u0627\u0632\u0645\u0627\u0646 \u0645\u0644\u0644 \u0646\u0634\u0627\u0646 \u0645\u06cc\u200c\u062f\u0647\u062f \u06a9\u0647 \u062a\u0647\u0631\u0627\u0646 \u0628\u0647 \u062a\u0639\u0647\u062f\u0627\u0634 \u0628\u0631\u0627\u06cc \u0645\u062d\u062f\u0648\u062f \u06a9\u0631\u062f\u0646 \u0628\u0631\u0646\u0627\u0645\u0647 \u0647\u0633\u062a\u0647\u200c\u0627\u06cc\u200c\u0627\u0634 \u0639\u0645\u0644 \u0645\u06cc\u200c\u06a9\u0646\u062f.
p59
aV\u06cc\u06a9 \u0645\u0646\u0628\u0639 \u0622\u06af\u0627\u0647 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f \u06a9\u0647 \u062d\u06a9\u0645 \u062c\u0644\u0628 «\u0639\u0628\u062f\u0627\u0644\u0631\u0636\u0627 \u062f\u0627\u0648\u0631\u06cc» \u06cc\u06a9\u06cc \u0627\u0632 \u0641\u0639\u0627\u0644\u0627\u0646 \u0631\u0633\u0627\u0646\u0647 \u0627\u06cc \u0646\u0632\u062f\u06cc\u06a9 \u0628\u0647 \u062c\u0631\u06cc\u0627\u0646 \u0627\u0646\u062d\u0631\u0627\u0641\u06cc \u0635\u0627\u062f\u0631 \u0634\u062f\u0647 \u0627\u0633\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0631\u0646\u0627\u060c \u0627\u06cc\u0646 \u0645\u0646\u0628\u0639 \u0631\u0648\u0632 \u0634\u0646\u0628\u0647 \u0627\u0641\u0632\u0648\u062f: \u0627\u062a\u0647\u0627\u0645\u0627\u062a \u062f\u0627\u0648\u0631\u06cc \u0646\u0634\u0631 \u0627\u06a9\u0627\u0630\u06cc\u0628 \u0648 \u0627\u0646\u062a\u0633\u0627\u0628 \u062f\u0631\u0648\u063a \u0628\u0647 \u0645\u0633\u0626\u0648\u0644\u0627\u0646 \u0627\u0633\u062a.\u0628\u0646\u0627 \u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0646 \u0645\u0646\u0628\u0639\u060c \u062f\u0627\u0648\u0631\u06cc \u062a\u0627\u06a9\u0646\u0648\u0646 \u0627\u0632 \u062d\u0636\u0648\u0631 \u062f\u0631 \u0645\u0631\u0627\u062c\u0639 \u0642\u0636\u0627\u06cc\u06cc \u062e\u0648\u062f\u062f\u0627\u0631\u06cc \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.\u062f\u0627\u0648\u0631\u06cc \u06a9\u0647 \u062f\u0631 \u0633\u0627\u0644\u200c\u0647\u0627\u06cc \u067e\u0627\u06cc\u0627\u0646 \u062f\u0648\u0644\u062a \u062f\u0647\u0645 \u0631\u06cc\u0627\u0633\u062a \u0635\u0646\u062f\u0648\u0642 \u0628\u0627\u0632\u0646\u0634\u0633\u062a\u06af\u06cc \u0645\u0633 \u06a9\u0634\u0648\u0631 \u0631\u0627 \u0628\u0631\u0639\u0647\u062f\u0647 \u062f\u0627\u0634\u062a\u060c \u062f\u0631 \u0637\u0648\u0644 \u062f\u0648\u0644\u062a \u0646\u0647\u0645 \u0648 \u062f\u0647\u0645 \u0627\u0632 \u0627\u0635\u0644\u06cc\u200c\u062a\u0631\u06cc\u0646 \u0686\u0647\u0631\u0647\u200c\u0647\u0627\u06cc \u0631\u0633\u0627\u0646\u0647 \u0627\u06cc \u0648\u0627\u0628\u0633\u062a\u0647 \u0628\u0647 \u062c\u0631\u06cc\u0627\u0646 \u0627\u0646\u062d\u0631\u0627\u0641\u06cc \u0634\u0646\u0627\u062e\u062a\u0647 \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u067e\u06cc\u0634 \u0627\u0632 \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0633\u0627\u0644 \u06f8\u06f8 \u0646\u06cc\u0632 \u0645\u0633\u0626\u0648\u0644\u06cc\u062a \u0627\u062f\u0627\u0631\u0647 \u06a9\u0644 \u067e\u0698\u0648\u0647\u0634 \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0645\u0639\u0627\u0648\u0646\u062a \u062e\u0628\u0631 \u062e\u0628\u0631\u06af\u0632\u0627\u0631\u06cc \u062c\u0645\u0647\u0648\u0631\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0631\u0627 \u0628\u0631\u0639\u0647\u062f\u0647 \u062f\u0627\u0634\u062a.\u0648\u06cc \u062f\u0631 \u062c\u0631\u06cc\u0627\u0646 \u062b\u0628\u062a \u0646\u0627\u0645 \u0646\u0627\u0645\u0632\u062f\u0647\u0627 \u0628\u0631\u0627\u06cc \u0627\u0646\u062a\u062e\u0627\u0628\u0627\u062a \u0631\u06cc\u0627\u0633\u062a \u062c\u0645\u0647\u0648\u0631\u06cc \u062e\u0631\u062f\u0627\u062f \u0645\u0627\u0647 \u06f9\u06f2 \u062a\u0627 \u067e\u06cc\u0634 \u0627\u0632 \u0627\u0639\u0644\u0627\u0645 \u0635\u0644\u0627\u062d\u06cc\u062a\u200c\u0647\u0627\u060c \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u06cc\u06a9\u06cc \u0627\u0632 \u062d\u0627\u0645\u06cc\u0627\u0646 \u0627\u0635\u0644\u06cc \u0646\u0627\u0645\u0632\u062f \u0645\u0648\u0631\u062f \u067e\u0634\u062a\u06cc\u0628\u0627\u0646\u06cc \u0627\u062d\u0645\u062f\u06cc \u0646\u0698\u0627\u062f\u060c \u062f\u0631 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627 \u0641\u0639\u0627\u0644\u06cc\u062a \u062f\u0627\u0634\u062a.
p60
aV"\u062f\u0627\u0633\u062a\u0627\u0646 \u0632\u0646\u062f\u06af\u06cc" \u0628\u0627 \u0646\u0627\u0645 \u0627\u0635\u0644\u06cc "\u0647\u0627\u0646\u06cc\u06a9\u0648" \u0628\u0627 \u0645\u062d\u0648\u0631\u06cc\u062a \u0632\u0646\u0627\u0646 \u0627\u0633\u062a \u06a9\u0647 \u062f\u0631 \u0633\u0627\u0644 \u06f1\u06f9\u06f8\u06f6 \u0648 \u062f\u0631 \u06f1\u06f5\u06f6 \u0642\u0633\u0645\u062a \u06f1\u06f5 \u062f\u0642\u06cc\u0642\u0647\u200c\u0627\u06cc \u062a\u0648\u0644\u06cc\u062f \u0634\u062f\u0647 \u0627\u0633\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0627\u06cc\u0646 \u0645\u062c\u0645\u0648\u0639\u0647\u060c \u0698\u0627\u067e\u0646\u0650 \u062f\u0631 \u062d\u0627\u0644 \u06af\u0630\u0627\u0631 \u0627\u0632 \u0633\u0646\u062a \u0628\u0647 \u0645\u062f\u0631\u0646\u06cc\u062a\u0647 \u062f\u0648\u0631\u0627\u0646 \u0645\u06cc\u062c\u06cc \u0648 \u062a\u0627\u06cc\u0634\u0648 \u0631\u0627 \u0628\u0647 \u062a\u0635\u0648\u06cc\u0631 \u0645\u06cc\u200c\u06a9\u0634\u062f \u0648 \u062f\u0627\u0633\u062a\u0627\u0646 \u062f\u062e\u062a\u0631\u06cc \u0628\u0647 \u0646\u0627\u0645 \u0631\u06cc\u0646 (\u0647\u0627\u0646\u06cc\u06a9\u0648) \u0631\u0627 \u0631\u0648\u0627\u06cc\u062a \u0645\u06cc\u200c\u06a9\u0646\u062f \u06a9\u0647 \u0628\u0627 \u062a\u0634\u0648\u06cc\u0642 \u0627\u0633\u062a\u0627\u062f \u0648 \u0633\u0631\u0633\u062e\u062a\u06cc \u062e\u0648\u062f\u0634 \u0645\u0648\u0641\u0642 \u0628\u0647 \u0627\u062f\u0627\u0645\u0647 \u062a\u062d\u0635\u06cc\u0644 \u0634\u062f\u0647 \u0648 \u0628\u0647 \u0639\u0646\u0648\u0627\u0646 \u0627\u0648\u0644\u06cc\u0646 \u062e\u0628\u0631\u0646\u06af\u0627\u0631 \u0632\u0646 \u0698\u0627\u067e\u0646\u06cc \u0628\u0647 \u0627\u0633\u062a\u062e\u062f\u0627\u0645 \u06cc\u06a9\u06cc \u0627\u0632 \u0631\u0648\u0632\u0646\u0627\u0645\u0647\u200c\u0647\u0627\u06cc \u0622\u0646 \u062f\u0648\u0631\u0627\u0646 \u062f\u0631\u0645\u06cc\u200c\u0622\u06cc\u062f.\u0627\u06cc\u0646 \u0633\u0631\u06cc\u0627\u0644 \u062f\u0631 \u0627\u06cc\u0631\u0627\u0646 \u0648 \u0628\u0627 \u0646\u0627\u0645 "\u062f\u0627\u0633\u062a\u0627\u0646 \u0632\u0646\u062f\u06af\u06cc" \u0628\u0631\u0627\u06cc \u0627\u0648\u0644\u06cc\u0646 \u0628\u0627\u0631 \u0627\u0632 \u0634\u0628\u06a9\u0647 \u062f\u0648 \u067e\u062e\u0634 \u0634\u062f\u0647 \u0627\u0633\u062a."\u062f\u0627\u0633\u062a\u0627\u0646 \u0632\u0646\u062f\u06af\u06cc" \u0627\u0632 \u0634\u0634\u0645 \u062e\u0631\u062f\u0627\u062f \u0633\u0627\u0639\u062a \u06f1\u06f8 \u0627\u0632 \u0634\u0628\u06a9\u0647 \u062a\u0645\u0627\u0634\u0627 \u067e\u062e\u0634 \u0645\u06cc\u200c\u0634\u0648\u062f.
p61
aV\u0639\u0635\u0631\u0627\u06cc\u0631\u0627\u0646\u061b \u200c \u0639\u0628\u062f\u0627\u0644\u0631\u062d\u06cc\u0645 \u0627\u0628\u0627\u0630\u0631\u06cc - \u0633\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u0633\u0627\u0644 \u06f1\u06f3\u06f6\u06f1 \u06cc\u0627\u062f \u0622\u0648\u0631 \u062d\u0645\u0627\u0633\u0647 \u062a\u0627\u0631\u06cc\u062e\u06cc \u0622\u0632\u0627\u062f \u0633\u0627\u0632\u06cc \u0634\u0647\u0631\u062e\u0631\u0645\u0634\u0647\u0631 \u0627\u0633\u062a.\u0627\u06cc\u0646 \u0631\u062e\u062f\u0627\u062f \u0645\u0627\u0646\u062f\u06af\u0627\u0631 \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0645\u0631\u062d\u0644\u0647 \u0686\u0647\u0627\u0631\u0645 \u0639\u0645\u0644\u06cc\u0627\u062a \u0628\u06cc\u062a \u0627\u0644\u0645\u0642\u062f\u0633 \u0627\u0646\u062c\u0627\u0645 \u06af\u0631\u0641\u062a \u06a9\u0647 \u0628\u0627 \u0631\u0645\u0632 «\u06cc\u0627 \u0639\u0644\u06cc \u0628\u0646 \u0627\u0628\u06cc \u0637\u0627\u0644\u0628» \u0622\u063a\u0627\u0632 \u0634\u062f\u0647 \u0628\u0648\u062f.\u0686\u0646\u062f \u0631\u0648\u0632 \u0645\u0627\u0646\u062f\u0647 \u0628\u0647 \u0634\u0631\u0648\u0639 \u0639\u0645\u0644\u06cc\u0627\u062a\u060c \u0645\u062f\u0631\u0633\u0647 \u0639\u0644\u0645\u06cc\u0647 \u0648\u0644\u06cc \u0639\u0635\u0631 (\u0639\u062c) \u062a\u0628\u0631\u06cc\u0632 \u062a\u0639\u0637\u06cc\u0644 \u0634\u062f.\u0645\u062f\u06cc\u0631\u06cc\u062a \u0645\u062d\u062a\u0631\u0645\u060c \u0627\u0633\u0627\u062a\u06cc\u062f \u0648 \u0637\u0644\u0627\u0628 \u062d\u0636\u0648\u0631 \u062f\u0631 \u0633\u0646\u06af\u0631 \u062e\u0627\u06a9\u06cc \u0631\u0627 \u0628\u0647 \u0633\u0646\u06af\u0631 \u0641\u0631\u0647\u0646\u06af\u06cc \u062a\u0631\u062c\u06cc\u062d \u062f\u0627\u062f\u0646\u062f \u062a\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u062a\u0627\u0631\u06cc\u062e\u06cc \u062f\u0631 \u06a9\u0646\u0627\u0631 \u0631\u0632\u0645\u0646\u062f\u06af\u0627\u0646 \u0627\u0633\u0644\u0627\u0645 \u0628\u0627\u0634\u0646\u062f \u0648\u0622\u0646\u0647\u0627 \u0631\u0627 \u06cc\u0627\u0631\u06cc \u062f\u0647\u0646\u062f \u06a9\u0647 \u0628\u0631\u0627\u06cc \u0646\u06af\u0627\u0631\u0646\u062f\u0647 \u0646\u06cc\u0632\u062a\u0648\u0641\u06cc\u0642 \u0647\u0645\u0631\u0627\u0647\u06cc \u0628\u0627 \u0627\u06cc\u0646 \u06a9\u0627\u0631\u0648\u0627\u0646 \u062d\u0627\u0635\u0644 \u0634\u062f.\u0622\u0632\u0627\u062f\u06cc "\u062e\u0648\u0646\u06cc\u0646 \u0634\u0647\u0631" (\u0646\u0627\u0645\u06cc \u06a9\u0647 \u062f\u0631 \u062c\u0631\u06cc\u0627\u0646 \u062c\u0646\u06af \u0628\u0647 \u062e\u0631\u0645\u0634\u0647\u0631 \u0627\u0637\u0644\u0627\u0642 \u0645\u06cc\u200c\u0634\u062f) \u06cc\u06a9 \u062d\u0627\u062f\u062b\u0647 \u0634\u06af\u0641\u062a \u0627\u0646\u06af\u06cc\u0632 \u0628\u0648\u062f\u060c \u062e\u06cc\u0644\u06cc \u0631\u0627\u062d\u062a \u0627\u0645\u062f\u0627\u062f\u0647\u0627\u06cc \u063a\u06cc\u0628\u06cc \u0631\u0627 \u0645\u06cc\u200c\u0634\u062f \u062f\u0631 \u0622\u0646 \u0634\u0628 \u0648 \u0631\u0648\u0632\u0647\u0627\u06cc \u0628\u0639\u062f \u0645\u0634\u0627\u0647\u062f\u0647 \u0648\u0644\u0645\u0633 \u06a9\u0631\u062f.\u0631\u0639\u0628 \u0648 \u0648\u062d\u0634\u062a \u0648 \u0633\u0631\u062f\u0631\u06af\u0645\u06cc \u0633\u0631\u0627\u067e\u0627\u06cc \u0648\u062c\u0648\u062f \u0633\u0631\u0628\u0627\u0632\u0627\u0646 \u0648\u0641\u0631\u0645\u0627\u0646\u062f\u0647\u0627\u0646 \u0639\u0631\u0627\u0642\u06cc \u0631\u0627 \u062a\u0633\u062e\u06cc\u0631 \u06a9\u0631\u062f\u0647 \u0628\u0648\u062f.\u0644\u062d\u0638\u0627\u062a \u0627\u0648\u0644\u06cc\u0647 \u0634\u06a9\u0633\u062a \u0648\u0641\u0631\u0627\u0631 \u062f\u0634\u0645\u0646  \u0648 \u0622\u063a\u0627\u0632 \u0648\u0631\u0648\u062f \u0631\u0632\u0645\u0646\u062f\u06af\u0627\u0646 \u0628\u0647 \u062e\u0631\u0645\u0634\u0647\u0631 \u0628\u0639\u062f \u0627\u0632 \u06f5\u06f7\u06f5 \u0631\u0648\u0632 \u0627\u0634\u063a\u0627\u0644\u060c \u0628\u0633\u06cc\u0627\u0631 \u062f\u06cc\u062f\u0646\u06cc \u0628\u0648\u062f \u0648 \u0633\u0631\u0627\u0646\u062c\u0627\u0645 \u0627\u0633\u0627\u0631\u062a \u062f\u0633\u062a\u0647 \u062c\u0645\u0639\u06cc\u060c \u06af\u0631\u0648\u0647\u0627\u0646\u06cc \u0648\u06af\u0631\u062f\u0627\u0646\u06cc \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0635\u062f\u0627\u0645\u06cc\u060c \u0628\u0647 \u063a\u0646\u06cc\u0645\u062a \u06af\u0631\u0641\u062a\u0647 \u0634\u062f\u0646 \u0635\u062f\u0647\u0627 \u062a\u0648\u067e \u0648\u062a\u0627\u0646\u06a9\u060c \u0646\u0641\u0631\u0628\u0631\u060c \u0627\u0646\u0648\u0627\u0639 \u062e\u0648\u062f\u0631\u0648\u200c\u0647\u0627 \u0648\u0633\u0627\u06cc\u0631 \u062a\u062c\u0647\u06cc\u0632\u0627\u062a \u0646\u0638\u0627\u0645\u06cc \u0647\u0631\u06a9\u062f\u0627\u0645 \u0635\u062d\u0646\u0647\u200c\u0647\u0627\u06cc \u0645\u0627\u0646\u062f\u06af\u0627\u0631\u06cc \u0628\u0648\u062f\u0646\u062f \u06a9\u0647 \u0634\u06cc\u0631\u06cc\u0646\u06cc \u0648\u0634\u0627\u062f\u06cc \u0627\u06cc\u0646 \u067e\u06cc\u0631\u0648\u0632\u06cc \u0631\u0627 \u062f\u0631 \u06a9\u0627\u0645 \u0631\u0632\u0645\u0646\u062f\u06af\u0627\u0646 \u0648\u0645\u0644\u062a \u0627\u06cc\u0631\u0627\u0646 \u0645\u0636\u0627\u0639\u0641 \u0645\u06cc\u200c\u06a9\u0631\u062f.\u0627\u0645\u0627\u0645 \u062e\u0645\u06cc\u0646\u06cc \u0637\u06cc \u067e\u06cc\u0627\u0645 \u0645\u0647\u0645\u06cc \u0627\u06cc\u0646 \u067e\u06cc\u0631\u0648\u0632\u06cc \u0628\u0632\u0631\u06af \u0631\u0627 \u0628\u0647 \u0645\u0644\u062a \u0627\u06cc\u0631\u0627\u0646 \u0648\u0631\u0632\u0645\u0646\u062f\u06af\u0627\u0646 \u062a\u0628\u0631\u06cc\u06a9 \u06af\u0641\u062a\u0646\u062f \u0648 \u062f\u0631 \u0622\u0646 \u0628\u0647 \u06cc\u06a9 \u0646\u06a9\u062a\u0647 \u062d\u06cc\u0627\u062a\u06cc \u0648\u0627\u0633\u0627\u0633\u06cc \u0627\u0634\u0627\u0631\u0647 \u0648\u062a\u0627\u06a9\u06cc\u062f \u06a9\u0631\u062f\u0647 \u0648\u0686\u0646\u06cc\u0646 \u0646\u0648\u0634\u062a\u0646\u062f: «\u0648 \u0647\u0627\u0646 \u0627\u06cc \u0641\u0631\u0632\u0646\u062f\u0627\u0646 \u0642\u0631\u0622\u0646 \u06a9\u0631\u06cc\u0645 \u0648 \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0627\u0631\u062a\u0634 \u0648\u0633\u067e\u0627\u0647\u06cc \u0648 \u0628\u0633\u06cc\u062c \u0648 \u0698\u0627\u0646\u062f\u0627\u0631\u0645\u0631\u06cc \u0648 \u0634\u0647\u0631\u0628\u0627\u0646\u06cc \u0648 \u06a9\u0645\u06cc\u062a\u0647\u200c\u0647\u0627 \u0648\u0639\u0634\u0627\u06cc\u0631 \u0648 \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0645\u0631\u062f\u0645\u06cc \u062f\u0627\u0648\u0637\u0644\u0628 \u0648\u0645\u0644\u062a \u0639\u0632\u06cc\u0632!\u0647\u0648\u0634\u06cc\u0627\u0631 \u0628\u0627\u0634\u06cc\u062f \u06a9\u0647 \u067e\u06cc\u0631\u0648\u0632\u06cc\u200c\u0647\u0627 \u0647\u0631 \u0686\u0646\u062f \u0639\u0638\u06cc\u0645 \u0648\u062d\u06cc\u0631\u062a \u0627\u0646\u06af\u06cc\u0632 \u0627\u0633\u062a\u060c \u0634\u0645\u0627 \u0631\u0627 \u0627\u0632 \u06cc\u0627\u062f \u062e\u062f\u0627\u0648\u0646\u062f \u06a9\u0647 \u0646\u0635\u0631 \u0648 \u0641\u062a\u062d \u062f\u0631 \u062f\u0633\u062a \u0627\u0648\u0633\u062a \u063a\u0627\u0641\u0644 \u0646\u06a9\u0646\u062f \u0648\u063a\u0631\u0648\u0631 \u0648 \u0641\u062a\u062d \u0634\u0645\u0627 \u0631\u0627 \u0628\u0647 \u062e\u0648\u062f \u062c\u0644\u0628 \u0646\u06a9\u0646\u062f \u06a9\u0647 \u0627\u06cc\u0646 \u0622\u0641\u062a\u06cc \u0628\u0632\u0631\u06af \u0648\u062f\u0627\u0645\u06cc \u062e\u0637\u0631\u0646\u0627\u06a9 \u0627\u0633\u062a \u06a9\u0647 \u0628\u0627 \u0648\u0633\u0648\u0633\u0647 \u0634\u06cc\u0637\u0627\u0646 \u0628\u0647 \u0633\u0631\u0627\u063a \u0622\u062f\u0645 \u0645\u06cc\u200c\u0622\u06cc\u062f \u0648\u0628\u0631\u0627\u06cc \u0627\u0648\u0644\u0627\u062f \u0622\u062f\u0645 \u062a\u0628\u0627\u0647\u06cc \u0645\u06cc\u200c\u0622\u0648\u0631\u062f.» (\u0635\u062d\u06cc\u0641\u0647 \u0627\u0645\u0627\u0645\u060c \u062c\u06f1\u06f6\u060c \u0635 \u06f2\u06f5\u06f8) \u0627\u0645\u0627\u0645 \u0628\u0627 \u0627\u06cc\u0646 \u0647\u0634\u062f\u0627\u0631 \u062d\u06a9\u06cc\u0645\u0627\u0646\u0647 \u062f\u0631 \u0648\u0627\u0642\u0639 \u0645\u06cc\u200c\u062e\u0648\u0627\u0633\u062a \u0628\u0647 \u0647\u0645\u0647 \u0645\u0644\u062a \u0648 \u0628\u062e\u0635\u0648\u0635 \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0646\u0638\u0627\u0645\u06cc \u06af\u0648\u0634\u0632\u062f \u0646\u0645\u0627\u06cc\u062f \u06a9\u0647 \u062d\u0636\u0648\u0631 \u062f\u0631 «\u062c\u0647\u0627\u062f \u0627\u0635\u063a\u0631» \u0648\u063a\u0644\u0628\u0647 \u0628\u0631 \u062f\u0634\u0645\u0646 \u062f\u0631 \u0633\u0646\u06af\u0631\u0647\u0627\u06cc \u062e\u0627\u06a9\u06cc \u0627\u06af\u0631\u0686\u0647 \u0645\u0647\u0645 \u0627\u0633\u062a \u0627\u0645\u0627 \u0645\u0647\u0645\u200c\u062a\u0631 \u0627\u0632 \u0622\u0646\u060c «\u062c\u0647\u0627\u062f \u0627\u06a9\u0628\u0631» \u0648\u0645\u0628\u0627\u0631\u0632\u0647 \u0628\u0627 \u0646\u0641\u0633 \u0634\u06cc\u0637\u0627\u0646\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0627\u0646\u0633\u0627\u0646 \u0627\u06af\u0631 \u062d\u0648\u0627\u0633\u0634 \u0646\u0628\u0627\u0634\u062f \u0627\u0648 \u0631\u0627 \u0628\u0647 \u0627\u0641\u0631\u0627\u0637 \u0648 \u062a\u0641\u0631\u06cc\u0637 \u0648\u0633\u0627\u06cc\u0631 \u0631\u0630\u0627\u0626\u0644 \u0627\u062e\u0644\u0627\u0642\u06cc \u0633\u0648\u0642 \u0645\u06cc\u200c\u062f\u0647\u062f.\u0686\u0646\u0627\u0646\u06a9\u0647 \u062f\u0631 \u0635\u062f\u0631 \u0627\u0633\u0644\u0627\u0645 \u0648\u0642\u062a\u06cc \u062c\u0645\u0627\u0639\u062a\u06cc \u0627\u0632 \u0645\u06cc\u062f\u0627\u0646 \u062c\u0646\u06af \u0628\u0631 \u0645\u06cc\u200c\u06af\u0634\u062a\u0646\u062f\u060c \u067e\u06cc\u0627\u0645\u0628\u0631 \u0627\u0633\u0644\u0627\u0645 \u062e\u0637\u0627\u0628 \u0628\u0647 \u0622\u0646\u0627\u0646 \u0641\u0631\u0645\u0648\u062f: «\u0645\u0631\u062d\u0628\u0627 \u0628\u0642\u0648\u0645 \u0642\u0636\u0648\u0627 \u0627\u0644\u062c\u0647\u0627\u062f \u0627\u0644\u0627\u0635\u063a\u0631 \u0648\u0628\u0642\u06cc \u0627\u0644\u062c\u0647\u0627\u062f\u0627\u0644\u0627\u06a9\u0628\u0631» \u0645\u0631\u062d\u0628\u0627 \u0628\u0647 \u0642\u0648\u0645\u06cc \u06a9\u0647 \u062c\u0647\u0627\u062f \u0627\u0635\u063a\u0631 \u0631\u0627 \u0627\u0646\u062c\u0627\u0645 \u062f\u0627\u062f\u0646\u062f \u0648 \u0647\u0646\u0648\u0632 \u062c\u0647\u0627\u062f \u0627\u06a9\u0628\u0631 \u0628\u0631\u0627\u06cc \u0622\u0646\u0647\u0627 \u0628\u0627\u0642\u06cc \u0645\u0627\u0646\u062f\u0647 \u0627\u0633\u062a.\u0639\u0631\u0636 \u06a9\u0631\u062f\u0646\u062f: \u06cc\u0627 \u0631\u0633\u0648\u0644 \u0627\u0644\u0644\u0647 \u062c\u0647\u0627\u062f \u0627\u06a9\u0628\u0631 \u0686\u06cc\u0633\u062a\u061f\u0641\u0631\u0645\u0648\u062f: \u062c\u0647\u0627\u062f \u0628\u0627 \u0646\u0641\u0633.(\u06a9\u0627\u0641\u06cc\u060c \u062c \u06f5\u060c \u0635\u06f1\u06f2\u060c \u0628\u0627\u0628 \u0648\u062c\u0648\u0647 \u0627\u0644\u062c\u0647\u0627\u062f) \u0627\u0645\u0627\u0645 \u0628\u0647 \u062e\u0648\u0628\u06cc \u0645\u06cc\u200c\u062f\u0627\u0646\u0633\u062a \u06a9\u0647 \u0647\u062f\u0641 \u0627\u0635\u0644\u06cc \u0627\u0632 \u0628\u0639\u062b\u062a \u067e\u06cc\u0627\u0645\u0628\u0631\u0635\u0644\u06cc \u0627\u0644\u0644\u0647 \u0639\u0644\u06cc\u0647 \u0648\u0622\u0644\u0647 \u0648\u0633\u0644\u0645\u060c \u0631\u0634\u062f \u0645\u06a9\u0627\u0631\u0645 \u0627\u062e\u0644\u0627\u0642\u06cc \u0648 \u062f\u0633\u062a \u06cc\u0627\u0641\u062a\u0646 \u0628\u0647 \u0645\u0631\u0627\u062a\u0628 \u0639\u0627\u0644\u06cc \u0627\u0646\u0633\u0627\u0646\u06cc \u0628\u0648\u062f \u0648 \u0647\u0645\u0647 \u0627\u06cc\u0646 \u062c\u0646\u06af \u0648\u06af\u0631\u06cc\u0632\u200c\u0647\u0627 \u0648\u062c\u0647\u0627\u062f\u200c\u0647\u0627 \u0648\u0633\u06cc\u0644\u0647 \u0631\u0633\u06cc\u062f\u0646 \u0628\u0647 \u062c\u0647\u0627\u062f \u0627\u06a9\u0628\u0631 \u0648\u062a\u0647\u0630\u06cc\u0628 \u0646\u0641\u0633 \u0627\u0633\u062a\u060c \u0627\u0632 \u0633\u0648\u06cc \u062f\u06cc\u06af\u0631 \u0686\u0648\u0646 \u062f\u0631 \u06cc\u06a9\u06cc \u062f\u0648\u0633\u0627\u0644\u0642\u0628\u0644 \u0627\u0632 \u0622\u0646\u060c \u200c \u0634\u0627\u0647\u062f \u0627\u0641\u0631\u0627\u0637 \u0648\u062a\u0641\u0631\u06cc\u0637 \u0648\u062a\u0646\u062f\u0631\u0648\u06cc \u0628\u0631\u062e\u06cc \u0646\u0638\u0627\u0645\u06cc\u0627\u0646 \u062f\u0631 \u0639\u0631\u0635\u0647\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0628\u0648\u062f\u060c \u0644\u0630\u0627 \u0645\u0631\u062a\u0628 \u062f\u0631 \u0641\u0631\u0635\u062a\u200c\u0647\u0627\u06cc \u0645\u0642\u062a\u0636\u06cc \u0627\u06cc\u0646 \u062a\u0630\u06a9\u0631\u0627\u062a \u0631\u0627 \u062a\u06a9\u0631\u0627\u0631 \u0648\u062a \u0627\u06a9\u06cc\u062f \u0645\u06cc\u200c\u06a9\u0631\u062f.\u0633\u0647 \u0631\u0648\u0632 \u0628\u0639\u062f \u0627\u0632 \u0641\u062a\u062d \u062e\u0631\u0645\u0634\u0647\u0631\u060c \u0634\u0634\u0645 \u062e\u0631\u062f\u0627\u062f\u060c \u0645\u0635\u0627\u062f\u0641 \u0628\u0627 \u0633\u0648\u0645 \u0634\u0639\u0628\u0627\u0646 \u06f1\u06f4\u06f0\u06f3 \u0648 \u0631\u0648\u0632 \u067e\u0627\u0633\u062f\u0627\u0631\u060c \u0627\u0645\u0627\u0645 \u0628\u0647 \u0647\u0645\u06cc\u0646 \u0645\u0646\u0627\u0633\u0628\u062a \u067e\u06cc\u0627\u0645 \u062f\u06cc\u06af\u0631\u06cc \u062f\u0627\u062f\u0646\u062f \u0648 \u062f\u0631\u0622\u0646 \u0636\u0645\u0646 \u062a\u0648\u0635\u06cc\u0647 \u0628\u0647 \u062f\u0644\u062c\u0648\u06cc\u06cc \u0648\u0631\u0641\u062a\u0627\u0631 \u0627\u0633\u0644\u0627\u0645\u06cc \u0648\u0627\u0646\u0633\u0627\u0646\u06cc \u0628\u0627 \u0627\u0633\u0631\u0627\u06cc \u0639\u0631\u0627\u0642\u06cc \u062f\u0648\u0628\u0627\u0631\u0647 \u0646\u0648\u0634\u062a\u0646\u062f: «\u0628\u0631\u0627\u062f\u0631\u0627\u0646 \u062f\u0644\u0627\u0648\u0631 \u0648\u0646\u0648\u0631\u0686\u0634\u0645\u0627\u0646 \u0639\u0632\u06cc\u0632!\u0645\u0628\u0627\u062f\u0627 \u0622\u0641\u062a\u200c\u0647\u0627\u06cc \u067e\u06cc\u0631\u0648\u0632\u06cc \u06a9\u0647 \u062f\u0631 \u0631\u0627\u0633 \u0622\u0646 \u063a\u0631\u0648\u0631 \u0627\u0633\u062a\u060c \u062e\u062f\u0627\u06cc \u0646\u0627\u062e\u0648\u0627\u0633\u062a\u0647 \u0634\u0645\u0627 \u0631\u0627 \u0628\u0647 \u062f\u0627\u0645 \u06a9\u0634\u062f \u0648\u0627\u0632 \u0635\u0627\u062d\u0628 \u0627\u0635\u0644\u06cc \u0646\u0635\u0631 \u06a9\u0647 \u062e\u062f\u0627\u0648\u0646\u062f \u0642\u0627\u062f\u0631 \u0627\u0633\u062a \u0627\u063a\u0641\u0627\u0644\u062a\u0627\u0646 \u0646\u0645\u0627\u06cc\u062f\u060c \u06a9\u0647 \u0627\u06cc\u0646 \u062d\u06cc\u0644\u0647 \u0627\u06cc \u0627\u0633\u062a \u0634\u06cc\u0637\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0646\u0628\u0627\u0644 \u0622\u0646 \u0639\u0644\u0627\u0648\u0647 \u0628\u0631 \u0627\u0632\u062f\u0633\u062a \u0631\u0641\u062a\u0646 \u0645\u0639\u0646\u0648\u06cc\u062a \u06a9\u0647 \u0627\u0633\u0627\u0633 \u067e\u06cc\u0631\u0648\u0632\u06cc \u0634\u0645\u0627 \u0639\u0632\u06cc\u0632\u0627\u0646 \u0627\u0633\u062a \u0645\u0648\u062c\u0628 \u063a\u0641\u0644\u062a \u0627\u0632 \u062d\u06cc\u0644\u0647\u200c\u0647\u0627\u06cc \u062f\u0634\u0645\u0646\u0627\u0646 \u0646\u06cc\u0632 \u0645\u06cc\u200c\u0628\u0627\u0634\u062f \u06a9\u0647 \u0634\u06a9\u0633\u062a \u0631\u0627 \u0647\u0645\u0631\u0627\u0647 \u062f\u0627\u0631\u062f...\u0633\u067e\u0627\u0647 \u0648\u0633\u0627\u06cc\u0631\u06cc\u0646 \u0628\u0627\u06cc\u062f \u0642\u0644\u0648\u0628 \u0622\u0646\u0627\u0646 \u0631\u0627 \u062c\u0644\u0628 \u0648\u0631\u0636\u0627\u06cc \u062e\u062f\u0627\u0648\u0646\u062f \u0631\u0627 \u0628\u0647 \u062f\u0633\u062a \u0622\u0648\u0631\u0646\u062f.» (\u0647\u0645\u0627\u0646\u060c \u062c\u06f1\u06f6\u060c \u0635\u06f2\u06f6\u06f7\u0648\u06f2\u06f6\u06f8) \u062f\u0631 \u0645\u06a9\u062a\u0628 \u0627\u062e\u0644\u0627\u0642\u06cc \u0648\u0639\u0631\u0641\u0627\u0646\u06cc \u0627\u0645\u0627\u0645 \u062e\u0645\u06cc\u0646\u06cc \u067e\u06cc\u0631\u0648\u0632\u06cc \u0628\u0631 \u0631\u0698\u06cc\u0645 \u062f\u0648\u0647\u0632\u0627\u0631 \u0648\u067e\u0627\u0646\u0635\u062f \u0633\u0627\u0644\u0647 \u0634\u0627\u0647\u0646\u0634\u0627\u0645\u06cc\u060c \u0622\u0645\u0631\u06cc\u06a9\u0627 \u0648\u0627\u0633\u0631\u0627\u0626\u06cc\u0644 \u0648 \u063a\u0644\u0628\u0647 \u0628\u0631 \u0646\u06cc\u0631\u0648\u0647\u0627\u06cc \u0645\u062a\u062c\u0627\u0648\u0632 \u0635\u062f\u0627\u0645\u06cc \u0648\u0634\u06a9\u0633\u062a \u062d\u0632\u0628 \u06a9\u0648\u0645\u0644\u0647 \u0648\u062f\u0645\u06a9\u0631\u0627\u062a \u0648\u0622\u0632\u0627\u062f\u06cc \u062e\u0631\u0645\u0634\u0647\u0631 \u062f\u0631 \u0635\u0648\u0631\u062a\u06cc \u062c\u0627\u06cc \u062c\u0634\u0646 \u0648\u0633\u0631\u0648\u0631 \u0648 \u0634\u0627\u062f\u0645\u0627\u0646\u06cc \u0648 \u062a\u0628\u0631\u06cc\u06a9 \u062f\u0627\u0631\u062f \u06a9\u0647 \u062f\u0631 \u06a9\u0646\u0627\u0631 \u0622\u0646 \u0627\u062e\u0644\u0627\u0642 \u0627\u0633\u0644\u0627\u0645\u06cc \u0646\u06cc\u0632 \u0631\u0639\u0627\u06cc\u062a \u0634\u0648\u062f \u0648\u0627\u0632 \u0647\u0631 \u06af\u0648\u0646\u0647 \u0627\u0641\u0631\u0627\u0637 \u0648\u062a\u0641\u0631\u06cc\u0637 \u0648 \u063a\u0631\u0648\u0631 \u0648 \u062a\u0646\u062f\u0631\u0648\u06cc \u0627\u062c\u062a\u0646\u0627\u0628 \u06af\u0631\u062f\u062f.
p62
aV\u0631\u0627\u0646\u0646\u062f\u06af\u06cc \u0628\u0627 \u062d\u0627\u0644\u062a \u0645\u0633\u062a\u06cc \u062c\u0627\u0646 \u06f5 \u0646\u0641\u0631 \u0631\u0627 \u062f\u0631 \u0645\u062d\u0648\u0631 \u062f\u0631\u06af\u0632- \u0642\u0648\u0686\u0627\u0646 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u06af\u0631\u0641\u062a.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0633\u0631\u0647\u0646\u06af \u0645\u062d\u0645\u062f \u0635\u0641\u0627\u06cc\u06cc\u060c \u0631\u0626\u06cc\u0633 \u067e\u0644\u06cc\u0633 \u0631\u0627\u0647 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u062f\u0631 \u0646\u0634\u0633\u062a \u062e\u0628\u0631\u06cc \u06a9\u0647 \u0627\u0645\u0631\u0648\u0632\u060c \u0633\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u062f\u0631 \u062f\u0641\u062a\u0631 \u0645\u0639\u0627\u0648\u0646\u062a \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0646\u0627\u062c\u0627 \u0628\u0631\u06af\u0632\u0627\u0631 \u0634\u062f\u060c \u0627\u0632 \u0648\u0642\u0648\u0639 \u062a\u0635\u0627\u062f\u0641\u06cc \u062f\u0631 \u0633\u0627\u0639\u062a \u06f1\u06f6 \u0631\u0648\u0632 \u062c\u0645\u0639\u0647\u060c \u062f\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u062f\u0631 \u062c\u0627\u062f\u0647 \u062a\u0628\u0627\u0631\u06a9 \u062f\u0631 \u0645\u062d\u0648\u0631 \u062f\u0631\u06af\u0632- \u0642\u0648\u0686\u0627\u0646 \u062e\u0628\u0631 \u062f\u0627\u062f.\u0648\u06cc \u062f\u0631 \u062a\u0634\u0631\u06cc\u062d \u0627\u06cc\u0646 \u062d\u0627\u062f\u062b\u0647 \u0627\u0638\u0647\u0627\u0631 \u06a9\u0631\u062f: \u062f\u0631 \u0627\u06cc\u0646 \u062a\u0635\u0627\u062f\u0641 \u06cc\u06a9 \u062f\u0633\u062a\u06af\u0627\u0647 \u062e\u0648\u062f\u0631\u0648\u06cc \u0647\u06cc\u0648\u0646\u062f\u0627 \u0622\u0632\u0631\u0627 \u062f\u0631 \u0627\u06cc\u0646 \u0645\u0633\u06cc\u0631 \u0628\u0627 \u0627\u0646\u062d\u0631\u0627\u0641 \u0628\u0647 \u0686\u067e \u0628\u0647 \u06cc\u06a9 \u062f\u0633\u062a\u06af\u0627\u0647 \u062e\u0648\u062f\u0631\u0648\u06cc \u067e\u0631\u0627\u06cc\u062f \u0628\u0647\u200c\u0635\u0648\u0631\u062a \u0631\u062e \u0628\u0647 \u0631\u062e \u0628\u0631\u062e\u0648\u0631\u062f \u06a9\u0631\u062f \u06a9\u0647 \u0628\u0631 \u0627\u062b\u0631 \u0622\u0646 \u062a\u0645\u0627\u0645\u06cc \u0633\u0631\u0646\u0634\u06cc\u0646\u0627\u0646 \u067e\u0631\u0627\u06cc\u062f \u062c\u0627\u0646 \u062e\u0648\u062f \u0631\u0627 \u0627\u0632 \u062f\u0633\u062a \u062f\u0627\u062f\u0646\u062f \u0627\u0645\u0627 \u0631\u0627\u0646\u0646\u062f\u0647 \u0648 \u0633\u0631\u0646\u0634\u06cc\u0646 \u0622\u0632\u0631\u0627 \u0628\u0647 \u062f\u0644\u06cc\u0644 \u062a\u062c\u0647\u06cc\u0632 \u062e\u0648\u062f\u0631\u0648 \u0628\u0647 \u0633\u06cc\u0633\u062a\u0645 \u0627\u06cc\u0645\u0646\u06cc \u0648 \u0627\u06cc\u0631\u0628\u06af\u060c \u0635\u062f\u0645\u0647 \u0646\u062f\u06cc\u062f\u0646\u062f.\u0635\u0641\u0627\u06cc\u06cc \u0627\u0641\u0632\u0648\u062f: \u062a\u062d\u0642\u06cc\u0627\u062a \u0627\u0648\u0644\u06cc\u0647 \u067e\u0644\u06cc\u0633 \u0628\u0627 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u062f\u0633\u062a\u06af\u0627\u0647 \u0627\u0644\u06a9\u0644\u200c\u0633\u0646\u062c \u062d\u0627\u06a9\u06cc \u0627\u0632 \u0645\u0633\u062a \u0628\u0648\u062f\u0646 \u0631\u0627\u0646\u0646\u062f\u0647 \u0627\u0633\u062a \u0627\u0645\u0627 \u0627\u06cc\u0646 \u0645\u0633\u0627\u0644\u0647 \u0647\u0646\u0648\u0632 \u0628\u0637\u0648\u0631 \u0642\u0637\u0639 \u062b\u0627\u0628\u062a \u0646\u0634\u062f\u0647 \u0627\u0633\u062a \u0648 \u062a\u062d\u0642\u06cc\u0642\u0627\u062a \u0645\u0642\u0627\u0645\u0627\u062a \u0642\u0636\u0627\u06cc\u06cc \u0628\u0631 \u0631\u0648\u06cc \u0627\u06cc\u0646 \u067e\u0631\u0648\u0646\u062f\u0647 \u0627\u062f\u0627\u0645\u0647 \u062f\u0627\u0631\u062f.\u0631\u06cc\u06cc\u0633 \u067e\u0644\u06cc\u0633 \u0631\u0627\u0647 \u062e\u0631\u0627\u0633\u0627\u0646 \u0631\u0636\u0648\u06cc \u0627\u0632 \u06a9\u0627\u0647\u0634 \u06f3\u06f2 \u062f\u0631\u0635\u062f\u06cc \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u0627\u0633\u062a\u0627\u0646 \u062f\u0631 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a\u200c\u0645\u0627\u0647 \u0633\u0627\u0644 \u062c\u0627\u0631\u06cc \u062e\u0628\u0631 \u062f\u0627\u062f \u0648 \u0627\u0641\u0632\u0648\u062f: \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u062f\u0631 \u0641\u0631\u0648\u0631\u062f\u06cc\u0646 \u0627\u0645\u0633\u0627\u0644 \u06a9\u0627\u0647\u0634 \u06f6 \u062f\u0631\u0635\u062f\u06cc \u062f\u0627\u0634\u062a\u0647 \u0627\u0633\u062a.\u0628\u0631 \u0627\u0633\u0627\u0633 \u0622\u0645\u0627\u0631 \u0645\u0631\u06a9\u0632 \u0641\u0631\u0645\u0627\u0646\u062f\u0647\u06cc \u0648 \u06a9\u0646\u062a\u0631\u0644 \u067e\u0644\u06cc\u0633 \u06f1\u06f1\u06f0\u060c \u062a\u0639\u062f\u0627\u062f \u062a\u0644\u0641\u0627\u062a \u062a\u0635\u0627\u062f\u0641\u0627\u062a \u0627\u0633\u062a\u0627\u0646 \u062f\u0631 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a \u062a\u0627 \u0627\u06cc\u0646 \u0644\u062d\u0638\u0647 \u0628\u0627 \u06a9\u0627\u0647\u0634 \u06f3\u06f2 \u062f\u0631\u0635\u062f\u06cc \u0646\u0633\u0628\u062a \u0628\u0647 \u0645\u062f\u062a \u0645\u0634\u0627\u0628\u0647 \u0633\u0627\u0644 \u0642\u0628\u0644\u060c \u06f1\u06f9 \u0646\u0641\u0631 \u0627\u0639\u0644\u0627\u0645 \u0634\u062f\u0647 \u0627\u0633\u062a.\u0635\u0641\u0627\u06cc\u06cc \u0628\u0627 \u0627\u0634\u0627\u0631\u0647 \u0628\u0647 \u062a\u0634\u062f\u06cc\u062f \u0627\u062c\u0631\u0627\u06cc \u0637\u0631\u062d \u0627\u0645\u0646\u06cc\u062a \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0648 \u0627\u062e\u0644\u0627\u0642\u06cc \u0646\u0627\u062c\u0627 \u0627\u0632 \u0627\u0648\u0644 \u062e\u0631\u062f\u0627\u062f \u062e\u0627\u0637\u0631\u0646\u0634\u0627\u0646 \u06a9\u0631\u062f: \u067e\u0644\u06cc\u0633\u200c\u0631\u0627\u0647 \u0628\u0647 \u0627\u06cc\u0646 \u0637\u0631\u062d \u0627\u0632 \u0628\u0639\u062f \u062a\u0631\u0627\u0641\u06cc\u06a9\u06cc \u0646\u06af\u0627\u0647 \u0645\u06cc\u200c\u06a9\u0646\u062f \u0648 \u0628\u0627 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u0627\u06cc\u062c\u0627\u062f \u0646\u0627\u0627\u0645\u0646\u06cc \u0627\u062c\u062a\u0645\u0627\u0639\u06cc \u0648 \u0627\u062e\u0644\u0627\u0642\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u0628\u0631\u062e\u0648\u0631\u062f \u062e\u0648\u0627\u0647\u062f \u06a9\u0631\u062f.\u0648\u06cc \u0627\u0632 \u0628\u0631\u062e\u0648\u0631\u062f \u0648 \u0627\u0639\u0645\u0627\u0644 \u0642\u0627\u0646\u0648\u0646 \u0628\u0627 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u0627\u0642\u062f\u0627\u0645 \u0628\u0647 \u0633\u06af\u200c\u06af\u0631\u062f\u0627\u0646\u06cc \u06cc\u0627 \u0647\u0631 \u062d\u06cc\u0648\u0627\u0646 \u062f\u06cc\u06af\u0631\u06cc \u062f\u0631 \u0645\u0639\u0627\u0628\u0631 \u0634\u0647\u0631\u06cc \u0645\u06cc\u200c\u06a9\u0646\u0646\u062f\u060c \u062e\u0628\u0631 \u062f\u0627\u062f \u0648 \u06af\u0641\u062a: \u0627\u06cc\u0646 \u0627\u0641\u0631\u0627\u062f \u0648 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u062d\u06cc\u0646 \u0631\u0627\u0646\u0646\u062f\u06af\u06cc \u0641\u0631\u0632\u0646\u062f \u062e\u0648\u062f \u0631\u0627 \u062f\u0631 \u0622\u063a\u0648\u0634 \u0645\u06cc\u200c\u06af\u06cc\u0631\u0646\u062f\u060c \u062f\u0631 \u0631\u0627\u0647\u200c\u0647\u0627\u06cc \u0627\u0635\u0644\u06cc \u0648 \u06a9\u0644\u0627\u0646\u200c\u0634\u0647\u0631\u0647\u0627 \u06f5\u06f0 \u0647\u0632\u0627\u0631 \u062a\u0648\u0645\u0627\u0646 \u0648 \u062f\u0631 \u0634\u0647\u0631\u0647\u0627\u06cc \u06a9\u0648\u0686\u06a9 \u06f4\u06f0 \u0647\u0632\u0627\u0631 \u062a\u0648\u0645\u0627\u0646 \u062c\u0631\u06cc\u0645\u0647 \u062e\u0648\u0627\u0647\u0646\u062f \u0634\u062f.\u0635\u0641\u0627\u06cc\u06cc \u0628\u0627 \u0628\u06cc\u0627\u0646 \u0627\u06cc\u0646\u06a9\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0628\u0648\u0642\u200c\u0647\u0627\u06cc \u0634\u06cc\u067e\u0648\u0631\u06cc \u062f\u0631 \u0628\u06cc\u0627\u0628\u0627\u0646 \u0645\u062c\u0627\u0632 \u0627\u0633\u062a\u060c \u0639\u0646\u0648\u0627\u0646 \u06a9\u0631\u062f: \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0628\u0648\u0642 \u0628\u06cc\u0627\u0628\u0627\u0646\u06cc \u062f\u0631 \u0634\u0647\u0631 \u0645\u0645\u0646\u0648\u0639 \u0627\u0633\u062a \u0648 \u062a\u062e\u0644\u0641 \u0645\u062d\u0633\u0648\u0628 \u0645\u06cc\u200c\u0634\u0648\u062f.
p63
aV\u0639\u0636\u0648 \u06a9\u0645\u06cc\u0633\u06cc\u0648\u0646 \u0645\u0639\u0645\u0627\u0631\u06cc \u0648 \u0634\u0647\u0631\u0633\u0627\u0632\u06cc \u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0634\u0647\u0631 \u062a\u0647\u0631\u0627\u0646 \u06af\u0641\u062a: \u062c\u0630\u0628 \u06af\u0631\u062f\u0634\u06af\u0631 \u0633\u0628\u0628 \u0627\u0631\u062a\u0628\u0627\u0637 \u0645\u06cc\u0627\u0646 \u0645\u0644\u062a\u200c\u0647\u0627 \u062e\u0648\u0627\u0647\u062f \u0634\u062f \u0648 \u062f\u0631 \u0648\u0627\u0642\u0639 \u0646\u0645\u0627\u06cc\u0634\u06af\u0627\u0647\u06cc \u0628\u0631\u0627\u06cc \u0645\u0639\u0631\u0641\u06cc \u0641\u0631\u0647\u0646\u06af \u06a9\u0634\u0648\u0631\u0647\u0627 \u0628\u0647 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u062f\u0631 \u0627\u06cc\u062c\u0627\u062f \u0627\u0631\u062a\u0628\u0627\u0637 \u0645\u06cc\u0627\u0646 \u0645\u0644\u062a\u200c\u0647\u0627 \u0647\u0645 \u062a\u0627\u062b\u06cc\u0631\u06af\u0630\u0627\u0631 \u0628\u0627\u0634\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0627\u06cc\u0633\u0646\u0627\u060c \u0645\u0647\u062f\u06cc \u0686\u0645\u0631\u0627\u0646 \u062f\u0631 \u062e\u0635\u0648\u0635 \u062a\u0627\u062b\u06cc\u0631\u0627\u062a \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc \u0628\u0631 \u062f\u06cc\u067e\u0644\u0645\u0627\u0633\u06cc \u0648 \u0631\u0648\u0627\u0628\u0637 \u0645\u06cc\u0627\u0646 \u0645\u0644\u062a\u200c\u0647\u0627 \u0627\u0638\u0647\u0627\u0631 \u06a9\u0631\u062f: \u06cc\u06a9\u06cc \u0627\u0632 \u0631\u0627\u0647 \u0647\u0627\u06cc\u06cc \u06a9\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u0645\u062e\u062a\u0644\u0641 \u0631\u0627 \u0628\u0647 \u0648\u0633\u06cc\u0644\u0647 \u0622\u0646 \u062c\u0630\u0628 \u06a9\u0631\u062f\u060c \u0645\u0639\u0645\u0627\u0631\u06cc \u0642\u062f\u06cc\u0645\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u06a9\u0647 \u062f\u0631 \u06a9\u0634\u0648\u0631 \u0648\u062c\u0648\u062f \u062f\u0627\u0631\u062f\u060c \u062f\u0631 \u0627\u06cc\u0646 \u0639\u0631\u0635\u0647 \u0633\u0631\u0622\u0645\u062f \u0627\u0633\u062a.\u0648\u06cc \u0627\u0641\u0632\u0648\u062f: \u062c\u0630\u0628 \u06af\u0631\u062f\u0634\u06af\u0631 \u0633\u0628\u0628 \u0627\u0631\u062a\u0628\u0627\u0637 \u0645\u06cc\u0627\u0646 \u0645\u0644\u062a\u200c\u0647\u0627 \u062e\u0648\u0627\u0647\u062f \u0634\u062f \u0648 \u062f\u0631 \u0648\u0627\u0642\u0639 \u0646\u0645\u0627\u06cc\u0634\u06af\u0627\u0647\u06cc \u0628\u0631\u0627\u06cc \u0645\u0639\u0631\u0641\u06cc \u0641\u0631\u0647\u0646\u06af \u06a9\u0634\u0648\u0631\u0647\u0627 \u0628\u0647 \u06cc\u06a9\u062f\u06cc\u06af\u0631 \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u062f\u0631 \u0627\u06cc\u062c\u0627\u062f \u0627\u0631\u062a\u0628\u0627\u0637 \u0645\u06cc\u0627\u0646 \u0645\u0644\u062a\u200c\u0647\u0627 \u0647\u0645 \u062a\u0627\u062b\u06cc\u0631\u06af\u0630\u0627\u0631 \u0628\u0627\u0634\u062f.\u0639\u0636\u0648 \u06a9\u0645\u06cc\u0633\u06cc\u0648\u0646 \u0645\u0639\u0645\u0627\u0631\u06cc \u0648 \u0634\u0647\u0631\u0633\u0627\u0632\u06cc \u0634\u0648\u0631\u0627\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0634\u0647\u0631 \u062a\u0647\u0631\u0627\u0646 \u062d\u0645\u0627\u06cc\u062a \u0627\u0632 \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc \u0648 \u0627\u0633\u0644\u0627\u0645\u06cc \u0631\u0627 \u0633\u0628\u0628 \u0631\u0648\u0646\u0642 \u06af\u0631\u062f\u0634\u06af\u0631\u06cc \u0628\u0647 \u0648\u06cc\u0698\u0647 \u062f\u0631 \u0634\u0647\u0631 \u062f\u0627\u0646\u0633\u062a \u0648 \u062a\u0635\u0631\u06cc\u062d \u06a9\u0631\u062f: \u0628\u0631\u0627\u06cc \u0627\u06cc\u0646 \u06a9\u0647 \u0634\u0647\u0631\u0647\u0627 \u0628\u062a\u0648\u0627\u0646\u0646\u062f \u0628\u0631\u0627\u06cc \u062c\u0630\u0628 \u06af\u0631\u062f\u0634\u06af\u0631 \u0627\u0632 \u0634\u0647\u0631\u0647\u0627\u06cc \u062f\u06cc\u06af\u0631 \u067e\u06cc\u0634\u06cc \u0628\u06af\u06cc\u0631\u0646\u062f\u060c \u0628\u0627\u06cc\u062f \u0646\u0634\u0627\u0646\u0647\u200c\u0647\u0627\u06cc \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc \u062f\u0631 \u0622\u0646\u0647\u0627 \u0648\u062c\u0648\u062f \u062f\u0627\u0634\u062a\u0647 \u0628\u0627\u0634\u062f \u0632\u06cc\u0631\u0627 \u0645\u0639\u0645\u0627\u0631\u06cc \u0645\u062f\u0631\u0646 \u0631\u0627 \u062c\u0627\u0647\u0627\u06cc \u062f\u06cc\u06af\u0631 \u062f\u0646\u06cc\u0627 \u0647\u0645 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u067e\u06cc\u062f\u0627 \u06a9\u0631\u062f \u0627\u0645\u0627 \u0646\u0642\u0637\u0647 \u062a\u0645\u0627\u06cc\u0632 \u0645\u0627 \u0628\u0627\u06cc\u062f \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc \u0627\u0633\u0644\u0627\u0645\u06cc \u0628\u0627\u0634\u062f.\u0686\u0645\u0631\u0627\u0646 \u0634\u0647\u0631\u0633\u0627\u0632\u06cc \u0645\u062f\u0631\u0646 \u0631\u0627 \u0628\u0647 \u062a\u0646\u0647\u0627\u06cc\u06cc \u062c\u0648\u0627\u0628\u06af\u0648\u06cc \u0635\u0646\u0639\u062a \u0645\u0639\u0645\u0627\u0631\u06cc \u0648 \u062a\u0648\u0631\u06cc\u0633\u0645 \u0646\u062f\u0627\u0646\u0633\u062a \u0648 \u062e\u0627\u0637\u0631\u0646\u0634\u0627\u0646 \u06a9\u0631\u062f: \u0634\u0647\u0631\u0633\u0627\u0632\u06cc \u0645\u062f\u0631\u0646 \u0628\u0627\u06cc\u062f \u0628\u0627 \u0628\u0631\u0646\u0627\u0645\u0647 \u0631\u06cc\u0632\u06cc \u0645\u0646\u0627\u0633\u0628 \u062f\u0631 \u062e\u062f\u0645\u062a \u0645\u0639\u0645\u0627\u0631\u06cc \u0633\u0646\u062a\u06cc \u0642\u0631\u0627\u0631 \u0628\u06af\u06cc\u0631\u062f\u060c \u0628\u0647 \u0637\u0648\u0631\u06cc \u0647\u0645 \u0627\u0628\u0632\u0627\u0631\u0647\u0627 \u0648 \u062a\u06a9\u0646\u0648\u0644\u0648\u0698\u06cc \u062c\u062f\u06cc\u062f \u062f\u0631 \u0622\u0646\u0647\u0627 \u0645\u0648\u0631\u062f \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0642\u0631\u0627\u0631 \u06af\u06cc\u0631\u062f \u0648 \u0647\u0645 \u0622\u062b\u0627\u0631\u06cc \u0627\u0632 \u0645\u0639\u0645\u0627\u0631\u06cc \u0627\u06cc\u0631\u0627\u0646\u06cc - \u0627\u0633\u0644\u0627\u0645\u06cc \u062f\u0631 \u0622\u0646 \u062f\u06cc\u062f\u0647 \u0634\u0648\u062f \u062a\u0627 \u06af\u0631\u062f\u0634\u06af\u0631\u0627\u0646 \u0631\u0627 \u0645\u062a\u0648\u062c\u0647 \u062e\u0648\u062f \u06a9\u0631\u062f\u0647 \u0628\u0627\u0634\u062f.
p64
aV\u0645\u0635\u0627\u062d\u0628\u0647 \u0646\u0641\u062a\u0646\u0627 \u0628\u0627 \u0639\u0644\u06cc \u0645\u0627\u062c\u062f\u06cc\u060c \u0645\u0639\u0627\u0648\u0646 \u0628\u06cc\u0646 \u0627\u0644\u0645\u0644\u0644 \u0648\u0632\u0627\u0631\u062a \u0646\u0641\u062a \u062f\u0648 \u0628\u062e\u0634 \u062f\u0627\u0631\u062f.\u0628\u062e\u0634 \u0627\u0648\u0644 \u0645\u0631\u0628\u0648\u0637 \u0628\u0647 \u0627\u062e\u0628\u0627\u0631\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0627\u06cc\u0646 \u0631\u0648\u0632\u0647\u0627 \u0627\u0632 \u0641\u0631\u0648\u0634 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0646\u0641\u062a\u06cc \u0645\u0627 \u062a\u0648\u0633\u0637 \u0686\u06cc\u0646\u06cc\u200c\u0647\u0627 \u0648 \u0631\u0648\u0633\u200c\u0647\u0627 \u0628\u0647 \u0639\u0631\u0627\u0642 \u0645\u06cc\u200c\u0631\u0633\u06cc\u062f \u0648 \u0646\u0641\u062a\u0646\u0627  \u062f\u0631 \u0686\u0646\u062f \u0645\u0648\u0631\u062f \u067e\u06cc \u06af\u06cc\u0631 \u0622\u0646 \u0628\u0648\u062f.\u062e\u0628\u0631\u06cc \u06a9\u0647 \u0646\u0641\u062a\u0646\u0627 \u0622\u0646 \u0631\u0627 \u0645\u0646\u062a\u0634\u0631 \u06a9\u0631\u062f \u0627\u06cc\u0646 \u0628\u0648\u062f \u06a9\u0647 \u0647\u0645 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0631 \u0645\u06cc\u062f\u0627\u0646 \u0622\u0630\u0631 \u0648 \u0647\u0645 \u0686\u06cc\u0646 \u062f\u0631 \u0622\u0632\u0627\u062f\u06af\u0627\u0646\u060c \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0646\u0641\u062a\u06cc \u0627\u06cc\u0631\u0627\u0646 \u0631\u0627 \u0628\u0647 \u0639\u0631\u0627\u0642\u06cc\u200c\u0647\u0627 \u0641\u0631\u0648\u062e\u062a\u0647\u200c\u200c\u0627\u0646\u062f.\u0645\u0627\u062c\u062f\u06cc \u062f\u0631 \u062e\u0635\u0648\u0635 \u0631\u0648\u0633\u06cc\u0647 \u0628\u0627 \u0627\u062d\u062a\u06cc\u0627\u0637 \u0633\u062e\u0646 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f.\u062f\u0631 \u06a9\u0644\u0627\u0645\u0634 \u0622\u0647\u0633\u062a\u0647 \u0648 \u0628\u0627 \u0637\u0645\u0627\u0646\u06cc\u0646\u0647 \u0645\u0648\u0636\u0639 \u06af\u0631\u0641\u062a\u0646 \u0645\u0648\u062c \u0645\u06cc\u200c\u0632\u0646\u062f.\u0627\u0645\u0627 \u062f\u0631 \u062e\u0635\u0648\u0635 \u0622\u0632\u0627\u062f\u06af\u0627\u0646 \u0648 \u0686\u06cc\u0646\u06cc\u200c\u0647\u0627 \u0627\u062d\u062a\u06cc\u0627\u0637\u0634 \u06a9\u0645\u062a\u0631 \u0627\u0633\u062a \u0648 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f «\u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0627\u0632 \u0646\u0638\u0631 \u0633\u06cc\u0627\u0633\u06cc\u060c \u0627\u062e\u0644\u0627\u0642\u06cc \u0648 \u062a\u0639\u0647\u062f \u062d\u0631\u0641\u0647\u200c\u0627\u06cc \u0642\u0627\u0628\u0644 \u067e\u06cc\u200c\u06af\u06cc\u0631\u06cc \u0627\u0633\u062a \u0648 \u0646\u0628\u0627\u06cc\u062f \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0631\u0627 \u0645\u06cc\u200c\u06a9\u0631\u062f\u0646\u062f.» \u0627\u0648 \u0627\u06cc\u0646 \u0631\u0648\u0632\u0647\u0627 \u0628\u0647 \u0634\u062f\u062a \u0645\u0634\u063a\u0648\u0644 \u0645\u0630\u0627\u06a9\u0631\u0627\u062a \u0628\u0627 \u0631\u0648\u0633\u200c\u0647\u0627 \u062f\u0631 \u062e\u0635\u0648\u0635 \u0642\u0631\u0627\u0631\u200c\u062f\u0627\u062f \u0641\u0631\u0648\u0634 \u0646\u0641\u062a \u0627\u0633\u062a \u06a9\u0647 \u0645\u0627\u062c\u0631\u0627\u06cc \u0627\u06cc\u0646 \u0642\u0631\u0627\u0631\u062f\u0627\u062f \u0645\u0648\u0636\u0648\u0639 \u0627\u0635\u0644\u06cc \u0628\u062e\u0634 \u062f\u0648\u0645 \u0645\u0635\u0627\u062d\u0628\u0647 \u0645\u0627 \u0628\u0627 \u0645\u0627\u062c\u062f\u06cc \u0627\u0633\u062a \u06a9\u0647 \u0628\u0647 \u0632\u0648\u062f\u06cc \u0645\u0646\u062a\u0634\u0631 \u062e\u0648\u0627\u0647\u062f \u0634\u062f.\u0645\u0631\u062f \u0627\u0648\u0644 \u062f\u06cc\u067e\u0644\u0645\u0627\u0633\u06cc \u0646\u0641\u062a \u0628\u0627 \u0645\u0648\u06cc\u06cc \u0633\u0641\u06cc\u062f \u0648 \u062c\u062b\u0647\u200c\u0627\u06cc \u0638\u0631\u06cc\u0641 \u0648 \u0631\u06cc\u0632\u060c \u062a\u0633\u0644\u0637\u06cc \u062f\u0631 \u06a9\u0644\u0627\u0645\u0634 \u0627\u0633\u062a \u06a9\u0647 \u062e\u0648\u062f\u0634 \u0645\u06cc\u200c\u06af\u0648\u06cc\u062f \u0628\u0631\u0622\u0645\u062f\u0647 \u0627\u0632 \u0633\u0627\u0644\u200c\u0647\u0627 \u0645\u0627\u0645\u0648\u0631\u06cc\u062a \u0628\u0631\u0627\u06cc \u067e\u06cc\u200c\u06af\u06cc\u0631\u06cc \u0645\u0646\u0627\u0641\u0639 \u0645\u0644\u06cc \u0627\u0633\u062a.\u0627\u0648 \u062f\u0648\u0628\u0627\u0631 \u062f\u0631 \u0645\u06cc\u0627\u0646 \u0645\u0635\u0627\u062d\u0628\u0647\u200c\u0627\u0634 \u0628\u0627 \u0645\u0627 \u062a\u0627\u06a9\u06cc\u062f \u0645\u06cc\u200c\u06a9\u0646\u062f «\u0645\u0646 \u0627\u062a\u0641\u0627\u0642\u0627 \u062e\u06cc\u0644\u06cc \u0645\u0644\u06cc \u0647\u0633\u062a\u0645.\u067e\u062f\u0631\u200c\u0628\u0632\u0631\u06af\u0645 \u0648\u06a9\u06cc\u0644 \u0645\u0635\u062f\u0642 \u0628\u0648\u062f\u0647 \u0627\u0633\u062a.» \u0628\u062e\u0634 \u0627\u0648\u0644 \u0645\u0635\u0627\u062d\u06cc\u0647 \u0645\u0627 \u0628\u0627 \u0645\u0627\u062c\u062f\u06cc \u067e\u06cc\u0634 \u0631\u0648\u06cc \u0634\u0645\u0627 \u0627\u0633\u062a\u06af\u0632\u0627\u0631\u0634\u0627\u062a\u06cc \u0628\u0647 \u0645\u0627 \u0631\u0633\u06cc\u062f\u0647 \u0628\u0648\u062f \u06a9\u0647 \u0631\u0648\u0633\u06cc\u0647 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0645\u06cc\u062f\u0627\u0646 \u0622\u0630\u0631 \u0631\u0627 \u06a9\u0647 \u0628\u0627 \u0639\u0631\u0627\u0642 \u0647\u0645 \u0645\u0634\u062a\u0631\u06a9 \u0627\u0633\u062a \u0631\u0627 \u0628\u0647 \u0627\u06cc\u0646 \u06a9\u0634\u0648\u0631 \u0641\u0631\u0648\u062e\u062a\u0647\u060c \u0634\u0645\u0627 \u0627\u06cc\u0646 \u06af\u0632\u0627\u0631\u0634 \u0631\u0627 \u062a\u0627\u06cc\u06cc\u062f \u0645\u06cc\u200c\u06a9\u0646\u06cc\u062f\u061f\u0645\u0646 \u0627\u06cc\u0646 \u0631\u0627 \u0627\u0632 \u0631\u0633\u0627\u0646\u0647\u200c\u0647\u0627 \u0634\u0646\u06cc\u062f\u0645.\u0645\u06af\u0631 \u062f\u0631 \u0645\u0630\u0627\u06a9\u0631\u0627\u062a\u06cc \u06a9\u0647 \u0628\u0627 \u0631\u0648\u0633\u06cc\u0647 \u062f\u0627\u0631\u06cc\u062f \u062e\u06cc\u0627\u0646\u062a \u0622\u0646\u200c\u0647\u0627 \u0646\u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u0628\u0631\u06af \u0628\u0631\u0646\u062f\u0647 \u0645\u0627 \u0628\u0627\u0634\u062f\u061f\u0686\u0648\u0646 \u0647\u0646\u0648\u0632 \u0628\u0631\u0627\u06cc \u0645\u0646 \u0645\u0633\u0644\u0645 \u0646\u0634\u062f\u0647 \u0627\u0633\u062a.\u0627\u0632 \u062f\u06cc\u0631\u0648\u0632 \u06cc\u0627 \u067e\u0631\u06cc\u0631\u0648\u0632 \u0686\u0646\u06cc\u0646 \u062e\u0628\u0631\u0647\u0627\u06cc\u06cc \u0645\u0646\u062a\u0634\u0631 \u0634\u062f\u0647 \u06a9\u0647 \u06cc\u06a9 \u0686\u0646\u06cc\u0646 \u062f\u0627\u0633\u062a\u0627\u0646\u06cc \u0631\u062e \u062f\u0627\u062f\u0647 \u0648 \u0627\u06cc\u0646 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0631\u0627 \u062f\u0627\u062f\u0647\u200c\u0627\u0646\u062f.\u06cc\u0639\u0646\u06cc \u0622\u0645\u062f\u0647\u200c\u0627\u0646\u062f \u0645\u06cc\u062f\u0627\u0646 \u0645\u0627 \u0631\u0627 \u062a\u0648\u0633\u0639\u0647 \u0628\u062f\u0647\u0646\u062f \u0648 \u0686\u0648\u0646 \u0645\u06cc\u062f\u0627\u0646 \u0645\u0634\u062a\u0631\u06a9 \u0628\u0648\u062f\u0647 \u0631\u0641\u062a\u0647\u200c\u0627\u0646\u062f \u0627\u06cc\u0646 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0631\u0627 \u0628\u0647 \u0637\u0631\u0641 \u0645\u0642\u0627\u0628\u0644 \u062f\u0627\u062f\u0647\u200c\u0627\u0646\u062f.\u0627\u06af\u0631 \u062f\u0631\u0633\u062a \u0628\u0627\u0634\u062f \u0628\u0644\u0647\u060c \u0628\u0627\u06cc\u062f \u067e\u06cc\u06af\u06cc\u0631\u06cc \u0634\u0648\u062f.\u0627\u06cc\u0646\u200c\u0647\u0627 \u0631\u0627 \u0646\u0628\u0627\u06cc\u062f \u062a\u0628\u062f\u06cc\u0644 \u06a9\u0631\u062f \u0628\u0647 \u06cc\u06a9 \u0645\u0639\u0636\u0644 \u0648 \u06cc\u06a9 \u0686\u0627\u0644\u0634 \u062f\u0627\u0626\u0645\u06cc.\u0648\u0644\u06cc \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0627\u0633\u0627\u0633\u06cc \u0645\u062e\u0632\u0646 \u06a9\u0647 \u0628\u0647 \u0627\u06cc\u0646 \u0634\u06a9\u0644 \u0646\u06cc\u0633\u062a.\u0648\u0644\u06cc \u06a9\u0644\u0627 \u0648\u0642\u062a\u06cc \u062f\u0648 \u06a9\u0634\u0648\u0631 \u0648\u0627\u0631\u062f \u0645\u0630\u0627\u06a9\u0631\u0647 \u0645\u06cc\u200c\u0634\u0648\u0646\u062f \u0642\u0637\u0639\u0627 \u0627\u06cc\u0646 \u06a9\u0627\u0631\u060c \u06a9\u0627\u0631 \u0632\u0634\u062a\u06cc \u0627\u0633\u062a \u0648 \u0647\u06cc\u0686 \u062a\u0631\u062f\u06cc\u062f\u06cc \u0646\u0628\u0627\u06cc\u062f \u06a9\u0631\u062f \u0648 \u0645\u0646 \u0645\u06cc\u200c\u06af\u0648\u06cc\u0645 \u0627\u06af\u0631 \u0627\u06cc\u0646\u200c\u0637\u0648\u0631 \u0634\u062f\u0647 \u0628\u0627\u0634\u062f \u062d\u062a\u0645\u0627 \u0628\u0627\u06cc\u062f \u06af\u0644\u0647\u200c\u06af\u0630\u0627\u0631\u06cc \u06a9\u0631\u062f.\u0627\u06cc\u0646 \u0645\u0633\u0626\u0644\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u062f \u062f\u0631 \u0633\u0637\u0648\u062d \u062f\u06cc\u06af\u0631 \u0647\u0645 \u0645\u0637\u0631\u062d \u0634\u0648\u062f \u0648 \u0645\u0645\u06a9\u0646 \u0627\u0633\u062a \u0634\u0645\u0627 \u0628\u062a\u0648\u0627\u0646\u06cc\u062f \u062f\u0631 \u062c\u0627\u0647\u0627\u06cc \u062f\u06cc\u06af\u0631\u06cc \u062a\u0644\u0627\u0641\u06cc \u06a9\u0646\u06cc\u062f \u0648\u0644\u06cc \u0627\u06cc\u0646 \u0646\u0628\u0627\u06cc\u062f \u0639\u0627\u0645\u0644\u06cc \u0634\u0648\u062f \u06a9\u0647 \u0631\u0627\u0628\u0637\u0647 \u062f\u0648 \u06a9\u0634\u0648\u0631 \u0631\u0627 \u0645\u062d\u062f\u0648\u062f \u06a9\u0646\u06cc\u062f \u0648 \u062a\u0645\u0627\u0645 \u0631\u0648\u0627\u0628\u0637 \u0631\u0627 \u062e\u0631\u0627\u0628 \u06a9\u0646\u06cc\u062f.\u0627\u0632 \u0637\u0631\u0641\u06cc \u0647\u0645 \u0645\u062c\u0631\u06cc \u0648 \u0645\u062f\u06cc\u0631 \u062a\u0648\u0633\u0639\u0647 \u0622\u0632\u0627\u062f\u06af\u0627\u0646 \u0634\u0645\u0627\u0644\u06cc \u06af\u0641\u062a\u0647 \u06a9\u0647 \u0686\u06cc\u0646\u06cc\u200c\u0647\u0627 \u0647\u0645 \u062f\u0631 \u0622\u0632\u0627\u062f\u06af\u0627\u0646 \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0631\u0627 \u06a9\u0631\u062f\u0647\u200c\u0627\u0646\u062f \u0648 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u0622\u0632\u0627\u062f\u06af\u0627\u0646 \u0631\u0627 \u06a9\u0647 \u0628\u0627 \u0639\u0631\u0627\u0642 \u062f\u0631 \u0645\u062c\u0646\u0648\u0646 \u0645\u0634\u062a\u0631\u06a9 \u0647\u0633\u062a\u06cc\u0645 \u0628\u0647 \u0622\u0646\u200c\u0647\u0627 \u062f\u0627\u062f\u0647\u200c\u0627\u0646\u062f.\u0628\u0647\u062a\u0631\u06cc\u0646 \u0631\u0648\u0634 \u0628\u0647\u0631\u0647\u200c\u0628\u0631\u062f\u0627\u0631\u06cc \u0627\u0632 \u0645\u06cc\u062f\u0627\u0646\u200c\u0647\u0627\u06cc \u0645\u0634\u062a\u0631\u06a9 \u062f\u0631 \u0648\u0627\u0642\u0639 \u0631\u0648\u0634 \u0628\u0647\u0631\u0647\u200c\u0628\u0631\u062f\u0627\u0631\u06cc \u0648\u0627\u062d\u062f \u0627\u0633\u062a.
p65
aV\u0631\u0648\u0627\u0628\u0637 \u0639\u0645\u0648\u0645\u06cc \u0633\u0627\u0632\u0645\u0627\u0646 \u0645\u062f\u06cc\u0631\u06cc\u062a \u0648 \u0646\u0638\u0627\u0631\u062a \u0628\u0631 \u062a\u0627\u06a9\u0633\u06cc\u0631\u0627\u0646\u06cc \u0634\u06cc\u0631\u0627\u0632 \u0648\u062d\u0648\u0645\u0647 \u0627\u0639\u0644\u0627\u0645 \u06a9\u0631\u062f: \u0639\u0646\u0627\u06cc\u062a \u0627\u0644\u0644\u0647 \u0627\u062d\u0645\u062f\u06cc \u06cc\u06a9\u06cc \u0627\u0632 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646 \u062a\u0627\u06a9\u0633\u06cc \u0627\u06cc\u0646 \u0634\u0647\u0631 \u0633\u0648\u0645\u06cc\u0646 \u06a9\u062a\u0627\u0628 \u062e\u0648\u062f \u0631\u0627 \u0628\u0627 \u0646\u0627\u0645 \u02c8 \u06af\u0646\u062c\u06cc\u0646\u0647 \u0628\u0642\u0627\u02c8 \u0628\u0627 \u0645\u0648\u0636\u0648\u0639 \u0622\u06cc\u06cc\u0646\u06cc \u0648\u062f\u06cc\u0646\u06cc \u0622\u0645\u0627\u062f\u0647 \u0686\u0627\u067e \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.\u0627\u06cc\u0646 \u0631\u0648\u0627\u0628\u0637 \u0639\u0645\u0648\u0645\u06cc\u060c \u0634\u0646\u0628\u0647 \u062f\u0631 \u0645\u0637\u0644\u0628 \u0627\u0631\u0633\u0627\u0644\u06cc \u0628\u0647 \u0627\u06cc\u0631\u0646\u0627 \u0627\u0641\u0632\u0648\u062f: \u0627\u06cc\u0646 \u0631\u0627\u0646\u0646\u062f\u0647 \u062a\u0627\u06a9\u0633\u06cc \u06a9\u0647 \u0645\u062a\u0648\u0644\u062f \u0648 \u0633\u0627\u06a9\u0646 \u0634\u06cc\u0631\u0627\u0632 \u0627\u0633\u062a\u060c \u067e\u06cc\u0634 \u0627\u0632 \u0627\u06cc\u0646 \u062f\u0648 \u06a9\u062a\u0627\u0628 \u062f\u06cc\u06af\u0631 \u0628\u0647 \u0646\u0627\u0645\u200c\u0647\u0627\u06cc \u02c8\u0634\u06a9\u0648\u0641\u0647\u200c\u0647\u0627\u06cc \u0686\u0647\u0627\u0631 \u0628\u0631\u06af\u02c8 \u0648 \u02c8\u062d\u06a9\u0627\u06cc\u062a\u0647\u0627\u06cc \u0645\u0648\u0632\u0648\u0646\u02c8 \u06a9\u0647 \u0645\u0648\u0636\u0648\u0639 \u0622\u0646 \u067e\u0646\u062f\u0622\u0645\u0648\u0632\u06cc \u0627\u0633\u062a \u0628\u0647 \u0686\u0627\u067e \u0631\u0633\u0627\u0646\u062f\u0647 \u0627\u0633\u062a.\u0648\u06cc \u06a9\u0647 \u062f\u0627\u0631\u0627\u06cc \u0645\u062f\u0631\u06a9 \u062a\u062d\u0635\u06cc\u0644\u06cc \u062f\u06cc\u067e\u0644\u0645 \u0627\u0633\u062a\u060c \u0627\u0648\u0642\u0627\u062a \u0641\u0631\u0627\u063a\u062a \u062e\u0648\u062f \u0631\u0627 \u0635\u0631\u0641 \u0633\u0631\u0648\u062f\u0646 \u0634\u0639\u0631 \u0645\u06cc\u200c\u06a9\u0646\u062f \u0648 \u0647\u0645 \u0627\u06cc\u0646\u06a9 \u0645\u0634\u063a\u0648\u0644 \u0646\u0648\u0634\u062a\u0646 \u06cc\u06a9 \u06a9\u062a\u0627\u0628 \u0637\u0646\u0632 \u0628\u0627 \u0639\u0646\u0648\u0627\u0646 \u02c8\u062e\u0646\u062f\u0647 \u0628\u0627\u0632\u0627\u0631\u06cc\u02c8 \u0627\u0632 \u0637\u0646\u0627\u0632 \u0634\u06cc\u0631\u0627\u0632\u06cc \u0648 \u062d\u06a9\u0627\u06cc\u062a \u02c8 \u062f\u0627\u0633\u062a\u0627\u0646\u200c\u0647\u0627\u06cc \u062c\u0628\u0647\u0647 \u0648 \u062c\u0646\u06af\u02c8 \u0627\u0633\u062a.\u0633\u0627\u0632\u0645\u0627\u0646 \u062a\u0627\u06a9\u0633\u06cc\u0631\u0627\u0646\u06cc \u0634\u06cc\u0631\u0627\u0632 \u0648 \u062d\u0648\u0645\u0647\u060c \u0636\u0645\u0646 \u0627\u0633\u062a\u0642\u0628\u0627\u0644 \u0627\u0632 \u0647\u0645\u0647 \u0631\u0627\u0646\u0646\u062f\u06af\u0627\u0646 \u062a\u0627\u06a9\u0633\u06cc \u06a9\u0647 \u062f\u0627\u0631\u0627\u06cc \u0647\u0631\u06af\u0648\u0646\u0647 \u0645\u062f\u0631\u06a9 \u0648 \u06cc\u0627 \u062a\u062e\u0635\u0635 \u062f\u0631 \u0632\u0645\u06cc\u0646\u0647\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0647\u0646\u0631\u06cc \u0627\u0632 \u062c\u0645\u0644\u0647 \u0634\u0639\u0631\u0648 \u0646\u0648\u06cc\u0633\u0646\u062f\u06af\u06cc \u0647\u0633\u062a\u0646\u062f \u062f\u0639\u0648\u062a \u0628\u0647 \u0647\u0645\u06a9\u0627\u0631\u06cc \u06a9\u0631\u062f\u0647 \u0627\u0633\u062a.
p66
aV\u0647\u0645\u0632\u0645\u0627\u0646 \u0628\u0627 \u0633\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u0648 \u0633\u0627\u0644\u0631\u0648\u0632 \u0622\u0632\u0627\u062f\u06cc \u062e\u0631\u0645\u0634\u0647\u0631 \u0635\u062f\u0648\u0631 \u062f\u0641\u062a\u0631\u0686\u0647\u200c\u0647\u0627\u06cc \u0628\u06cc\u0645\u0647 \u0647\u0645\u06af\u0627\u0646\u06cc \u0627\u0632 \u0633\u0648\u06cc \u0633\u0627\u0632\u0645\u0627\u0646 \u0628\u06cc\u0645\u0647 \u0633\u0644\u0627\u0645\u062a \u0622\u063a\u0627\u0632 \u0634\u062f.\u0628\u0647 \u06af\u0632\u0627\u0631\u0634 \u0645\u0647\u0631\u060c \u062b\u0628\u062a \u0646\u0627\u0645 \u0628\u06cc\u0645\u0647 \u0647\u0645\u06af\u0627\u0646\u06cc \u0627\u0632 \u0631\u0648\u0632 \u0627\u0648\u0644 \u0627\u0631\u062f\u06cc\u0628\u0647\u0634\u062a \u0645\u0627\u0647 \u062f\u0631 \u0633\u0631\u0627\u0633\u0631 \u06a9\u0634\u0648\u0631 \u0622\u063a\u0627\u0632 \u0634\u062f \u0648 \u062a\u0627 \u0628\u0647 \u0627\u0645\u0631\u0648\u0632 \u06f4 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0648 \u06f5\u06f0\u06f0 \u0647\u0632\u0627\u0631 \u0646\u0641\u0631 \u062f\u0631 \u0627\u0633\u062a\u0627\u0646\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0628\u0647 \u0645\u0646\u0638\u0648\u0631 \u062f\u0631\u06cc\u0627\u0641\u062a \u062f\u0641\u062a\u0631\u0686\u0647 \u0628\u06cc\u0645\u0647 \u0631\u0627\u06cc\u06af\u0627\u0646 \u062b\u0628\u062a \u0646\u0627\u0645 \u06a9\u0631\u062f\u0646\u062f.\u067e\u06cc\u0634 \u0628\u06cc\u0646\u06cc \u0634\u062f\u0647 \u0628\u0648\u062f \u062f\u0631 \u0627\u06cc\u0646 \u0645\u062f\u062a \u06cc\u06a9 \u0645\u0627\u0647 \u062d\u062f\u0648\u062f \u06f6 \u0645\u06cc\u0644\u06cc\u0648\u0646 \u0646\u0641\u0631 \u0627\u0632 \u0627\u0641\u0631\u0627\u062f \u0641\u0627\u0642\u062f \u0628\u06cc\u0645\u0647 \u062b\u0628\u062a \u0646\u0627\u0645 \u06a9\u0646\u0646\u062f \u0627\u0645\u0627 \u0627\u06cc\u0646\u06af\u0648\u0646\u0647 \u0646\u0634\u062f \u0648 \u0628\u0631\u0627\u0633\u0627\u0633 \u06af\u0641\u062a\u0647 \u0645\u062f\u06cc\u0631\u0639\u0627\u0645\u0644 \u0633\u0627\u0632\u0645\u0627\u0646 \u0628\u06cc\u0645\u0647 \u0633\u0644\u0627\u0645\u062a \u062b\u0628\u062a \u0646\u0627\u0645 \u0628\u0631\u0627\u06cc \u0645\u062a\u0642\u0627\u0636\u06cc\u0627\u0646 \u0628\u0633\u062a\u0647 \u0646\u06cc\u0633\u062a \u0648 \u0627\u0641\u0631\u0627\u062f \u0628\u0627\u0642\u06cc \u0645\u0627\u0646\u062f\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0628\u0647 \u0633\u0627\u06cc\u062a \u0633\u0627\u0632\u0645\u0627\u0646 \u0628\u06cc\u0645\u0647 \u0633\u0644\u0627\u0645\u062a \u0645\u0631\u0627\u062c\u0639\u0647 \u06a9\u0646\u0646\u062f.\u0647\u0645\u0686\u0646\u06cc\u0646 \u0628\u0647 \u0627\u0641\u0631\u0627\u062f\u06cc \u06a9\u0647 \u0648\u0627\u062c\u062f \u0634\u0631\u0627\u06cc\u0637 \u062f\u0631\u06cc\u0627\u0641\u062a \u062f\u0641\u062a\u0631\u0686\u0647 \u0628\u06cc\u0645\u0647 \u0633\u0644\u0627\u0645\u062a \u0647\u0633\u062a\u0646\u062f \u067e\u0633 \u0627\u0632 \u0628\u0631\u0631\u0633\u06cc\u200c\u0647\u0627\u06cc \u0644\u0627\u0632\u0645 \u0628\u0647 \u0635\u0648\u0631\u062a \u067e\u06cc\u0627\u0645\u06a9 \u0627\u0637\u0644\u0627\u0639\u200c\u0631\u0633\u0627\u0646\u06cc \u0645\u06cc\u200c\u0634\u0648\u062f \u06a9\u0647 \u0628\u0631\u0627\u06cc \u062f\u0631\u06cc\u0627\u0641\u062a \u062f\u0641\u062a\u0631\u0686\u0647 \u0645\u0631\u0627\u062c\u0639\u0647 \u06a9\u0646\u0646\u062f \u0648 \u067e\u06cc\u0634 \u0627\u0632 \u062f\u0631\u06cc\u0627\u0641\u062a \u067e\u06cc\u0627\u0645\u06a9 \u0627\u0632 \u0645\u0631\u0627\u062c\u0639\u0647 \u062d\u0636\u0648\u0631\u06cc \u067e\u0631\u0647\u06cc\u0632 \u0634\u0648\u062f.\u0644\u0627\u0632\u0645 \u0628\u0647 \u0630\u06a9\u0631 \u0627\u0633\u062a \u0627\u0645\u0631\u0648\u0632 \u0634\u0646\u0628\u0647 \u0633\u0648\u0645 \u062e\u0631\u062f\u0627\u062f \u062f\u0631 \u0647\u0645\u0627\u06cc\u0634 \u0628\u06cc\u0645\u0647 \u0647\u0645\u06af\u0627\u0646\u06cc \u0628\u0647 \u0635\u0648\u0631\u062a \u0646\u0645\u0627\u062f\u06cc\u0646 \u0648 \u0628\u0647 \u0647\u0631 \u06cc\u06a9 \u0627\u0632 \u0646\u0645\u0627\u06cc\u0646\u062f\u06af\u0627\u0646 \u0642\u0648\u0645\u06cc\u062a\u200c\u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u062f\u0641\u062a\u0631\u0686\u0647\u200c\u0647\u0627\u06cc \u0628\u06cc\u0645\u0647 \u0633\u0644\u0627\u0645\u062a \u0627\u0632 \u0633\u0648\u06cc \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631 \u062a\u062d\u0648\u06cc\u0644 \u062f\u0627\u062f\u0647 \u0634\u062f \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u062f\u0648 \u0646\u0648\u0632\u0627\u0631\u062f \u0628\u06cc \u0633\u0631\u067e\u0631\u0633\u062a \u0648 \u062f\u0648 \u06a9\u0648\u062f\u06a9 \u06a9\u0627\u0631 \u0646\u06cc\u0632 \u062f\u0641\u062a\u0631\u0686\u0647 \u062f\u0631\u06cc\u0627\u0641\u062a \u06a9\u0631\u062f\u0646\u062f \u0648 \u0631\u0626\u06cc\u0633 \u062c\u0645\u0647\u0648\u0631 \u067e\u062f\u0631\u062e\u0648\u0627\u0646\u062f\u0647 \u062f\u0648 \u06a9\u0648\u062f\u06a9 \u06cc\u062a\u06cc\u0645 \u0634\u062f.
p67
aV\u062a\u0635\u0627\u0648\u06cc\u0631 \u06a9\u0627\u0645\u0644 \u0627\u06cc\u0646 \u0645\u062c\u0645\u0648\u0639\u0647 \u0631\u0627 \u0627\u06cc\u0646\u062c\u0627 \u0628\u0628\u06cc\u0646\u06cc\u062f
